        <CommandText> -- SELECT 
 -- LeadId = l.Id
 -- BPCode = l.BusinessPointCode
 -- FROM lpm.Lead l
 -- WHERE l.CreatedOn &gt;= @fromDate AND l.CreatedOn &lt;= @toDate

    --Πίνακας Manager--
	IF OBJECT_ID('tempdb..#manager') IS NOT NULL DROP TABLE #manager;
	SELECT ll.Id, ll.LeadId, ll.AssignmentUser, ll.CreatedOn as CreatedOn,
	RowNo = ROW_NUMBER() Over( partition by ll.leadId order by ll.id desc)
	INTO #manager
	FROM [lpm].[LeadLog] ll
	WHERE ll.actionid in (1,8)
	AND charindex('.',logdesc) &gt; 2 --select * from  #manager where LeadId=76870

	--LEFT JOIN yps.dbo.SalesTreeHistory sth on sth.SalesmanID = u.id and sth.[Year] = YEAR(ctr.CreatedOn) and sth.SegmentID = MONTH(ctr.CreatedOn) and sth.IsDeleted=0
	--LEFT JOIN yps.dbo.[User] area on sth.ManagerID = area.ID

	--select distinct AssignmentUser from #manager

	--Πίνακας Supervisor--
	IF OBJECT_ID('tempdb..#supervisor') IS NOT NULL DROP TABLE #supervisor;
	SELECT ll.Id, ll.LeadId, ll.AssignmentUser, ll.CreatedOn as CreatedOn,
	RowNo = ROW_NUMBER() Over( partition by ll.leadId order by ll.id desc)
	INTO #supervisor
	FROM [lpm].[LeadLog] ll
	WHERE ll.actionid in (2,9)
	AND charindex('.',logdesc) &gt; 2

    --Πίνακας ΣΔ--
	IF OBJECT_ID('tempdb..#advisor') IS NOT NULL DROP TABLE #advisor;
	SELECT ll.Id, ll.LeadId, ll.AssignmentUser, ll.CreatedOn as CreatedOn,
	RowNo = ROW_NUMBER() Over( partition by ll.leadId order by ll.id desc) 
	INTO #advisor
	FROM  [lpm].[LeadLog] ll
	WHERE ll.actionid in (3,10)
	AND charindex('.',logdesc) &gt; 2
	
	--Πίνακας Επιστροφής--

	IF OBJECT_ID('tempdb..#return') IS NOT NULL DROP TABLE #return;
	SELECT ll.Id, ll.LeadId, ll.AssignmentUser, ll.CreatedOn as CreatedOn,
	RowNo = ROW_NUMBER() Over( partition by ll.leadId order by ll.id asc)
	INTO #return
	FROM [lpm].[LeadLog] ll
	WHERE ll.actionid = 5 
	AND charindex('.',logdesc) &gt; 2

	
	
	--Πίνακας Επανάθεσης--
	IF OBJECT_ID('tempdb..#reassign') IS NOT NULL DROP TABLE #reassign;
	SELECT ll.Id, ll.LeadId, ll.AssignmentUser, ll.CreatedOn as CreatedOn,
	RowNo = ROW_NUMBER() Over( partition by ll.leadId order by ll.id asc)
	INTO #reassign
	FROM [lpm].[LeadLog] ll
	WHERE ll.ActionId IN (8,9,10)
	AND charindex('.',logdesc) &gt; 2

	
	
	--Πίνακας Προσωρινής Κατάστασης--
	IF OBJECT_ID('tempdb..#tempStatus') IS NOT NULL DROP TABLE #tempStatus;
	SELECT ll.Id, ll.LeadId, ll.Status as TempStatus, ll.CreatedOn as CreatedOn,
	RowNo = ROW_NUMBER() Over( partition by ll.leadId order by ll.id asc)
	INTO #tempStatus
	FROM [lpm].[LeadLog] ll
	WHERE ll.ActionId = 6


    --Πίνακας Καταχώρησης Οριστικού Αποτελέσματος--
	IF OBJECT_ID('tempdb..#entryOutcome') IS NOT NULL DROP TABLE #entryOutcome;
	SELECT ll.Id, ll.LeadId, ll.CreatedOn, ll.Outcome as EntryOutcome, 
	RowNo = ROW_NUMBER() Over( partition by ll.leadId order by ll.id asc)
	INTO #entryOutcome
	FROM [lpm].[LeadLog] ll
	WHERE ll.ActionId = 4

    --Πίνακας Επιβεβαίωσης Οριστικού Αποτελέσματος--
	IF OBJECT_ID('tempdb..#finalOutcome') IS NOT NULL DROP TABLE #finalOutcome;
	SELECT ll.Id, ll.LeadId, ll.CreatedOn, 
	CASE 
		WHEN ll.Outcome IS NULL THEN (
			select top 1 Outcome from lpm.LeadLog l 
			where LeadId=ll.LeadId and Outcome is not null 
			order by CreatedOn desc)
		ELSE ll.Outcome END
	as FinalOutcome,
	RowNo = ROW_NUMBER() Over( partition by ll.leadId order by ll.id asc)
	INTO #finalOutcome
	FROM [lpm].[LeadLog] ll
	WHERE ll.ActionId = 7


	--Πίνακας Lead Filtered by Input / Parameter Dates--
	IF OBJECT_ID('tempdb..#leadsByDate') IS NOT NULL DROP TABLE #leadsByDate;
	SELECT *
	INTO #leadsByDate
	FROM [lpm].[Lead] l
	WHERE CAST(l.CreatedOn AS DATE) &gt;= CAST(@fromDate AS DATE)--cast('20200101' as date)
	  AND CAST(l.CreatedOn as DATE) &lt;= CAST(@toDate AS DATE)--cast('20210301' as date)


	  IF OBJECT_ID('tempdb..#admin') IS NOT NULL DROP TABLE #admin;
		select id=0, 
			u.USERNAME, 
			UserFullName = 'Admin',
			YpsUserId=0,
			SalesmanIds = (select SalesmanYPSIds from ItApps.scd.Salesman where Username = 'araptis'),
			ShortName = '',
			AppCode = a.APPLICATION_CODE,
			UserID = usr.Id
	   into #admin 
	   from [ITApps].[dbo].IT_APPLICATION_RIGHTS r
			join [ITApps].[dbo].IT_APPLICATION_USERS u on u.ADID = r.USER_ADID
			join [ITApps].[dbo].IT_APPLICATIONS a on a.ID = r.APPLICATION_ID
			--join [ITApps].[dbo].[YPSUsersAll] usr on usr.Username = u.USERNAME
			join YPS.dbo.[user] usr on usr.USERNAME = u.USERNAME 
			where a.APPLICATION_CODE = 'rep' 
----------------------------------------------------------------------------------------

SELECT 
  LeadId = l.Id,
  BPCode = l.BusinessPointCode,
  Source = l.TicketSource,
  CreatedOn = l.CreatedOn,
  DateAssignedToArea = dM.CreatedOn,
  AreaManager= dM.AssignmentUser,
  DateAssignedToSuper = dS.CreatedOn,
  Supervisor = dS.AssignmentUser,
  DateAssignedToAdvisor = dA.CreatedOn,
  Advisor = dA.AssignmentUser,
  ReturnDate = re.CreatedOn,
  ReturnedUsr = re.AssignmentUser,
  ReassignDate = rea.CreatedOn,
  ReassignUsr = rea.AssignmentUser,
  InitialEditDate = l.FirstOpenOn,
  InitialCommDate = l.FirstCommunicationOn,
  TmpStatusDate = tmp.CreatedOn,
  TmpStatus = tmp.TempStatus,
  eoCreatedOn =eo.CreatedOn,
  eoOutcome = eo.EntryOutcome,
  foCreatedOn = fo.CreatedOn,
  foOutcome = fo.FinalOutcome,
  curOutcome = ou.Name
  FROM #leadsByDate l
  LEFT JOIN #manager dM ON dM.LeadId = l.Id AND dM.RowNo = 1 
  LEFT JOIN #supervisor dS ON dS.LeadId = l.Id AND dS.RowNo = 1
  LEFT JOIN #advisor dA ON dA.LeadId = l.Id AND dA.RowNo = 1
  LEFT JOIN #return re ON re.LeadId = l.Id AND re.RowNo = 1
  LEFT JOIN #reassign rea ON rea.LeadId = l.Id AND rea.RowNo = 1
  LEFT JOIN #tempStatus tmp ON tmp.LeadId = l.Id AND tmp.RowNo = 1
  LEFT JOIN #entryOutcome eo ON eo.LeadId = l.Id AND eo.RowNo = 1
  LEFT JOIN #finalOutcome fo ON fo.LeadId = l.Id AND fo.RowNo = 1
  LEFT JOIN lpm.Outcome ou ON ou.Id = l.OutcomeId
WHERE 
((dM.AssignmentUser IN (@areamanagerName) OR dM.AssignmentUser IS NULL)
AND (dS.AssignmentUser IN (@supervisor) OR dS.AssignmentUser IS NULL)
AND (dA.AssignmentUser IN (@salesman) OR dA.AssignmentUser IS NULL)
AND ISNULL(ou.Name,'-') IN (@leadOutcome)
AND ISNULL(l.TicketSource,'-') IN (@sourceType)
AND COALESCE(dM.AssignmentUser,dS.AssignmentUser,dA.AssignmentUser) IS NOT NULL ) OR 

((dM.AssignmentUser IN (@areamanagerName) OR dM.AssignmentUser IS NULL)
AND (dS.AssignmentUser IN (@supervisor) OR dS.AssignmentUser IS NULL)
AND (dA.AssignmentUser IN (@salesman) OR dA.AssignmentUser IS NULL) AND
(ISNULL(ou.Name,'-') IN (@leadOutcome)
AND ISNULL(l.TicketSource,'-') IN (@sourceType)
AND COALESCE(dM.AssignmentUser,dS.AssignmentUser,dA.AssignmentUser) IS NULL
AND (@ypscurrentuserid in (1189,2264,524 ,426,2262,493) /*araptis,pissadaki,boulouxou,karmiri*/) OR
 (@ypscurrentuserid  in (select UserID from #admin UNION select 2264 ))))
ORDER BY l.Id DESC


	
	
 </CommandText>
      </Query>
