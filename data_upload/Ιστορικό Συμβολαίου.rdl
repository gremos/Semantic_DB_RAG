        </QueryParameters>
        <CommandText>/*
SELECT CASE WHEN ta.fTableField1Code = '001' THEN 'GREEN' 
			WHEN ta.fTableField1Code = '002' THEN 'YELLOW' 
			WHEN ta.fTableField1Code = '003' THEN 'ORANGE' 
			WHEN ta.fTableField1Code = '004' THEN 'RED' 
		END AS Solvency
FROM dbo.ESFITradeAccount ta JOIN dbo.ESFITradeAccountContract tac ON ta.GID = tac.fTradeAccountGID
WHERE tac.Code = @ContractID
*/
SELECT cs.Description AS Solvency 
FROM dbo.Contract ct 
JOIN dbo.BusinessPoint bp ON bp.id = ct.ActualBPOwnerID
JOIN dbo.CustomerSolvency cs ON cs.id = bp.CustomerSolvencyID
WHERE ct.id = @ContractID</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
