  <DataSets>
    <DataSet Name="Main">
      <Query>
        <DataSourceName>YPS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@mediumVersion">
            <Value>=Parameters!mediumVersion.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@versionSection">
            <Value>=Parameters!versionSection.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select  
[Μέσο] = mv.name,
[Κωδ. Μέσου] = mv.MediumID,
[Κύκλος] = mv.code,
[Section] = vs.Name , 
[Κωδ. ΕΣ] = bp.code,
[Διακρτικός] = bp.Name,
[Επωνυμία] = bp.BrandName,

[Συμβόλαιο] = ct.id,
[Κατάσταση Συμβ.] = (CASE ct.state
						   WHEN 0 THEN 'Pending'
						   WHEN 1 THEN 'Approved'
						   WHEN 2 THEN 'Rejected'
						   WHEN 3 THEN 'Cancelled'
						   WHEN 4 THEN 'Replaced'
						   WHEN 5 THEN 'CancelledOffer'
						   WHEN 6 THEN 'Modified'
						 END),
[Κατάσταση Προβολής] = (CASE cp.state
						   WHEN 0 THEN 'Pending'
						   WHEN 1 THEN 'Accepted'
						   WHEN 2 THEN 'Rejected'
						   WHEN 3 THEN 'Cancelled'
						   WHEN 4 THEN 'Interrupted'
						   WHEN 5 THEN 'Completed'
						   WHEN 6 THEN 'Modified'
						   WHEN 7 THEN 'ModifiedForEBS'
						 END),
[Πωλητής] = CASE WHEN FirstName LIKE '%AD01%' THEN salesman.LastName ELSE salesman.LastName+' '+salesman.FirstName end,
[Κωδ. Πωλητή] = salesman.SalesCode,
[Bundle] = cppr.code,
[Προιόν] = pr.Code ,
[Όνομα Προιόντος] = pr.Name,
[Ημερομ. Υπογραφής] = convert(nvarchar(15),ct.SignedDate,103),
[Τιμή] =      cast (  ad.price as money),-- cp.Price,
[Έκπτωση] =        cast ( ad.discount+ad.RulesDiscount as money),
[Τιμή μετά Έκπτωση] = cast (       ad.Price-ad.discount-ad.RulesDiscount  as money),
[Μεταβεβλημένη Τιμή] = CAST ( ad.AlteredAmount as money),
[Ποσό Συμβολαίου] =     
(cast((SELECT        sum(CASE WHEN cp1.state IN (3,4) AND cp1.AlteredAmount IS NOT NULL THEN cp1.AlteredAmount else Price - Discount - RulesDiscount end)
			FROM            dbo.ContractProduct cp1
			WHERE        RelatedContractID = ct.id) as money))
        
        
from businesspoint bp 
join [contract] ct on bp.id=ct.actualbpownerid
join contractproduct cp on cp.relatedcontractid=ct.id
join product cppr on cppr.id=cp.productid
join advertisement ad on cp.id=ad.contractproductid
join versionsection vs on ad.versionsectionid=vs.id
join product pr on pr.id=ad.productid
join MediumVersion mv on mv.ID=vs.MediumVersionID
LEFT JOIN dbo.[User] salesman ON ct.SalesManID = salesman.ID
where 
mv.id = @mediumVersion
and vs.MediumVersionID = @mediumVersion
and vs.id in ( @versionSection)
--and pr.iscomposite=0
and ct.State not in (3,5,10) and ct.type in (0,2,3,4) AND cp.State &lt;&gt; 3
order by bp.code,ct.id, cp.id,ad.id</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
