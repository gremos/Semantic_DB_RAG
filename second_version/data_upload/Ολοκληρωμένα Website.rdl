  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>YPS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@from">
            <Value>=Parameters!from.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@to">
            <Value>=Parameters!to.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@option">
            <Value>=Parameters!option.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>--DECLARE @from DATE
--  , @to DATE
--  , @option BIT ;
--SET @from = '20130101' ;
--SET @to = '20130801' ;
--SET @option =1;
if object_id('tempdb..#new') is not null
drop table #new

SELECT 
        bpid
      , [Κωδ. ΕΣ] = Branch_ID
      , [Διακριτικός] = Brand_Name
      , [Επωνυμία] = Customer_Name
      , [Διεύθυνση] = [Address]
      , [ΤΚ] = Zip
      , [Δήμος] = Municipality
      , [Νομός] = Prefecture
      , prcode = prcode
      , [Πωλητής] = salesman
      , [Site Preview File] = preview
      , [Ημερ. Ολοκλήρωσης] = CONVERT(NVARCHAR(15), completed_on, 103)
      , completed_on
      , [Ημερ. Λήξης Προβολής] = Adv_EndDate
      , advid
      , cpid
      , Heading
INTO #new
FROM    ( SELECT   DISTINCT
                    bp.id AS bpid
                  , bp.code AS Branch_ID
                  , bp.Name AS Brand_Name
                  , bp.BrandName AS Customer_Name
                  , [ad].[Line1] AS [Address]
                  , [ad].GeoModelZip AS [Zip]
                  , ad.Municipality
                  , ad.Prefecture
                  , mh.Code AS internet_heading_id
                  , mh.Description AS internet_heading_description
                  , pr.code AS prcode
                  , advid = adv.ID
                  , cpid = cp.ID
                  , Heading = vgi.Description
                  , ( SELECT    username + '@yellowpages.gr'
                      FROM      [User]
                      WHERE     id = c.SalesManID
                    ) AS salesman
                  , CASE WHEN ( SELECT	MAX(tsk.completedon)
									FROM	yps.dbo.[Case] cc
											JOIN YPS.dbo.[Case] mcc ON mcc.id = cc.MasterCaseID
											JOIN yps.dbo.Task tsk ON tsk.CaseID = cc.id
									WHERE	Cc.ContextEntityID = ADV.ID
											AND cc.ContextEntityType = 'ADV'
											AND mcc.ContextEntityType = 'TGIV'
											AND tsk.TaskTypeID=192
								  ) IS NULL  
								  AND c.state NOT IN ( 3, 5 , 10)
								  AND adv.QA = 1 AND c.MigratedCode IS NOT NULL
							 THEN adv.AdvBeginDate
							 when c.OrigionalContractID IS Not null then adv.AdvBeginDate
							  ELSE ( SELECT	MAX(tsk.completedon)
									FROM	yps.dbo.[Case] cc
											JOIN YPS.dbo.[Case] mcc ON mcc.id = cc.MasterCaseID and  mcc.ContextEntityType = 'TGIV'
											JOIN yps.dbo.Task tsk ON tsk.CaseID = cc.id
											join yps.dbo.Advertisement adv2 on adv2.ID=cc.ContextEntityID AND cc.ContextEntityType = 'ADV'
									WHERE   adv2.QA=1 and adv.ID=adv2.id
									and tsk.TaskTypeID IN (193,516)
									and tsk.Outcome  in  ('Σύνθετη καταχώρηση δεν παρουσιάζει λάθη','Δεν Απαιτούνται Διορθώσεις') 
								  )
						END AS completed_on 
                  , isnull(cp.interruptionDate,fccv2.Adv_EndDate) Adv_EndDate
                  , '\\junco12\YPSDATA\CaseAttachments\' + fccv2.Cav_Value AS preview
          FROM      yps.dbo.BusinessPoint bp
                    JOIN FullAddressViewV2 ad ON bp.LocationAddressID = ad.id
                    JOIN yps.dbo.Contract c ON bp.ID = c.ActualBPOwnerID AND c.OrigionalContractID IS NULL AND c.OriginalContractType IS NULL
                    JOIN yps.dbo.ContractProduct cp ON c.ID = cp.RelatedContractID
                    JOIN yps.dbo.Advertisement adv ON adv.ContractProductID = cp.ID
                    JOIN yps.dbo.Product pr ON adv.ProductID = pr.ID
                    JOIN yps.dbo.udac ud ON pr.UdacID = ud.ID
                    JOIN yps.dbo.VersionGroupItem vgi ON vgi.ID = adv.HeadingID
                    JOIN yps.dbo.MasterHeading mh ON mh.ID = vgi.SourceHeadingID
                    JOIN yps.dbo.FullContractCavView fccv2 ON adv.id = fccv2.Adv_ID
                                                              AND fccv2.Cad_Name in ('Εικόνα Web','ScreenShot for Cred')
          WHERE     pr.MediumVersionID IN ( SELECT  id
                                            FROM    MediumVersion
                                            WHERE   Mediumid = 9 )
                    AND c.type IN ( 0, 2, 3, 4 )
                    AND c.state IN (1,6)
					AND cp.State NOT IN (3,4)
        ) AS xx
WHERE (completed_on between @from and @to) OR (completed_on is null AND @option = 1)      

