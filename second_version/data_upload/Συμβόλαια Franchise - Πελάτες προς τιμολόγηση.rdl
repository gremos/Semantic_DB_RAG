        <CommandText> 
USE yps;
--DECLARE	--@campaign NVARCHAR(20) ,
--	@Campaign INT ,
--	@from DATE ,
--	@to DATE;
    
--SET @Campaign = 43;
--SET @from = DATEADD(DAY, -7, CONVERT(DATE, GETDATE()));  
--SET @to = DATEADD(DAY, -1, CONVERT(DATE, GETDATE()));

IF OBJECT_ID('tempdb..#CtrsBeforeThreshold') IS NOT NULL 
	DROP TABLE #CtrsBeforeThreshold;

IF OBJECT_ID('tempdb..#CtrsAfterThreshold') IS NOT NULL 
	DROP TABLE #CtrsAfterThreshold;
    
IF OBJECT_ID('tempdb..#ModCtrsBeforeThreshold') IS NOT NULL 
	DROP TABLE #ModCtrsBeforeThreshold;
    
IF OBJECT_ID('tempdb..#ModCtrsAfterThreshold') IS NOT NULL 
	DROP TABLE #ModCtrsAfterThreshold;
    
IF OBJECT_ID('tempdb..#b') IS NOT NULL 
	DROP TABLE #b;            
    
----------------------------------Contracts After Threshold---------------------------------------------------        
SELECT DISTINCT
		cmp.Name AS 'Καμπάνια' ,
		bp.code AS 'Κωδικός Επιχ. Σημείου' ,
		pr.TargetBussinessPointID ,
		ct.id AS 'Συμβόλαιο' ,
		prbp.BrandName AS 'Φορολογική Επωνυμία' ,
		prd.code AS 'Κωδ. Προϊόντος' ,
		adv.AdvBeginDate AS 'Ημερ. Έναρξης' ,
		adv.AdvEndDate AS 'Λήξη Προβολής' ,
		cp.InterruptionDate AS 'Ημερ. Διακοπής/Ακύρωσης' ,
		'Τελική Αξία Προϊόντος Προ ΦΠΑ' = CASE WHEN prd.id = apr.id THEN ( SELECT	CASE WHEN cp.state IN ( 3, 4, 6, 7 )
																							  AND cp.AlteredAmount IS NOT NULL THEN cp.AlteredAmount
																						 ELSE icp.Price - ( icp.Discount + icp.RulesDiscount )
																					END
																		   FROM		dbo.ContractProduct icp
																		   WHERE	icp.id = cp.id
																		 )
											   ELSE NULL
										  END ,
		CASE WHEN prd.IsComposite = 1
				  AND prd.id &lt;&gt; apr.id THEN apr.Code + '//' + apr.Name
			 ELSE NULL
		END AS 'Περιεχόμενα Bundle' ,
		m.name AS 'Μέσο Προβολής' ,
		mv.code AS 'Έκδοση' ,
		cp.id AS cpid ,
		adv.id AS advid ,
		'Νέο' AS Modified ,
		cp.[state] AS CpState ,
		cp.AlteredAmount
INTO	#CtrsAfterThreshold
FROM	dbo.Campaign cmp
		JOIN dbo.TargetGroupItem tgi ON cmp.ID = tgi.CampaignID
		JOIN dbo.BusinessPoint bp ON tgi.BusinessPointID = bp.ID
		JOIN dbo.PointRelationship pr ON bp.ID = pr.BusinessPointID
		JOIN dbo.BusinessPoint prbp ON pr.TargetBussinessPointID = prbp.ID
									   AND pr.type = 10
		JOIN dbo.TargetGroupItemValue tgiv ON tgi.ID = tgiv.TargetGroupItemID
		JOIN dbo.Contract ct ON ISNULL(tgiv.ContractID, tgiv.AltOfferID) = ct.ID
		LEFT JOIN dbo.InvoicePaymentMethod ipm ON ct.ID = ipm.ID
		JOIN dbo.ContractProduct cp ON ct.ID = cp.RelatedContractID
		JOIN dbo.Advertisement adv ON cp.ID = adv.ContractProductID
		JOIN dbo.Product apr ON adv.ProductID = apr.ID
		JOIN dbo.VersionSection vs ON adv.VersionSectionID = vs.ID
		JOIN dbo.MediumVersion mv ON vs.MediumVersionID = mv.ID
		JOIN dbo.Medium m ON mv.MediumID = m.ID
		JOIN dbo.Product prd ON cp.ProductID = prd.ID
		JOIN dbo.CampaignAllocationPlan cap ON tgiv.CampaignAllocationPlanID = cap.ID
											   AND cmp.SelectedAllocationPlanID = cap.ID
WHERE	cmp.id = @Campaign
		AND adv.QA = 1
		AND ct.State IN ( 1, 6 )
		AND cp.State &lt;&gt; 3
		AND ct.Type NOT IN ( 1, 10 )
		AND ct.CreatedBy NOT IN ( 'SubScriptionRenewalALL', 'SubScriptionRenewal' )
		AND ipm.PaymentMonth IS NOT NULL
		AND ipm.PaymentMonth BETWEEN @from AND @to
----------------------------------Contracts Before Threshold---------------------------------------------------        

SELECT DISTINCT
		cmp.Name AS 'Καμπάνια' ,
		bp.code AS 'Κωδικός Επιχ. Σημείου' ,
		pr.TargetBussinessPointID ,
		ct.id AS 'Συμβόλαιο' ,
		prbp.BrandName AS 'Φορολογική Επωνυμία' ,
		prd.code AS 'Κωδ. Προϊόντος' ,
		adv.AdvBeginDate AS 'Ημερ. Έναρξης' ,
		adv.AdvEndDate AS 'Λήξη Προβολής' ,
		cp.InterruptionDate AS 'Ημερ. Διακοπής/Ακύρωσης' ,
		'Τελική Αξία Προϊόντος Προ ΦΠΑ' = CASE WHEN prd.id = apr.id THEN ( SELECT	CASE WHEN cp.state IN ( 3, 4, 6, 7 )
																							  AND cp.AlteredAmount IS NOT NULL THEN cp.AlteredAmount
																						 ELSE icp.Price - ( icp.Discount + icp.RulesDiscount )
																					END
																		   FROM		dbo.ContractProduct icp
																		   WHERE	icp.id = cp.id
																		 )
											   ELSE NULL
										  END ,
		CASE WHEN prd.IsComposite = 1
				  AND prd.id &lt;&gt; apr.id THEN apr.Code + '//' + apr.Name
			 ELSE NULL
		END AS 'Περιεχόμενα Bundle' ,
		m.name AS 'Μέσο Προβολής' ,
		mv.code AS 'Έκδοση'         --,pm.PaymentMonth     
		,
		cp.id AS cpid ,
		adv.id AS advid ,
		'Νέο' AS Modified ,
		cp.[state] AS CpState ,
		cp.AlteredAmount
INTO	#CtrsbeforeThreshold
FROM	dbo.Campaign cmp
		JOIN dbo.TargetGroupItem tgi ON cmp.ID = tgi.CampaignID
		JOIN dbo.BusinessPoint bp ON tgi.BusinessPointID = bp.ID
		JOIN dbo.PointRelationship pr ON bp.ID = pr.BusinessPointID
		JOIN dbo.BusinessPoint prbp ON pr.TargetBussinessPointID = prbp.ID
									   AND pr.type = 10
		JOIN dbo.TargetGroupItemValue tgiv ON tgi.ID = tgiv.TargetGroupItemID
		JOIN dbo.Contract ct ON ISNULL(tgiv.ContractID, tgiv.AltOfferID) = ct.ID
		JOIN dbo.ContractProduct cp ON ct.ID = cp.RelatedContractID
		JOIN dbo.PaymentMethod pm ON cp.PaymentMethodID = pm.ID
		JOIN dbo.Advertisement adv ON cp.ID = adv.ContractProductID
		JOIN dbo.Product apr ON adv.ProductID = apr.ID
		JOIN dbo.VersionSection vs ON adv.VersionSectionID = vs.ID
		JOIN dbo.MediumVersion mv ON vs.MediumVersionID = mv.ID
		JOIN dbo.Medium m ON mv.MediumID = m.ID
		JOIN dbo.Product prd ON cp.ProductID = prd.ID
		JOIN dbo.CampaignAllocationPlan cap ON tgiv.CampaignAllocationPlanID = cap.ID
											   AND cmp.SelectedAllocationPlanID = cap.ID
WHERE	cmp.id = @Campaign
		AND adv.QA = 1
		AND ct.State IN ( 1, 6 )
		AND cp.State &lt;&gt; 3
		AND ct.Type NOT IN ( 1, 10 )
		AND ct.CreatedBy NOT IN ( 'SubScriptionRenewalALL', 'SubScriptionRenewal' )
		AND pm.PaymentMonth BETWEEN @from AND @to
ORDER BY [Κωδικός Επιχ. Σημείου] ,
		[Κωδ. Προϊόντος] ,
		7 ,
		8 DESC;
        
 ----------------------------------Modified Contracts After Threshold---------------------------------------------------               
        
SELECT DISTINCT
		cmp.Name AS 'Καμπάνια' ,
		bp.code AS 'Κωδικός Επιχ. Σημείου' ,
		pr.TargetBussinessPointID ,
		ct.id AS 'Συμβόλαιο' ,
		prbp.BrandName AS 'Φορολογική Επωνυμία' ,
		prd.code AS 'Κωδ. Προϊόντος' ,
		adv.AdvBeginDate AS 'Ημερ. Έναρξης' ,
		adv.AdvEndDate AS 'Λήξη Προβολής' ,
		cp.InterruptionDate AS 'Ημερ. Διακοπής/Ακύρωσης' ,
		'Τελική Αξία Προϊόντος Προ ΦΠΑ' = CASE WHEN prd.id = apr.id THEN ( SELECT	CASE WHEN cp.state IN ( 3, 4, 6, 7 )
																							  AND cp.AlteredAmount IS NOT NULL THEN cp.AlteredAmount
																						 ELSE icp.Price - ( icp.Discount + icp.RulesDiscount )
																					END
																		   FROM		dbo.ContractProduct icp
																		   WHERE	icp.id = cp.id
																		 )
											   ELSE NULL
										  END ,
		CASE WHEN prd.IsComposite = 1
				  AND prd.id &lt;&gt; apr.id THEN apr.Code + '//' + apr.Name
			 ELSE NULL
		END AS 'Περιεχόμενα Bundle' ,
		m.name AS 'Μέσο Προβολής' ,
		mv.code AS 'Έκδοση' ,
		cp.id AS cpid ,
		adv.id AS advid ,         --,ipm.PaymentMonth 
		'Μεταβολή' AS Modified ,
		cp.[state] AS CpState ,
		cp.AlteredAmount
INTO	#ModCtrsAfterThreshold
FROM	dbo.Campaign cmp
		JOIN dbo.TargetGroupItem tgi ON cmp.ID = tgi.CampaignID
		JOIN dbo.BusinessPoint bp ON tgi.BusinessPointID = bp.ID
		JOIN dbo.PointRelationship pr ON bp.ID = pr.BusinessPointID
		JOIN dbo.BusinessPoint prbp ON pr.TargetBussinessPointID = prbp.ID
									   AND pr.type = 10
		JOIN dbo.TargetGroupItemValue tgiv ON tgi.ID = tgiv.TargetGroupItemID
		JOIN dbo.Contract ct ON ISNULL(tgiv.ContractID, tgiv.AltOfferID) = ct.ID
		LEFT JOIN dbo.InvoicePaymentMethod ipm ON ct.ID = ipm.ID
		JOIN dbo.ContractProduct cp ON ct.ID = cp.RelatedContractID
		JOIN dbo.Advertisement adv ON cp.ID = adv.ContractProductID
		JOIN dbo.Product apr ON adv.ProductID = apr.ID
		JOIN dbo.VersionSection vs ON adv.VersionSectionID = vs.ID
		JOIN dbo.MediumVersion mv ON vs.MediumVersionID = mv.ID
		JOIN dbo.Medium m ON mv.MediumID = m.ID
		JOIN dbo.Product prd ON cp.ProductID = prd.ID
		JOIN dbo.CampaignAllocationPlan cap ON tgiv.CampaignAllocationPlanID = cap.ID
											   AND cmp.SelectedAllocationPlanID = cap.ID
WHERE	cmp.id = @Campaign
		AND adv.QA = 1
		AND ct.State IN ( 1, 6 )
                    --AND cp.State &lt;&gt; 3
		AND ct.Type NOT IN ( 1, 10 )
		AND ct.CreatedBy NOT IN ( 'SubScriptionRenewalALL', 'SubScriptionRenewal' )
		AND ipm.PaymentMonth IS NOT NULL
		AND ipm.PaymentMonth &lt; @from
		AND EXISTS ( SELECT	1
					 FROM	dbo.Contract ict
							JOIN dbo.ContractProduct icp ON ict.ID = icp.RelatedContractID
					 WHERE	ict.id = ct.ID
							AND icp.State IN ( 3, 4 ) )
		AND ct.ModifiedOn BETWEEN DATEADD(day, -7, @from) AND @from			     
        
  
  
  -------------------------------Modified Contracts Before Threshold------------------------------------------------------      
  
  
SELECT DISTINCT
		cmp.Name AS 'Καμπάνια' ,
		bp.code AS 'Κωδικός Επιχ. Σημείου' ,
		pr.TargetBussinessPointID ,
		ct.id AS 'Συμβόλαιο' ,
		prbp.BrandName AS 'Φορολογική Επωνυμία' ,
		prd.code AS 'Κωδ. Προϊόντος' ,
		adv.AdvBeginDate AS 'Ημερ. Έναρξης' ,
		adv.AdvEndDate AS 'Λήξη Προβολής' ,
		cp.InterruptionDate AS 'Ημερ. Διακοπής/Ακύρωσης' ,
		'Τελική Αξία Προϊόντος Προ ΦΠΑ' = CASE WHEN prd.id = apr.id THEN ( SELECT	CASE WHEN cp.state IN ( 3, 4, 6, 7 )
																							  AND cp.AlteredAmount IS NOT NULL THEN cp.AlteredAmount
																						 ELSE icp.Price - ( icp.Discount + icp.RulesDiscount )
																					END
																		   FROM		dbo.ContractProduct icp
																		   WHERE	icp.id = cp.id
																		 )
											   ELSE NULL
										  END ,
		CASE WHEN prd.IsComposite = 1
				  AND prd.id &lt;&gt; apr.id THEN apr.Code + '//' + apr.Name
			 ELSE NULL
		END AS 'Περιεχόμενα Bundle' ,
		m.name AS 'Μέσο Προβολής' ,
		mv.code AS 'Έκδοση'         --,pm.PaymentMonth     
		,
		cp.id AS cpid ,
		adv.id AS advid ,
		'Μεταβολή' AS Modified ,
		cp.[state] AS CpState ,
		cp.AlteredAmount
INTO	#ModCtrsbeforeThreshold
FROM	dbo.Campaign cmp
		JOIN dbo.TargetGroupItem tgi ON cmp.ID = tgi.CampaignID
		JOIN dbo.BusinessPoint bp ON tgi.BusinessPointID = bp.ID
		JOIN dbo.PointRelationship pr ON bp.ID = pr.BusinessPointID
		JOIN dbo.BusinessPoint prbp ON pr.TargetBussinessPointID = prbp.ID
									   AND pr.type = 10
		JOIN dbo.TargetGroupItemValue tgiv ON tgi.ID = tgiv.TargetGroupItemID
		JOIN dbo.Contract ct ON ISNULL(tgiv.ContractID, tgiv.AltOfferID) = ct.ID
		JOIN dbo.ContractProduct cp ON ct.ID = cp.RelatedContractID
		JOIN dbo.PaymentMethod pm ON cp.PaymentMethodID = pm.ID
		JOIN dbo.Advertisement adv ON cp.ID = adv.ContractProductID
		JOIN dbo.Product apr ON adv.ProductID = apr.ID
		JOIN dbo.VersionSection vs ON adv.VersionSectionID = vs.ID
		JOIN dbo.MediumVersion mv ON vs.MediumVersionID = mv.ID
		JOIN dbo.Medium m ON mv.MediumID = m.ID
		JOIN dbo.Product prd ON cp.ProductID = prd.ID
		JOIN dbo.CampaignAllocationPlan cap ON tgiv.CampaignAllocationPlanID = cap.ID
											   AND cmp.SelectedAllocationPlanID = cap.ID
WHERE	cmp.id = @Campaign
		AND adv.QA = 1
		AND ct.State IN ( 1, 6 )
                    --AND cp.State &lt;&gt; 3
		AND ct.Type NOT IN ( 1, 10 )
		AND ct.CreatedBy NOT IN ( 'SubScriptionRenewalALL', 'SubScriptionRenewal' )
		AND pm.PaymentMonth IS NOT NULL
		AND pm.PaymentMonth &lt; @from
		AND EXISTS ( SELECT	1
					 FROM	dbo.Contract ict
							JOIN dbo.ContractProduct icp ON ict.ID = icp.RelatedContractID
					 WHERE	ict.id = ct.ID
							AND icp.State IN ( 3, 4 ) )
		AND ct.ModifiedOn BETWEEN DATEADD(day, -7, @from) AND @from
ORDER BY [Κωδικός Επιχ. Σημείου] ,
		[Κωδ. Προϊόντος] ,
		7 ,
		8 DESC;
  
  
  
  
  
  
  ---------------------------------------------------------------------------------------------------------------------      
     
SELECT	*
INTO	#b
FROM	( SELECT	*
		  FROM		#CtrsBeforeThreshold
		  UNION
		  SELECT	*
		  FROM		#CtrsAfterThreshold
		  UNION
		  SELECT	*
		  FROM		#ModCtrsAfterThreshold
		  UNION
		  SELECT	*
		  FROM		#ModCtrsBeforeThreshold
		) a;
        
  
        
WITH	details
		  AS ( SELECT	bp.Code AS [YPS Code] ,
						ISNULL(bp.BrandName, ') AS [Επωνυμία] 
                                          ,ISNULL(bp.Name, ') AS [Διακριτικός] ,
						bp.Profession AS [Επάγγελμα] ,
						( SELECT TOP 1
									bl.AreaName
						  FROM		yps.dbo.BroaderArea ba
									JOIN yps.dbo.BroaderAreaLookUp bl ON ba.BroaderAreaID = bl.BroaderAreaID
						  WHERE		ba.AddressID = ad.ID
									AND ba.BroaderAreaID LIKE 'E%'
						  ORDER BY	ba.BroaderAreaID DESC
						) AS [Νομός] ,
						ad.GeoModelLocText AS [Πόλη] ,
						ISNULL(ad.GeoModelStreet1Text, Line1) AS [Διεύθυνση] ,
						ad.GeoModelStreet1Num AS [Αριθμός] ,
						ad.GeoModelZip AS [ΤΚ]
			   FROM		yps.dbo.BusinessPoint bp
						JOIN yps.dbo.Address ad ON ad.ID = bp.LocationAddressID
						JOIN yps.dbo.ClassifierNode cn ON cn.ID = bp.ActivityClassifierNodeID
						LEFT JOIN yps.dbo.TaxCode tx ON bp.ID = tx.id
						LEFT JOIN yps.dbo.TaxOffice tf ON tf.ID = bp.TaxOfficeID
						LEFT JOIN yps.dbo.PointEmail em ON em.BusinessPointID = bp.id
						LEFT JOIN yps.dbo.CustomerSource src ON src.ID = bp.CustomerSourceID
						LEFT JOIN yps.dbo.CustomerSolvency slv ON slv.ID = bp.CustomerSolvencyID
			   WHERE	bp.IsActive = 1
						AND ISNULL(bp.CustomerSourceID, '') &lt;&gt; 16
						AND ad.CountryID = 1
			   GROUP BY	bp.ID ,
						ad.ID ,
						bp.Code ,
						bp.BrandName ,
						bp.Name ,
						bp.Profession ,
						tx.TaxCode ,
						tf.Description ,
						ad.GeoModelStreet1Text ,
						ad.Line1 ,
						ad.GeoModelStreet1Num ,
						ad.GeoModelStreet2Text ,
						ad.GeoModelStreet2Num ,
						ad.GeoModelZip ,
						ad.GeoModelLocText ,
						src.Description ,
						slv.Description ,
						cn.Name
			 )
	SELECT	#b.[Καμπάνια] ,
			#b.[Κωδικός Επιχ. Σημείου] ,
			#b.[Φορολογική Επωνυμία] ,
			#b.[Συμβόλαιο] ,
			tc.TaxCode AS 'ΑΦΜ' ,
			d.[Διεύθυνση] ,
			d.[Αριθμός] ,
			d.[Πόλη] ,
			d.[Νομός] ,
			d.[ΤΚ] ,
			#b.[Κωδ. Προϊόντος] ,
			CONVERT(NVARCHAR(15), #b.[Ημερ. Έναρξης], 103) AS [Ημερ. Έναρξης] ,
			CONVERT(NVARCHAR(15), #b.[Λήξη Προβολής], 103) AS [Λήξη Προβολής] ,
			CONVERT(NVARCHAR(15), #b.[Ημερ. Διακοπής/Ακύρωσης], 103) AS [Ημερ. Διακοπής/Ακύρωσης] ,
			CAST(#b.[Τελική Αξία Προϊόντος Προ ΦΠΑ] AS MONEY) AS [Τελική Αξία Προϊόντος Προ ΦΠΑ] ,
			#b.[Περιεχόμενα Bundle] ,
			#b.[Μέσο Προβολής] ,
			#b.[Έκδοση] ,
			#b.modified AS [Χαρακτηρισμός Συμβολαίου] ,
			CASE WHEN #b.modified = 'Μεταβολή'
					  AND CpState = 3 THEN 'Ακύρωση'
				 WHEN #b.modified = 'Μεταβολή'
					  AND CpState = 4 THEN 'Διακοπή'  --,cpid, advid          
				 WHEN #b.modified = 'Μεταβολή'
					  AND CpState = 3 THEN 'Ολοκληρωμένη'
			END AS 'Κατάσταση Γραμμής' ,
			#b.AlteredAmount AS 'Μεταβεβλημένη Αξία Προ ΦΠΑ'
	FROM	#b
			JOIN dbo.BusinessPoint bp ON bp.id = #b.targetbussinesspointid
			LEFT JOIN dbo.TaxCode tc ON tc.ID = bp.id
			LEFT JOIN details d ON bp.code = d.[YPS Code]   
  --WHERE [Κωδικός Επιχ. Σημείου] = '340622'          
	ORDER BY [Κωδικός Επιχ. Σημείου] ,
			[Κωδ. Προϊόντος] ,
			cpid ,
			advid DESC  </CommandText>
      </Query>
