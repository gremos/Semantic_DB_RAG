      <Query>
        <DataSourceName>EBS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@_FROM_DATECRITERIO">
            <Value>=Parameters!ID_FROM_DATECRITERIO.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@_TO_DATECRITERIO">
            <Value>=Parameters!ID_TO_DATECRITERIO.Value</Value>
