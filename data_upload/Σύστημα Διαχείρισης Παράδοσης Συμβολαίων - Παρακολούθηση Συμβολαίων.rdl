  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@HistoricElements">
            <Value>=Parameters!HistoricElements.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DateFrom">
            <Value>=Parameters!DateFrom.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DateTo">
            <Value>=Parameters!DateTo.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select 
	[Ημ.Μεταφοράς] = ci.CreatedOn,
	[Βήμα] = t.Description,
	[Ραντεβού] = convert(nvarchar(50),ti.AppointmentDateFrom , 103) + ' ' + convert(nvarchar(50),ti.AppointmentDateFrom , 108)
			+' - ' + convert(nvarchar(50),ti.AppointmentDateTo , 103) + ' ' + convert(nvarchar(50),ti.AppointmentDateTo , 108),
	[Κωδ ΕΣ] = ci.BusinessPointCode,
	[Κωδ Συμβολαίου] = ci.ContractId,
	[Πωλητής] = isnull(u.SalesCode + ' - ','') + u.LastName,
	[Επωνυμία]=ci.BusinessPointName,
	[Διακριτικός]=ci.BusinessPointBrandName,
	[Αξία Συμβολαίου(χωρίς ΦΠΑ)]=cast(ci.ContractValue as money),
	[Τρόπος Πληρωμής Συμβολαίου] = ci.ContractPaymentMethod,
	[Αξία Προκαταβολής]=cast(ci.AdvancedPaymentAmount as money),
	[Αφορά σε Εξόφληση] = case when ci.AdvancedPaymentAmount &gt; 0 then 1 else 0 end,
	[Εξόφληση Από Εισπράκτορα] = ci.IsCollectorPayoff,
	[Σχόλια Πωλήσεων] = ci.CaseComments
from cdm.[Case] cs
	join cdm.CaseInfo ci on ci.CaseId = cs.Id
	join yps.dbo.Contract ct on ct.id = ci.ContractId
	join yps.dbo.[User] u on u.id = ct.SalesManID
	join cdm.Task t on t.CaseId = cs.Id
	join cdm.TaskInfo ti on ti.TaskId = t.Id
where
	(@HistoricElements = 'False' and t.Closed = 0)
	or
	(@HistoricElements = 'True'
		and t.id = (select max(tin.id)
					from cdm.[Case] csin
						join cdm.Task tin on tin.CaseId = csin.Id
					where csin.id = cs.Id)
		and cast(ci.CreatedOn as date) between @DateFrom and @DateTo)
order by ci.ContractId asc, ci.CreatedOn desc</CommandText>
      </Query>
