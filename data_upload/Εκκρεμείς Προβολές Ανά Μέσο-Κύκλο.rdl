  <DataSets>
    <DataSet Name="OpenAdvsPerMediumDataSet">
      <Query>
        <DataSourceName>YPS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@MediumCode">
            <Value>=Parameters!MediumCode.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText> if object_id('tempdb..#CustomerData') is not null
drop table #CustomerData
 SELECT DISTINCT
                        bp.Code AS BPCode,
                        bp.Name AS BPName,
                        ctPrimary.ID,
                        p.Code AS PCode,
                        p.Name AS PName,
                        ISNULL(taTGIV.ID, taAdv.ID) AS TaskID, u.FirstName + ' ' + u.LastName AS SalesMan,
						adv.AdvBeginDate AS StartDate, ctPrimary.SignedDate AS SignDate,
adv.id as advid  ,
(select ch.Description from dbo.channeltype ch where ch.id=ctPrimary.ChannelTypeID)  salesChannel                                                        

				into #CustomerData
               FROM     dbo.Contract ctPrimary
						JOIN dbo.[User] u ON ctPrimary.SalesManID = u.ID
                        JOIN dbo.BusinessPoint bp ON ctPrimary.ActualBPOwnerID = bp.ID
                        JOIN dbo.ContractProduct cp ON ctPrimary.ID = cp.RelatedContractID
                        JOIN dbo.Advertisement adv ON cp.ID = adv.ContractProductID
                        JOIN dbo.Product p ON adv.ProductID = p.ID
                                              AND p.IsComposite = 0
                        JOIN dbo.VersionSection vs ON adv.VersionSectionID = vs.ID
                        JOIN dbo.MediumVersion mv ON vs.MediumVersionID = mv.ID
                        JOIN dbo.Medium m ON mv.MediumID = m.ID
                        LEFT JOIN dbo.TargetGroupItemValue tgiv ON ctPrimary.id = ISNULL(tgiv.ContractID, tgiv.AltOfferID)
                        LEFT JOIN dbo.[Case] caAdv ON caAdv.ContextEntityID = adv.ID
                                                      AND caAdv.ContextEntityType = 'ADV'
                        LEFT JOIN dbo.Task taAdv ON caAdv.ID = taAdv.CaseID
                                                    AND taAdv.id = ( SELECT TOP 1
                                                                            taAdv2.ID
                                                                     FROM   dbo.Task taAdv2
                                                                            JOIN dbo.TaskType ttAdv ON taAdv2.TaskTypeID = ttAdv.ID
                                                                     WHERE  taAdv.CaseID = taAdv2.CaseID
                                                                            AND ttAdv.ActivityType = 'H'
                                                                            AND taAdv2.Status = 0
                                                                     ORDER BY taAdv2.id DESC
                                                                   )
                        LEFT JOIN dbo.[Case] caTGIV ON caTGIV.ContextEntityID = tgiv.ID
                                                       AND caTGIV.ContextEntityType = 'TGIV'
                        LEFT JOIN dbo.Task taTGIV ON caTGIV.id = taTGIV.CaseID
                                                     AND taTGIV.id = ( SELECT TOP 1
                                                                                taTGIV2.ID
                                                                       FROM     dbo.Task taTGIV2
                                                                                JOIN dbo.TaskType ttTGIV ON taTGIV2.TaskTypeID = ttTGIV.ID
                                                                       WHERE    taTGIV.CaseID = taTGIV2.CaseID
                                                                                AND ttTGIV.ActivityType = 'H'
                                                                                AND taTGIV2.Status = 0
                                                                       ORDER BY taTGIV2.id DESC
                                                                     )
               WHERE    adv.QA = 0
                        AND adv.VersionSectionID in (
													SELECT  vs2.ID
													FROM    dbo.Medium m2
															JOIN dbo.MediumVersion mv2 ON m2.ID = mv2.MediumID
															JOIN dbo.VersionSection vs2 ON mv2.ID = vs2.MediumVersionID
													WHERE   m2.IsActive = 1
															AND mv2.IsActive = 1
															--AND MediumTypeID = 3
															AND mv2.ID =@MediumCode
															)
                        AND ctPrimary.State not in  (3,5) AND (ctPrimary.type IN (0, 2, 3, 4)) AND (cp.State &lt;&gt; 3)

             

if object_id('tempdb..#result') is not null
drop table #result

    SELECT DISTINCT
            cd.BPCode,
            cd.BPName,
            cd.id AS ContractID,
            cd.PCode,
            cd.PName,
            t.CreatedOn,
            t.Description AS TAction,
            r.Description AS TRole,
            u.UserName, SalesMan, StartDate, SignDate, advid,salesChannel   
	into #result
    FROM    #CustomerData cd
            left JOIN dbo.Task t ON cd.TaskID = t.ID
            LEFT JOIN dbo.TaskAssignment ta ON t.ID = ta.TaskID
                                               AND ta.AssignmentType = 0
            LEFT JOIN dbo.Role r ON ta.RoleID = r.ID
            LEFT JOIN dbo.[User] u ON t.ActualOwnerID = u.ID
    WHERE  PCode IS NOT NULL

	select * 
	from #result r
	where TAction is not null
	union 
	select * 
	from #result r
	where TAction is  null and r.advid not in  (select r0.advid from #result r0 where r0.CreatedOn is not null)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
