    <DataSet Name="MainDataSet">
      <Query>
        <DataSourceName>YPS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Solvency">
            <Value>=Parameters!SolvencyParameter.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@Score">
            <Value>=Parameters!ScoreParameter.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@AmountFrom">
            <Value>=Parameters!AmountFrom.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@AmountTo">
            <Value>=Parameters!AmountTo.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>declare @allscores int = 
(
SELECT case when count(cn.id) &gt; 0 then 0 else 1 end
from
   ClassifierNode cn
   JOIN Classifier cl ON cn.ClassifierTreeID = cl.ClassifierTreeID
  WHERE Cl.ID = 13 and cn.id not in (@Score)
)

select 
	  [Φερεγγυότητα] = s.Description
	, [Score] = sc.Name
	, [Κωδικός Πελάτη] = tbp.Code  
	, [ΑΦΜ] = tc.TaxCode
	, [Επωνυμία] = tbp.Name --Η επωνυμία του πελάτη 
	, [Επάγγελμα] = tbp.Profession --Η περιγραφή του επαγγέλματος 
	, [Τηλέφωνο 1] = (select top 1 Number from PointPhone where isactive = 1 and BusinessPointid = tbp.id order by id)
	, [Τηλέφωνο 2] = (select  Number from PointPhone where isactive = 1 and BusinessPointid = tbp.id order by id OFFSET 1 ROWS FETCH NEXT 1 ROWS ONLY) 
	, [Διεύθυνση] = concat(ad.GeoModelStreet1Text, ' ', ad.GeoModelStreet1Num)
	, [Πόλη] = 	ad.GeoModelLocText
	, [Νομός] = ad.GeoModelDistrict
	, [ΤΚ] = ad.NumericZip 
	, [Portfolio] = ch.PortfolioDescription --Portfolio στο οποίο ανήκειο χρήστης
	, [Χρήστης] = ch.LastCallResultUser--	Ο χρήστης εισαγωγής τελευταίου Action 
	, [Ημερομηνία Τελευταίου Action] = ch.LastCallResultDate --	Ημερομηνία εισαγωγής για το τελευταίο καταχωρημένο Action
	, [ActionCode] = ch.LastCallResultID --	Κωδικός για το τελευταίο καταχωρημένο Action
	, [Περιγραφή] = ch.LastCallResultDescription --	Περιγραφή για το τελευταίο καταχωρημένο Action
	, [Ληξιπρόθεσμο Υπόλοιπο] =  ISNULL(ch.OverdueBalance, 0)--	Συνολικό ληξιπρόθεσμο ποσό του πελάτη 
	, [Λογιστικό Υπόλοιπο] = ISNULL(ch.AccountingBalance, 0) --	Συνολικό ανοιχτό λογιστικό υπόλοιπο του πελάτη 
	--, [Ημερομηνία Αλλαγής Φερεγγυότητας] = 	
	, [Ημέρες καθυστέρησης] = ISNULL(ch.DelayDays, 0) --	Ο μέγιστος αριθμός των Ημερών Καθυστέρησης των παραστατικών του πελάτη
	, [Επισφάλεια/Νομικό] = ch.Episfalia --	Check/Unchecked ανάλογα με το αν ο πελάτης έχει χαρακτηρισμό Επισφάλεια/Νομικό
	, [Κατηγορία Πελάτη] = 	 (select cn.name from Classification cf join ClassifierNode cn on cf.ClassifierNodeID = cn.id and cn.ClassifierTreeID = 6 where cf.BusinessPointID = tbp.id)
from (
	select distinct tbp.id, tbp.code, tbp.Name, tbp.Profession, tbp.CustomerSolvencyID, tbp.LocationAddressID
	from BusinessPoint bp 
	JOIN PointRelationship pr ON bp.ID = pr.BusinessPointID AND pr.Type = 10
	JOIN BusinessPoint tbp ON pr.TargetBussinessPointID = tbp.ID
	where tbp.IsActive = 1 
	group by tbp.id, tbp.code, tbp.Name, tbp.Profession, tbp.CustomerSolvencyID, tbp.LocationAddressID
) as tbp 
join CustomerSolvency s on s.id = tbp.CustomerSolvencyID 
join TaxCode tc on tc.id = tbp.id 
left join Address ad on ad.id = tbp.LocationAddressID 
left join BroaderArea ba on ba.AddressID = ad.id 
left join cardinal12.OC_XO_UPGRADE.dbo.XO_Col_LastCallHistory ch on ch.CustomerCode = tbp.Code collate Greek_CI_AI
left join (
	select cn.id, cn.name, cf.BusinessPointID
	from Classification cf 
	join ClassifierNode cn on cf.ClassifierNodeID = cn.id and cn.ClassifierTreeID = 12
) sc on sc.BusinessPointID = tbp.id 
where 
 (s.id in (@Solvency))
and (sc.id in (@Score) or @allscores = 1) --εάν είναι επιλεγμένα όλα τα scrores, τότε θα έρθουν κι οι πελάτες που έχουν null score
and (@AmountFrom is null or ch.AccountingBalance &gt;= @AmountFrom)
and (@AmountTo is null or ch.AccountingBalance &lt;= @AmountTo)
group by s.Description,  tbp.Code, tbp.id, tc.TaxCode, tbp.Name, sc.Name, tbp.Profession, ad.GeoModelLocText, ad.GeoModelDistrict, ad.NumericZip, ad.GeoModelStreet1Num, ad.GeoModelStreet1Text ,
ch.PortfolioDescription, ch.LastCallResultUser, ch.LastCallResultDate, ch.LastCallResultID, ch.LastCallResultDescription, ch.OverdueBalance, ch.AccountingBalance, ch.DelayDays, ch.Episfalia
order by tbp.Code </CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
