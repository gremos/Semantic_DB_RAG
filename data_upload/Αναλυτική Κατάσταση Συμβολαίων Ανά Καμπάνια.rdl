    <DataSet Name="CampaignDS">
      <Query>
        <DataSourceName>YPS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@dateFrom">
            <Value>=Parameters!dateFrom.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@dateTo">
            <Value>=Parameters!dateTo.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        ID, '[' + Code + '] ' + Name AS Description,CreatedOn
FROM            Campaign
UNION
SELECT        - 1 AS Expr1, '(κενή)' AS Expr2, getdate() as Expr3
WHERE        (@dateFrom IS NOT NULL) AND (@dateTo IS NOT NULL)
ORDER BY CreatedOn desc</CommandText>
      </Query>
