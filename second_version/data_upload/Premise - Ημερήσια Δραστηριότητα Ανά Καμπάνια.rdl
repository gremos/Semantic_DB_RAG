        <Field Name="CampaignDesc">
          <DataField>CampaignDesc</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Supervisor">
          <DataField>Supervisor</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Salesman">
