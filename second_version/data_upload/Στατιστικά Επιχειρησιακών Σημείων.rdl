        <CommandText>--DECLARE @from DATEtime, @to DATEtime;
--SET @from = '20140701';
--SET @to = '20140715';

if object_id('tempdb..#ci') is not null
drop table #ci
select cn1.name as CusInd,cn1.ID as CusIndID
into #ci
from ClassifierNode cn1 
where cn1.id in (3,4,5)

if object_id('tempdb..#pr') is not null
drop table #pr
select bal.AreaName, bal.BroaderAreaID
into #pr
from dbo.BroaderAreaLookUp bal
WHERE BroaderAreaID LIKE 'E%'
UNION 
SELECT '(κενό)' , '-1'


if object_id('tempdb..#tmp0') is not null
drop table #tmp0
select *
INTO #tmp0
from #ci,#pr

if object_id('tempdb..#tmp1') is not null
drop table #tmp1
SELECT DISTINCT
		bp.id ,
		ISNULL(ad.Prefecture,'(κενό)') Prefecture,
		cl.ClassifierNodeID ,
		bp.ModifiedOn ,
		1 AS [ALL] ,
		CAST(bp.isActive AS INT) IsActive ,
		1 - CAST(bp.isActive AS INT) AS IsInactive ,
		( CASE WHEN ( bp.CreatedOn BETWEEN @from AND DATEADD(day, 1, @to) )
					AND bp.IsActive = 1 THEN 1
				ELSE 0
			END ) AS newBP ,
		( CASE WHEN ( bp.ModifiedOn BETWEEN @from AND DATEADD(day, 1, @to) )
					AND bp.IsActive = 0 THEN 1
				ELSE 0
			END ) AS deleted1
INTO #tmp1
FROM	BusinessPoint bp
		JOIN Classification cl ON cl.BusinessPointID = bp.id
									AND ClassifierID = 5 
		JOIN dbo.FullAddressViewV2 ad ON bp.LocationAddressID = ad.id


if object_id('tempdb..#z') is not null
drop table #z

select distinct id, [CusInd], Prefecture, [ALL],[IsActive],[IsInactive],[newBP],[deleted1]--,[deleted2]
into #z
from #tmp0 t0
left join #tmp1 t1 on t1.ClassifierNodeID = t0.CusIndID AND t1.Prefecture = t0.AreaName
order by 1,2

select [CusInd],Prefecture ,
isnull(sum([ALL]),0) as Σύνολο,
isnull(sum([IsActive]),0) as Ενεργά,
isnull(sum([IsInactive]),0) as Ανενεργά,
isnull(sum([newBP]),0) as Νέα, 
isnull(sum([deleted1]),0) as Απενεργοποιήσεις
--,isnull(sum([deleted2]),0) as Απενεργοποιήσεις2
from #z
group by [CusInd],Prefecture
order by 1,2</CommandText>
