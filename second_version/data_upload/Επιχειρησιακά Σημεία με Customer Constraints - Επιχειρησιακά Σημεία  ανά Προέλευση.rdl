  <DataSets>
    <DataSet Name="ConstraintsMain">
      <Query>
        <DataSourceName>MainDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@constraint">
            <Value>=Parameters!constraint.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CustomerSource">
            <Value>=Parameters!CustomerSource.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>IF OBJECT_ID('tempdb..#Constraint') IS NOT NULL
    DROP TABLE #Constraint

 IF OBJECT_ID('tempdb..#a') IS NOT NULL
    DROP TABLE #a


 select distinct top 5000
       bp.code as 'Κωδ. ΕΣ'
	  ,bp.BrandName as 'Φορ. Επωνυμία', bp.Name as 'Διακριτικός Τίτλος'
	  ,cln.name as 'Constraint'
	  ,cs.description as 'Προέλευση'

  into #Constraint
  from 
  BusinessPoint bp 
  left join dbo.Classification cl on cl.BusinessPointID = bp.ID and cl.ClassifierID = 6
  left JOIN dbo.ClassifierNode cln ON cl.ClassifierNodeID = cln.ID
left join CustomerSource cs on bp.CustomerSourceID = cs.id
 where bp.IsActive = 1	
   and isnull(cln.id,9999) in ( 
   @constraint)
   and bp.Name &lt;&gt; 'Empty-Name' 
 	and isnull(cs.id,1111) in (@CustomerSource)	

 


	


	select top 5000 c.[Κωδ. ΕΣ], c.[Φορ. Επωνυμία],c.[Διακριτικός Τίτλος],min(pp.Number) as 'Τηλέφωνο',c.[Constraint],c.[Προέλευση]
                     from #Constraint c 

				left join BusinessPoint bp on bp.code = c.[Κωδ. ΕΣ]
				left join PointPhone pp on pp.BusinessPointID = bp.id
                    where pp.IsActive = 1
	                  and pp.Usage in (1,4,9,10,11) 
group by c.[Κωδ. ΕΣ], c.[Φορ. Επωνυμία],c.[Διακριτικός Τίτλος],c.[Constraint],c.[Προέλευση]</CommandText>
      </Query>
      <Fields>
        <Field Name="Κωδ__ΕΣ">
          <DataField>Κωδ. ΕΣ</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Φορ__Επωνυμία">
          <DataField>Φορ. Επωνυμία</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Διακριτικός_Τίτλος">
          <DataField>Διακριτικός Τίτλος</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Τηλέφωνο">
          <DataField>Τηλέφωνο</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Constraint">
          <DataField>Constraint</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Προέλευση">
          <DataField>Προέλευση</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="CustomerConstraints">
      <Query>
        <DataSourceName>MainDS</DataSourceName>
        <CommandText>select distinct cln.id,cln.name
   from  dbo.Classification cl 
   JOIN dbo.ClassifierNode cln ON cl.ClassifierNodeID = cln.ID
   JOIN dbo.Classifier clr ON cl.ClassifierID = clr.ID
   where clr.id = 6
union
select 9999,'(Χωρίς Customer Constraints)'--distinct cln.id,
   from  dbo.Classification cl 
   JOIN dbo.ClassifierNode cln ON cl.ClassifierNodeID = cln.ID
   JOIN dbo.Classifier clr ON cl.ClassifierID = clr.ID
   where clr.id &lt;&gt; 6</CommandText>
      </Query>
      <Fields>
        <Field Name="ID">
          <DataField>id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Name">
          <DataField>name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Source">
      <Query>
        <DataSourceName>MainDS</DataSourceName>
        <CommandText>  
  select cs.id, cs.Description 
    from CustomerSource cs
	union 
  select 1111, '(Χωρίς Προέλευση)' 
    from CustomerSource cs</CommandText>
      </Query>
      <Fields>
        <Field Name="id">
          <DataField>id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Description">
          <DataField>Description</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="MainCount">
      <Query>
        <DataSourceName>MainDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@constraint">
            <Value>=Parameters!constraint.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CustomerSource">
            <Value>=Parameters!CustomerSource.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>
 select 
 count(bp.code) bpCodeCnt
  from 
  BusinessPoint bp 
  left join dbo.Classification cl on cl.BusinessPointID = bp.ID and cl.ClassifierID = 6
  left JOIN dbo.ClassifierNode cln ON cl.ClassifierNodeID = cln.ID
left join CustomerSource cs on bp.CustomerSourceID = cs.id
 where bp.IsActive = 1	
   and isnull(cln.id,9999) in ( 
   @constraint)
   and bp.Name &lt;&gt; 'Empty-Name' 
 	and isnull(cs.id,1111) in (@CustomerSource)	

 


	
</CommandText>
      </Query>
      <Fields>
        <Field Name="bpCodeCnt">
