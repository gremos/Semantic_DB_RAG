    <DataSet Name="YPSContractViewDataset">
      <Query>
        <DataSourceName>YPS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ContractID">
            <Value>=Parameters!ContractID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT	c.ID AS ContractID ,
		bpActual.Code AS ActualBPCode ,
		bpActual.Name AS ActualBPName ,
		bpBilling.Code AS BillingBPCode ,
		bpBilling.Name AS BillingBPName ,
		c.CreatedOn ,
		c.SignedDate ,
		( CASE WHEN c.DMKey IS NOT NULL THEN 1
			   ELSE 0
		  END ) AS IsMigrated ,
		( CASE WHEN c.DMKey IS NOT NULL THEN c.DMKey
			   ELSE ''
		  END ) AS MigrateCode ,
		( CASE c.State
				WHEN 0 THEN 'Pending'
				WHEN 1 THEN 'Approved'
				WHEN 2 THEN 'Rejected'
				WHEN 3 THEN 'Cancelled'
				WHEN 4 THEN 'Replaced'
				WHEN 5 THEN 'CancelledOffer'
				WHEN 6 THEN 'Modified'
		  END ) AS ContractState ,
		ISNULL(( SELECT TOP ( 1 )
						CAST(ipm.PaymentMonth AS DATE) AS Expr1
				 FROM	InvoicePaymentMethod AS ipm
				 WHERE	( ipm.id = c.ID )
				 ORDER BY ipm.PaymentMonth
			   ),
			   ( SELECT TOP ( 1 )
						CAST(pmED.PaymentMonth AS DATE) AS Expr1
				 FROM	ContractProduct AS cpED
						INNER JOIN PaymentMethod AS pmED ON cpED.PaymentMethodID = pmED.ID
				 WHERE	( cpED.RelatedContractID = c.ID )
				 ORDER BY pmED.PaymentMonth
			   )) AS ExpectedFirstPaymentDT ,
		( CASE WHEN c.ShippingType = 0 THEN 'Email'
			   WHEN c.ShippingType = 1 THEN 'Fax'
			   WHEN c.ShippingType = 2 THEN 'Ταχυδρομείο'
			   WHEN c.ShippingType IS NULL THEN 'Μη Διαθέσιμο'
		  END ) AS ShippingType
FROM	Contract AS c
		INNER JOIN BusinessPoint AS bpActual ON c.ActualBPOwnerID = bpActual.ID
		INNER JOIN BusinessPoint AS bpBilling ON c.BillingPointID = bpBilling.ID
WHERE	( c.ID = @ContractID )
		AND ( c.Type IN ( 0, 2, 3, 4, 5 ) )</CommandText>
      </Query>
      <Fields>
