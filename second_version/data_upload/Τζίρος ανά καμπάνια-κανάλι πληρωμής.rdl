  <DataSets>
    <DataSet Name="ReportDS">
      <Query>
        <DataSourceName>YPS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Mode">
            <Value>=Parameters!Mode.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DateFrom">
            <Value>=Parameters!DateFrom.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DateTo">
            <Value>=Parameters!DateTo.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Autorenewal">
            <Value>=Parameters!Autorenewal.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>--DECLARE @DateFrom DATETIME
--  , @DateTo DATETIME
--  , @Mode BIT
--  , @Autorenewal BIT ;
--SET @DateFrom = '20130301'
--SET @DateTo = '20130331'
--SET @Mode = 0
--SET @Autorenewal = 0


;WITH ContractPaymentChannel AS
(
SELECT distinct ContractId = cp.RelatedContractID
      , PMId = pm.id
      , PaymentChannel = CASE pmt.PaymentWay
							   WHEN 1 THEN 'ΠΙΣΤΩΤΙΚΗ ΚΑΡΤΑ'
							   WHEN 2 THEN 'ΟΤΕ'
							   WHEN 3 THEN 'ΜΕΤΡΗΤΑ'
							   WHEN 4 THEN 'ΕΠΙΤΑΓΗ'
							   WHEN 5 THEN 'ΤΑΧΥΠΛΗΡΩΜΗ'
							   WHEN 6 THEN 'ΑΝΤΑΛΛΑΚΤΙΚΗ ΠΩΛΗΣΗ'
							   WHEN 7 THEN 'ONLINE ΠΙΣΤΩΤΙΚΗ'
							   WHEN 8 THEN 'OFFLINE ΠΙΣΤΩΤΙΚΗ'
							   WHEN 9 THEN 'ΚΑΤΑΘΕΣΗ ΣΕ ΤΡΑΠΕΖΑ'
							WHEN 10 THEN 'ΕΠΙ ΠΙΣΤΩΣΕΙ'
							   else 'other'
                         END
      , RankId = RANK() OVER ( PARTITION BY cp.RelatedContractID ORDER BY pm.id)

FROM    dbo.ContractProduct cp
        JOIN dbo.PaymentMethod pm ON cp.PaymentMethodID = pm.ID --3148247
        JOIN dbo.PaymentMethodType pmt ON pm.PaymentMethodTypeID = pmt.ID

union

SELECT distinct ContractId = c.id
		  , PMId = ipm.id
		  , PaymentChannel = CASE pmt.PaymentWay
								   WHEN 1 THEN 'ΠΙΣΤΩΤΙΚΗ ΚΑΡΤΑ'
								   WHEN 2 THEN 'ΟΤΕ'
								   WHEN 3 THEN 'ΜΕΤΡΗΤΑ'
								   WHEN 4 THEN 'ΕΠΙΤΑΓΗ'
								   WHEN 5 THEN 'ΤΑΧΥΠΛΗΡΩΜΗ'
								   WHEN 6 THEN 'ΑΝΤΑΛΛΑΚΤΙΚΗ ΠΩΛΗΣΗ'
								   WHEN 7 THEN 'ONLINE ΠΙΣΤΩΤΙΚΗ'
								   WHEN 8 THEN 'OFFLINE ΠΙΣΤΩΤΙΚΗ'
								   WHEN 9 THEN 'ΚΑΤΑΘΕΣΗ ΣΕ ΤΡΑΠΕΖΑ'
								WHEN 10 THEN 'ΕΠΙ ΠΙΣΤΩΣΕΙ'
								   else 'other'
							 END
		  , RankId = 1
	FROM    dbo.contract c
	join dbo.InvoicePaymentMethod ipm on c.id = ipm.id
	JOIN dbo.PaymentMethodType pmt ON ipm.PaymentMethodTypeID = pmt.ID
),
AllContracts AS
(	
SELECT  Campaign = '[' + cm.Code + '] ' + cm.Name
      , ch.PaymentChannel
      , AutoRenewal = CASE WHEN ct.OriginalContractType = 1
                                AND ct.OrigionalContractID IS NOT NULL THEN 1
                           ELSE 0
                      END
      , ContractId = ct.ID
      , ContractValue = (SELECT   CAST(SUM( CASE WHEN cp.[STATE] in (3,4) AND cp.AlteredAmount IS NOT NULL 
													THEN cp.AlteredAmount 
													ELSE (cp.Price - cp.Discount - cp.RulesDiscount) END ) AS money)
													
                                          FROM      dbo.ContractProduct cp
										  JOIN dbo.Contract ct1 ON cp.RelatedContractID = ct1.id
                                          WHERE     cp.RelatedContractID = ct.id AND cp.State NOT IN (3))
FROM    dbo.Contract ct
        left JOIN dbo.Campaign cm ON ct.CampaignID = cm.ID
        JOIN ( SELECT   ContractId
                      , PaymentChannel
               FROM     ContractPaymentChannel
               WHERE    RankId = 1
             ) ch ON ch.ContractId = ct.id
WHERE   /*not cancelled*/
        ct.State not in  (3,5,10) /*not fix offers*/
        AND ct.Type NOT IN ( 1, 10 ) /*have passed supervisor approval*/
        AND (
              /*Production */
					EXISTS ( SELECT   1
                                       FROM     dbo.TargetGroupItemValue tgiv
                                                JOIN dbo.[Case] cs ON cs.ContextEntityID = tgiv.ID
                                                              AND cs.ContextEntityType = 'TGIV'
                                                JOIN dbo.Task T ON cs.ID = T.CaseID
                                                              AND t.TaskTypeID = 189
                                       WHERE    tgiv.ContractID = ct.ID )
              OR
					/*Autorenewal*/ (
                                                ct.OriginalContractType = 1
                                                AND ct.OrigionalContractID IS NOT NULL
                                              )
            )
            
        
		AND CASE WHEN @Mode = 1 THEN ct.SignedDate
             ELSE ct.CreatedOn
        END BETWEEN @DateFrom
            AND     DATEADD(day, 1, @DateTo)
 )      
    
SELECT  isnull(Campaign, '(άνευ)') as Campaign
      , PaymentChannel
      , AutoRenewal
      , ContractNbr = COUNT(DISTINCT ContractId)
      , ContractValue = CAST(SUM(ContractValue) AS MONEY)
FROM    AllContracts rc
WHERE  CAST(AutoRenewal AS NVARCHAR(1)) LIKE CASE WHEN @Autorenewal = 1
                                                       THEN '%'
                                                       ELSE '0'
                                                  END
GROUP BY Campaign
      , PaymentChannel
      , AutoRenewal
ORDER BY 1
      , 2</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Campaign">
          <DataField>Campaign</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="PaymentChannel">
          <DataField>PaymentChannel</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ContractNbr">
          <DataField>ContractNbr</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="AutoRenewal">
          <DataField>AutoRenewal</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="ContractValue">
          <DataField>ContractValue</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
