        </Field>
        <Field Name="TaskDesc">
          <DataField>TaskDesc</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="RoleDesc">
          <DataField>RoleDesc</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="TaskTypeID">
          <DataField>TaskTypeID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="TaskCnt">
          <DataField>TaskCnt</DataField>
