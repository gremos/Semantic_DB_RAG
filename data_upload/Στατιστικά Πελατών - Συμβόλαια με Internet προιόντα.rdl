    <DataSet Name="products">
      <Query>
        <DataSourceName>YPS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@MediumCode">
            <Value>=Parameters!MediumCode.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT DISTINCT '['+pr.code+'] '+pr.name prname, pr.code
FROM dbo.Product pr 
JOIN dbo.MediumVersion mv ON pr.MediumVersionID = mv.ID
JOIN dbo.Medium m ON mv.MediumID = m.ID
JOIN dbo.Udac u ON pr.UdacID = u.id
WHERE m.MediumTypeID = 2 AND mv.ID in (@MediumCode) 
AND u.UdacType IN (0,1)
ORDER BY 1</CommandText>
      </Query>
      <Fields>
        <Field Name="prname">
          <DataField>prname</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="code">
          <DataField>code</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="MediumDataSet">
      <Query>
        <DataSourceName>YPS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Inactive">
            <Value>=Parameters!Inactive.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT  m.Name + '( ' + mv.Code + '-' + m.Code +  ' )' AS MName, mv.ID
FROM    dbo.Medium m
        JOIN dbo.MediumVersion mv ON m.ID = mv.MediumID
WHERE   (m.IsActive = 1
        AND mv.IsActive = 1
	and @Inactive = 0)
or @Inactive = 1
        --AND MediumTypeID = 3
ORDER BY m.Name</CommandText>
      </Query>
      <Fields>
        <Field Name="MName">
          <DataField>MName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ID">
          <DataField>ID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
