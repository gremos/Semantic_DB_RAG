  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>YPS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@datefrom">
            <Value>=Parameters!datefrom.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@dateto">
            <Value>=Parameters!dateto.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>with data as (
select distinct a.BpCode as 'Κωδικός ΕΣ'
     , bp.name as 'Διακριτικός Τίτλος ΕΣ' 
	 , a.phone as 'Τηλέφωνο'
	 , a.[Νέα Κατάσταση Επικαιροποίησης]
	 , a.action as 'Ενέργεια'
	 , a.ActionDate
	 , cn.Name as [PrimaryActivity]
	 , bp.id as bpid
from (select BpCode,null as Phone,null as 'Νέα Κατάσταση Επικαιροποίησης','Απενεργοποίηση ΕΣ' as 'Action',ActionDate from itapps.dbo.DINT401BPDeactivations
		union
		select BpCode,Phone,null as 'Νέα Κατάσταση Επικαιροποίησης','Απενεργοποίηση Τηλεφώνου' as 'Action',ActionDate from itapps.dbo.DINT401PhoneDeactivations
		union
		select BpCode,NULL as Phone,UpdateValue as 'Νέα Κατάσταση Επικαιροποίησης','Ενημέρωση Κατάστασης Επικαιροποίησης' as 'Action',ActionDate from itapps.dbo.DINT401CU
	  )a
join yps.dbo.BusinessPoint bp on bp.code = a.BpCode
join yps.dbo.ClassifierNode cn on cn.id=bp.ActivityClassifierNodeID
where cast(actiondate as date) &gt;= @datefrom
and cast(actiondate as date) &lt;= @dateto
)
, secActivities as (

select  distinct data.bpid,cn.name as secondaryActivity 
from yps.dbo.Classification cl
join data on data.bpid=cl.BusinessPointID 
join yps.dbo.ClassifierNode cn on cn.id=cl.ClassifierNodeID 
where  cl.ClassifierID=4
)
,delimitedActivities as (
 select
   distinct  bpid,
    stuff((
        select ' || ' + u.secondaryActivity
        from secActivities u
        where u.bpid = i.bpid
        order by u.secondaryActivity
        for xml path('')
    ),1,4,'') as secActs
from secActivities i
group by bpid
)
select  distinct a.[Κωδικός ΕΣ] , a.[Διακριτικός Τίτλος ΕΣ] , a.[Τηλέφωνο], a.[Νέα Κατάσταση Επικαιροποίησης], a.[Ενέργεια]
	 , a.ActionDate , a.[PrimaryActivity], delimitedActivities.secActs as [SecondaryActivities]
from data a
left join delimitedActivities on a.bpid=delimitedActivities.bpid
order by a.actiondate</CommandText>
