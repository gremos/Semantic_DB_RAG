        <CommandText>--temp για να παρουμε ολα τα προιοντα του συμβολαιου comma-separated
--μπαινει και στα 3 history datasets (YPSContractViewHistoryDataset, YPSContractViewHistoryCRM, YPSContractViewHistoryDeactivations)

IF OBJECT_ID('tempdb..#Products') IS NOT NULL
    DROP TABLE #Products

IF OBJECT_ID('tempdb..#Products2') IS NOT NULL
DROP TABLE #Products2

 select distinct cp.RelatedContractID as ContractID,pr.code+' - '+isnull(pr.Name,'') as code
 into #Products
   from udac u
   left join UdacProdRole upr on u.id = upr.UdacID
   join Product pr on u.id = pr.UdacID
   join Advertisement adv on adv.ProductID = pr.id
   join VersionSection vs on adv.VersionSectionID = vs.id
   join MediumVersion mv on mv.id = vs.MediumVersionID
   join Medium m on m.id = mv.MediumID
   join ContractProduct cp on cp.id = adv.ContractProductID
   where upr.UdacID is null 
     and pr.IsComposite = 0
	 and m.id not in (84,83,73,75,76,9)
	 and cp.RelatedContractID = @ContractID 
	 --group by cp.RelatedContractID
	 --having count(pr.code)&gt;1
	 order by 1 desc


SELECT p1.ContractID,
       ( SELECT code + ' || '
           FROM #Products p2
          WHERE p2.ContractID = p1.ContractID
          ORDER BY Code
            FOR XML PATH('') ) AS Products
      into #products2
      FROM #Products p1
      GROUP BY ContractID ;

select
	t.id
	,t.CreatedOn
	,t.[Description]
	,ISNULL(pu.LastName +' '+ pu.FirstName,r.Description) [PotentialRole]
	,au.UserName ActualOwner
	,t.Outcome
	,t.DynamicDescription
	,t.CompletedOn
	,case when t.[Status] = 0 then 'Pending' else 'Completed' end as [Status]
	,p.Code TaskProductCode
	,p.[Description] TaskProduct
	,cp.RelatedContractID
	,mv.Code+'-'+mv.Description Medium
	,ad.AdvBeginDate
	,adr.[Description] DeactivationReason
from Advertisement ad
	join [Case] c on c.ContextEntityID = ad.ID and c.ContextEntityType = 'ADV'
	join [Case] cc on cc.MasterCaseID = c.ID
	join Task t on t.CaseID = cc.ID
	join TaskType tt on tt.ID = t.TaskTypeID
	join CaseType ct on ct.ID = cc.CaseTypeID
	left join TaskAssignment ta on ta.TaskID = t.ID and ta.AssignmentType = 0
	left join [User] pu on pu.ID = ta.UserID
	left join [Role] r on r.ID = ta.RoleID
	left join [User] au on au.ID = t.ActualOwnerID
	join ContractProduct cp on cp.ID = ad.ContractProductID
	join Product p on p.ID = cp.ProductID
	join VersionSection vs on vs.ID = ad.VersionSectionID
	join MediumVersion mv on mv.ID = vs.MediumVersionID
	left join AdvertisementDeactivationReason adr on adr.ID = ad.DeactivationReason
where
	cp.RelatedContractID = @ContractID and tt.ActivityType = 'H' and ct.ID in (108,109)


order by CreatedOn</CommandText>
