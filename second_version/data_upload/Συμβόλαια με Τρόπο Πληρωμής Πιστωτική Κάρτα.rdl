  <DataSets>
    <DataSet Name="DS">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@dateFrom">
            <Value>=Parameters!dateFrom.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@dateTo">
            <Value>=Parameters!dateTo.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>--DECLARE @datefrom DATE, @dateto DATE;
--SET @datefrom = '20120801';
--SET @dateto = '20131011';


IF OBJECT_ID('tempdb..#customerRelationship') IS NOT NULL 
    DROP TABLE #customerRelationship
SELECT  MainCustomerCode AS edra ,
        CustomerCode AS ypokatasthma
INTO    #customerRelationship
FROM    CustomerRelationshipsView AS crv
IF OBJECT_ID('tempdb..#price') IS NOT NULL 
    DROP TABLE #price
SELECT  c.ID ,
        cp.PaymentMethodID AS pm_id ,
        SUM(CASE WHEN cp.state IN (3,4,6,7) AND cp.AlteredAmount IS NOT NULL THEN cp.AlteredAmount else cp.Price - cp.Discount - cp.RulesDiscount end) AS price
INTO    #price
FROM    Contract AS c
        INNER JOIN ContractProduct AS cp ON c.ID = cp.RelatedContractID
        INNER JOIN PaymentMethod AS pm ON cp.PaymentMethodID = pm.ID
GROUP BY c.ID ,
        cp.PaymentMethodID
IF OBJECT_ID('tempdb..#main') IS NOT NULL 
    DROP TABLE #main
SELECT DISTINCT
        bp.Code AS BranchCode ,
        bp.BrandName AS Customer ,
        c.ID AS ContractCode ,
        c.SignedDate AS SignDate ,
        pm.Installments ,
        pm.NumberOfInvoices ,
        pm.ID AS pmId ,
        CASE WHEN pmt.Description LIKE '%OFF%' THEN 'OFF LINE'
             WHEN pmt.Description LIKE '%ON%' THEN 'ON LINE'
             ELSE 'OTHER'
        END AS PaymentMethod
INTO    #main
FROM    Contract AS c
        INNER JOIN ContractProduct AS cp ON c.ID = cp.RelatedContractID
        INNER JOIN BusinessPoint AS bp ON bp.ID = c.ActualBPOwnerID
        INNER JOIN PaymentMethod AS pm ON cp.PaymentMethodID = pm.ID
        INNER JOIN PaymentMethodType AS pmt ON pm.PaymentMethodTypeID = pmt.ID
WHERE   ( c.State NOT IN ( 3, 5 , 10 ) )
        AND ( c.Type IN ( 0, 2, 3, 4 ) )
		AND ( cp.State &lt;&gt; 3 )
        AND ( pmt.IsActive = 1 )
        AND ( pmt.PaymentWay = 1 )
        AND ( cp.IsRenewed &lt;&gt; 1 )
        AND (( c.SignedDate BETWEEN @dateFrom AND     DATEADD(day, 1, @dateTo) AND c.OrigionalContractID IS null)
			OR ( c.CreatedOn BETWEEN @dateFrom AND     DATEADD(day, 1, @dateTo) AND c.OrigionalContractID IS  NOT null)
		)
IF OBJECT_ID('tempdb..#result1') IS NOT NULL 
    DROP TABLE #result1
SELECT  m.BranchCode ,
        m.Customer ,
        m.ContractCode ,
        CONVERT(NVARCHAR, m.SignDate, 103) AS SignDate ,
        m.Installments ,
        m.NumberOfInvoices ,
        m.PaymentMethod ,
        ISNULL(crm.edra, crs.edra) AS Mcustomer ,
        ISNULL(crs.ypokatasthma, crm.ypokatasthma) AS Scustomer ,
        SUM(DISTINCT p.price) AS ContractAmount
INTO    #result1
FROM    #main AS m
        INNER JOIN #price AS p ON m.ContractCode = p.ID
                                  AND m.pmId = p.pm_id
        LEFT OUTER
                                                                                                                                                                                                                                                                                                                                       JOIN #customerRelationship
        AS crm ON m.BranchCode = crm.edra
        LEFT
                                                                                                                                                                                                                                                                                                                                       OUTER JOIN #customerRelationship
        AS crs ON m.BranchCode = crs.ypokatasthma
GROUP BY m.BranchCode ,
        m.Customer ,
        m.ContractCode ,
        m.SignDate ,
        m.Installments ,
        m.NumberOfInvoices ,
        m.PaymentMethod ,
        ISNULL(crm.edra, crs.edra) ,
        ISNULL(crs.ypokatasthma, crm.ypokatasthma)
ORDER BY m.PaymentMethod
 /*-*/
IF OBJECT_ID('tempdb..#price2') IS NOT NULL 
    DROP TABLE #price2
SELECT  c.ID ,
        ipm.id AS pm_id ,
        SUM(CASE WHEN cp.state IN (3,4) AND cp.AlteredAmount IS NOT NULL THEN cp.AlteredAmount else cp.Price - cp.Discount - cp.RulesDiscount end) AS price
INTO    #price2
FROM    Contract AS c
        INNER JOIN ContractProduct AS cp ON c.ID = cp.RelatedContractID
        INNER JOIN InvoicePaymentMethod AS ipm ON cp.RelatedContractID = ipm.ID
GROUP BY c.ID ,
        ipm.id
IF OBJECT_ID('tempdb..#main2') IS NOT NULL 
    DROP TABLE
                                                                                                                                                                                                                                                                                                                                                                     #main2
SELECT 
                                                                                                                                                                                                                                                                                                                                                                         DISTINCT
        bp.Code AS BranchCode ,
        bp.BrandName AS Customer ,
        c.ID AS ContractCode ,
        c.SignedDate AS SignDate ,
        ipm.Installments ,
        ipm.NumberOfInvoices ,
        ipm.ID AS pmId ,
        CASE pmt.PaymentWay
          WHEN 7 THEN 'ON LINE'
          WHEN 8 THEN 'OFF LINE'
          ELSE 'OTHER'
        END AS PaymentMethod
INTO    #main2
FROM    Contract AS c
        INNER
                                                                                                                                                                                                                                                                                                                                                                                                   JOIN ContractProduct
        AS cp ON c.ID = cp.RelatedContractID
        INNER
                                                                                                                                                                                                                                                                                                                                                                                                   JOIN BusinessPoint
        AS bp ON bp.ID = c.ActualBPOwnerID
        INNER
                                                                                                                                                                                                                                                                                                                                                                                                   JOIN InvoicePaymentMethod
        AS ipm ON cp.RelatedContractID = ipm.ID
        INNER
                                                                                                                                                                                                                                                                                                                                                                                                   JOIN PaymentMethodType
        AS pmt ON ipm.PaymentMethodTypeID = pmt.ID
WHERE   ( c.State &lt;&gt; 3 )
                                                                                                                                                                                                                                                                                                                                                                                                   /*and c.id = 1806772*/
        AND ( c.Type IN ( 0, 2, 3, 4 ) )
		AND ( cp.State &lt;&gt; 3 )
        AND ( pmt.IsActive = 1 )
        AND ( pmt.PaymentWay IN ( 7, 8 ) )
        AND ( cp.IsRenewed &lt;&gt; 1 )
        AND (( c.SignedDate BETWEEN @dateFrom AND     DATEADD(day, 1, @dateTo) AND c.OrigionalContractID IS null)
			OR ( c.CreatedOn BETWEEN @dateFrom AND     DATEADD(day, 1, @dateTo) AND c.OrigionalContractID IS  NOT null)
		)
                                                                                                                                                                                                                                                                                                                                                                                                /*select * from Contract where id = 1806772*/
IF OBJECT_ID('tempdb..#result2') IS
                                                                                                                                                                                                                                                                                                                                                                                                   NOT
                                                                                                                                                                                                                                                                                                                                                                                                   NULL 
    DROP
                                                                                                                                                                                                                                                                                                                                                                                                   TABLE
                                                                                                                                                                                                                                                                                                                                                                                                   #result2
SELECT  m.BranchCode ,
        m.Customer ,
        m.ContractCode ,
        CONVERT(NVARCHAR, m.SignDate, 103) AS SignDate ,
        m.Installments ,
        m.NumberOfInvoices ,
        m.PaymentMethod ,
        ISNULL(crm.edra, crs.edra) AS Mcustomer ,
        ISNULL(crs.ypokatasthma, crm.ypokatasthma) AS Scustomer ,
        SUM(DISTINCT p.price) AS ContractAmount
INTO    #result2
FROM    #main2 AS m
        INNER
                                                                                                                                                                                                                                                                                                                                                                                                                                 JOIN #price2
        AS p ON m.ContractCode = p.ID
                AND m.pmId = p.pm_id
        LEFT
                                                                                                                                                                                                                                                                                                                                                                                                                                 OUTER
                                                                                                                                                                                                                                                                                                                                                                                                                                 JOIN #customerRelationship
        AS crm ON m.BranchCode = crm.edra
        LEFT
                                                                                                                                                                                                                                                                                                                                                                                                                                 OUTER
                                                                                                                                                                                                                                                                                                                                                                                                                                 JOIN #customerRelationship
        AS crs ON m.BranchCode = crs.ypokatasthma
GROUP BY m.BranchCode ,
        m.Customer ,
        m.ContractCode ,
        m.SignDate ,
        m.Installments ,
        m.NumberOfInvoices ,
        m.PaymentMethod ,
        ISNULL(crm.edra, crs.edra) ,
        ISNULL(crs.ypokatasthma, crm.ypokatasthma)
ORDER BY m.PaymentMethod
SELECT  *
FROM    #result1
UNION
SELECT  *
FROM    #result2</CommandText>
      </Query>
