  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>YPS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@mediumid">
            <Value>=Parameters!mediumid.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@state">
            <Value>=Parameters!state.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select ct.id as ctid,bp.Code as bpcode,
      name=isnull((select  bp2.BrandName from yps.dbo.PointRelationship pr join yps.dbo.BusinessPoint bp2 on bp2.ID=pr.TargetBussinessPointID where pr.type=10 and pr.BusinessPointID=bp.id),bp.brandName),
      m.id as mid,m.name as medium,ct.signedDate,usr.username,adv.AdvBeginDate,
      isnull(cp.InterruptionDate,adv.AdvEndDate) AdvEndDate,
      cast(isnull(adv.AlteredAmount,adv.Price-adv.Discount-adv.RulesDiscount) as float) as advprice,
      pr.Code as prcode, 
      email= (select top 1 pe.Email from yps.dbo.PointEmail pe where  pe.BusinessPointID=bp.id and isnull(pe.Email,'')&lt;&gt;''),
      domain=(select top 1 cav.Cav_Value from yps.dbo.FullContractCavView cav where cav.Adv_ID=adv.ID and cav.Cad_Name='url'),
      status=case when adv.AdvBeginDate&gt;CAST(GETDATE() as DATE) then 'Μελλοντικές'
                  when isnull(cp.InterruptionDate,adv.AdvEndDate)&lt;=CAST(GETDATE() as DATE) then  'Ληγμένες το προηγούμενο έτος'
                  else 'Ενεργές' end
     
from  yps.dbo.Contract ct 
join yps.dbo.BusinessPoint bp on bp.ID=ct.ActualBPOwnerID
join yps.dbo.ContractProduct cp on cp.RelatedContractID=ct.ID
join yps.dbo.Advertisement adv on adv.ContractProductID=cp.ID
join yps.dbo.VersionSection vs on vs.ID=adv.VersionSectionID
join yps.dbo.MediumVersion mv on mv.ID=vs.MediumVersionID
join yps.dbo.Medium m on m.ID=mv.MediumID
join yps.dbo.Product pr on pr.ID=adv.ProductID
join yps.dbo.Product p on p.ID=cp.ProductID
join yps.dbo.[user] usr on usr.id=ct.salesmanid
where --m.MediumTypeID=2  --and m.ID&lt;&gt;10--and 
adv.qa=1
and ((adv.productid = cp.productid AND p.IsComposite = 0) OR (adv.productid &lt;&gt; cp.productid AND p.IsComposite = 1))
and ct.State not in (3,5)
and cp.State not in (0,1,3)
and ct.Type not in (1,10)
and mv.Code&lt;&gt;'20'
and m.isactive=1
and dateadd(yy,1,isnull(cp.InterruptionDate,adv.AdvEndDate))&gt;CAST(GETDATE() as DATE)
and m.id in (@mediumid)
and case when adv.AdvBeginDate&gt;CAST(GETDATE() as DATE) then 'Μελλοντικές'
                  when isnull(cp.InterruptionDate,adv.AdvEndDate)&lt;=CAST(GETDATE() as DATE) then  'Ληγμένες το προηγούμενο έτος'
                  else 'Ενεργές' end in (@state)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
