        <CommandText>--temp για να παρουμε ολα τα προιοντα του συμβολαιου comma-separated
IF OBJECT_ID('tempdb..#Products') IS NOT NULL DROP TABLE #Products
SELECT DISTINCT
	[ContractID] = cp.RelatedContractID,
	[code] = pr.code+' - '+isnull(pr.[Name],'')
into #Products
from udac u
	left join UdacProdRole upr on u.id = upr.UdacID
	join Product pr on u.id = pr.UdacID
	join Advertisement adv on adv.ProductID = pr.id
	join VersionSection vs on adv.VersionSectionID = vs.id
	join MediumVersion mv on mv.id = vs.MediumVersionID
	join Medium m on m.id = mv.MediumID
	join ContractProduct cp on cp.id = adv.ContractProductID
where upr.UdacID is null 
	and pr.IsComposite = 0
	and m.id not in (84,83,73,75,76,9)
	and cp.RelatedContractID = @ContractID 
	--group by cp.RelatedContractID
	--having count(pr.code)&gt;1
order by 1 desc

IF OBJECT_ID('tempdb..#Products2') IS NOT NULL DROP TABLE #Products2
SELECT
	p1.ContractID,
	[Products] = ( SELECT code + ' || '
					FROM #Products p2
					WHERE p2.ContractID = p1.ContractID
					ORDER BY Code
					FOR XML PATH('') )
INTO #products2
FROM #Products p1
GROUP BY ContractID ;

IF OBJECT_ID('tempdb..#t1') IS NOT NULL DROP TABLE #t1
--tasks πωλησεων και παραγωγης
	SELECT
		t.id,
		t.CreatedOn,
		t.[Description],
		[PotentialRole] = ISNULL(potusr.LastName +' '+ potusr.FirstName,r.[Description]), 
		[ActualOwner] = u.UserName,
		[Outcome] = CASE
						WHEN t.Status = 0 THEN ''
						ELSE t.Outcome
					END, 
		t.DynamicDescription,
		t.CompletedOn,
		[Status] = CASE
						WHEN t.Status = 0 THEN 'Pending'
						ELSE 'Completed'
				   END,
		[TaskProductCode] = CASE
								WHEN tt.id  = 189 /*έλεγχος πληρότητας*/ THEN prs.Products
								ELSE pr.Code
							END,
		--prs.Code
		[TaskProduct] = pr.[Description],
		[ContractId] = COALESCE(tgiv.ContractID, tgiv.AltOfferID),
		[Medium] = mv.Code+'-'+mv.[Description],
		adv.AdvBeginDate
INTO #t1
	FROM TargetGroupItemValue tgiv
		JOIN [Case] c ON c.ContextEntityID = tgiv.ID AND c.ContextEntityType = 'TGIV'
		JOIN [Case] cc ON cc.MasterCaseID = c.ID
		JOIN Task t ON t.CaseID = cc.ID
		--LEFT JOIN dbo.TaskLog tl ON t.ID = tl.TaskID
		JOIN dbo.TaskType tt ON t.TaskTypeID = tt.ID
		LEFT JOIN dbo.TaskAssignment ta ON t.ID = ta.TaskID AND ta.AssignmentType = 0
		LEFT JOIN dbo.[User] potusr ON ta.UserID = potusr.ID
		LEFT JOIN dbo.Role r ON ta.RoleID = r.ID
		LEFT JOIN dbo.[User] u ON t.ActualOwnerID = u.ID
		LEFT JOIN dbo.[User] su ON t.SenderID = su.ID
		LEFT JOIN dbo.Advertisement ADV ON Cc.ContextEntityID = ADV.ID AND Cc.ContextEntityType = 'ADV'
		LEFT JOIN dbo.Product pr ON adv.ProductID = pr.ID
		LEFT JOIN dbo.VersionSection vs on vs.ID=adv.VersionSectionID
		LEFT JOIN dbo.MediumVersion mv on mv.ID=vs.MediumVersionID
		LEFT JOIN #products2 prs on prs.ContractID = @ContractID
		WHERE (tgiv.ContractID =@ContractID OR tgiv.AltOfferID =@ContractID ) 
		AND tt.ActivityType = 'H'
		  
UNION ALL


--tasks απο CDM (ContractDeliveryManagement (ITApp))
	SELECT
		t.id,
		t.CreatedOn,
		[Description] = t.[Description],
		[PotentialRole] = NULL,
		[ActualOwner] = t.CompletedBy,
		[Outcome] = o.[Description],
		[DynamicDescription] = NULL,
		t.CompletedOn,
		[Status] = CASE
						WHEN t.Closed =1 THEN 'Completed'
						ELSE 'Pending'
					END,
		[TaskProductCode] = isnull('Επανάκληση:' + convert(varchar(10), ti.recalldate, 103),'')
						  + isnull(' ' + convert(varchar(5), ti.recalldate, 108) ,'')
						  + isnull(' || Ραντεβού:' + convert(varchar(10), ti.AppointmentDateFrom, 103),'')
						  + isnull(' ' + convert(varchar(5), ti.AppointmentDateFrom, 108) ,'')
						  + isnull(' - ' + convert(varchar(5), ti.AppointmentDateTo, 108) ,'')
						  + isnull(' || Ανάθεση σε:' + au.[Name],'') 
						  + isnull(' || Barcode' + ti.Barcode,'') ,
		[TaskProduct] = NULL,
		ci.Contractid,
		[Medium] = NULL,
		[Advbegindate] = NULL
	FROM Itapps.cdm.[case] cs
		JOIN Itapps.cdm.caseinfo ci ON ci.caseid = cs.Id
		JOIN Itapps.cdm.task t ON t.CaseId = cs.Id
		JOIN Itapps.cdm.TaskInfo ti ON ti.TaskId = t.Id
		LEFT JOIN Itapps.cdm.AssignUsers au ON au.id = ti.AssignedTo
		LEFT JOIN Itapps.cdm.Outcome o ON t.OutcomeId = o.Id
	WHERE
		ci.ContractId = @ContractID

UNION ALL

	SELECT
		t.id,
		t.CreatedOn,
		[Description] = t.[Description],
		[PotentialRole] = NULL,
		[ActualOwner] = t.CompletedBy,
		[Outcome] = o.[Description],
		[DynamicDescription] = NULL,
		t.CompletedOn,
		[Status] = CASE
						WHEN t.Closed =1 THEN 'Completed'
						ELSE 'Pending'
					END,
		[TaskProductCode] = isnull('Επανάκληση:' + convert(varchar(10), ti.recalldate, 103),'')
						  + isnull(' ' + convert(varchar(5), ti.recalldate, 108) ,'')
						  + isnull(' || Ραντεβού:' + convert(varchar(10), ti.AppointmentDateFrom, 103),'')
						  + isnull(' ' + convert(varchar(5), ti.AppointmentDateFrom, 108) ,'')
						  + isnull(' - ' + convert(varchar(5), ti.AppointmentDateTo, 108) ,'')
						  + isnull(' || Ανάθεση σε:' + au.[Name],'') 
						  + isnull(' || Barcode' + ti.Barcode,'') ,
		[TaskProduct] = NULL,
		ci.Contractid,
		[Medium] = NULL,
		[Advbegindate] = NULL
	FROM Itapps.cdm.[caseHistory] cs
		JOIN Itapps.cdm.caseinfoHistory ci ON ci.caseid = cs.Id
		JOIN Itapps.cdm.taskHistory t ON t.CaseId = cs.Id
		JOIN Itapps.cdm.TaskInfoHistory ti ON ti.TaskId = t.Id
		LEFT JOIN Itapps.cdm.AssignUsers au ON au.id = ti.AssignedTo
		LEFT JOIN Itapps.cdm.Outcome o ON t.OutcomeId = o.Id
	WHERE
		ci.ContractId = @ContractID

ORDER BY CreatedOn


-- docusign pseudo-task
IF OBJECT_ID('tempdb..#t2') IS NOT NULL DROP TABLE #t2
SELECT TOP 1
	[id] = cast(NULL as int),
	[CreatedOn] = e.CreatedOn,
	[Description] = 'Αναμονή Υπογραφής μέσω Docusign',
	[PotentialRole] = cast(NULL as nvarchar(20)),
	[ActualOwner] = 'Docusign System',
	[Outcome] = CASE
					WHEN s.StatusTypeID = 0 THEN 'Pending'
					WHEN s.StatusTypeID = 1 AND p.ID is not null THEN 'Completed'
					WHEN s.StatusTypeID = 1 AND p.ID is null THEN 'SignNoPayment'
					WHEN s.StatusTypeID = 2 THEN 'RefusedToSign'
					WHEN s.StatusTypeID = 3 THEN 'Expired'
				END,
	[DynamicDescription] = cast(NULL as nvarchar(20)),
	[CompletedOn] = CASE
						WHEN s.StatusTypeID = 0 THEN NULL
						ELSE s.[Date]
					END,
	[Status] = CASE
					WHEN s.StatusTypeID = 0 THEN 'Pending'
					ELSE 'Completed'
				END,
	[TaskProductCode] = cast(NULL as nvarchar(20)),
	[TaskProduct] = cast(NULL as nvarchar(20)),
	[Contractid] = e.ContractID,
	[Medium] = cast(NULL as nvarchar(20)),
	[Advbegindate] = NULL
INTO #t2
FROM Itapps.docusign.Envelope e
	JOIN Itapps.docusign.EnvelopeStatus s on s.EnvelopeID = e.ID
	LEFT JOIN [avocet\avocet].XODB.xopayment.Contractpayment p on p.ContractID = e.ContractID
WHERE
	e.ContractID = @ContractID
order by s.ID desc


select *
from #t1
union all
select *
from #t2
order by CreatedOn</CommandText>
