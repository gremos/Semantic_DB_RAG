    <DataSet Name="CampaignDS">
      <Query>
        <DataSourceName>YPS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@UserIds">
            <Value>=Parameters!UserIds.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@UserName">
            <Value>=Parameters!UserName.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>IF SUBSTRING(@UserName, CHARINDEX('\', @UserName) + 1, 50) IN (
    SELECT  UserName
    FROM    ITApps.dbo.PPR_SpecialUsers )

SELECT distinct CampaingId = cm.id,
					CampaignDesc = '[' + cm.Code + '] ' + cm.Name
					FROM dbo.Campaign cm
					JOIN dbo.TargetGroupItem tgi ON cm.ID = tgi.CampaignID
					JOIN dbo.TargetGroupItemValue tgiv ON tgi.ID = tgiv.TargetGroupItemID AND tgiv.CampaignAllocationPlanID = cm.SelectedAllocationPlanID

else

SELECT distinct CampaingId = cm.id,
					CampaignDesc = '[' + cm.Code + '] ' + cm.Name
					FROM dbo.Campaign cm
					JOIN dbo.TargetGroupItem tgi ON cm.ID = tgi.CampaignID
					JOIN dbo.TargetGroupItemValue tgiv ON tgi.ID = tgiv.TargetGroupItemID AND tgiv.CampaignAllocationPlanID = cm.SelectedAllocationPlanID
				WHERE tgiv.Supervisor IN (@UserIds)</CommandText>
