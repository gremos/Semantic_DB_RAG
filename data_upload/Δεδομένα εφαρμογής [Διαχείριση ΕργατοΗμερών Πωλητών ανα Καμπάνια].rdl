  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>YPS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@campaign">
            <Value>=Parameters!campaign.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@user">
            <Value>=Parameters!user.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select t.id,
('['+c.Code+'] '+c.Name) as Καμπάνια , 
u.username as [Πωλητής],
u.SalesCode as [κωδ. Πωλητή],
u2.UserName as [Supervisor],
u2.SalesCode as [κωδ. Supervisor],
convert(nvarchar(20),t.WorkingDate,103) as [Εργατοημέρα]
from ITApps.wdc.WorkingDaysPerCampaign t
join yps.dbo.[User] u on u.id = t.UserId
join yps.dbo.[User] u2 on u2.id = t.SupervisorId
join yps.dbo.Campaign c on t.CampaignId = c.ID
where c.id in ( @campaign ) and u.id in( @user )
order by 1</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="id">
          <DataField>id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Καμπάνια">
          <DataField>Καμπάνια</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Πωλητής">
