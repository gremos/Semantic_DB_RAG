  <DataSets>
    <DataSet Name="ContractsPerYearDataSet">
      <Query>
        <DataSourceName>YPS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@year">
            <Value>=Parameters!year.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>--DECLARE  @year nvarchar(5);
--SET @year = '2012';
SELECT	ct.ID ,
		bp.BrandName ,
		bp.Name ,
		CASE ct.state
			WHEN 0 THEN 'Pending'
			WHEN 1 THEN 'Approved'
			WHEN 2 THEN 'Rejected'
			WHEN 3 THEN 'Cancelled'
			WHEN 4 THEN 'Replaced'
			WHEN 5 THEN 'CancelledOffer'
			WHEN 6 THEN 'Modified'
		END AS state ,
		SUM(CAST(CASE WHEN cp.[STATE] in (3,4)
						   AND cp.AlteredAmount IS NOT NULL
					  THEN cp.AlteredAmount
					  ELSE ( cp.Price - cp.Discount - cp.RulesDiscount )
				 END AS MONEY)) AS value ,
		CAST(SUM(cp.Price) AS FLOAT) AS initialvalue ,
		( SELECT	ch.Description
		  FROM		[User] AS usr
					INNER JOIN Unit AS unt ON unt.ID = usr.UnitID
					INNER JOIN ChannelType AS ch ON ch.ID = unt.ChannelTypeID
		  WHERE		( usr.ID = ct.SalesManID )
		) AS channel ,
		CASE WHEN ( ct.OrigionalContractID IS NOT NULL
					AND YEAR(ct.Createdon) = @year
				  ) THEN 'Ναι'
			 ELSE 'Οχι'
		END AS Expr1 ,
		CONVERT(VARCHAR(20), ct.SignedDate, 103) AS Expr2 ,
		ISNULL(cmp.Code + ' - ' + cmp.Name,
			   ( SELECT	cmp.Code + ' - ' + cmp.Name AS Expr1
				 FROM	TargetGroupItemValue AS tgiv
						INNER JOIN TargetGroupItem AS tgi ON tgi.ID = tgiv.TargetGroupItemID
						INNER JOIN Campaign AS cmp ON cmp.ID = tgi.CampaignID
				 WHERE	( tgiv.ContractID = ct.ID )
			   )) AS Expr3
FROM	Contract AS ct
		INNER JOIN BusinessPoint AS bp ON bp.ID = ct.ActualBPOwnerID
		INNER JOIN ContractProduct AS cp ON cp.RelatedContractID = ct.ID
		LEFT OUTER JOIN Campaign AS cmp ON cmp.ID = ct.CampaignID
WHERE	( ( YEAR(ct.SignedDate) = @year )
		  AND ( ct.State NOT IN ( 3, 5, 10 ) )
		  AND ( ct.Type IN ( 0, 2, 3, 4 ) )
		  AND ( cp.State &lt;&gt; 3 )
		  AND ( ct.OrigionalContractID IS NULL )
		)
		OR ( ( YEAR(ct.CreatedOn) = @year )
			 AND ( ct.State NOT IN ( 3, 5, 10 ) )
			 AND ( ct.Type IN ( 0, 2, 3, 4 ) )
			 AND ( cp.State &lt;&gt; 3 )
			 AND ( ct.OrigionalContractID IS NOT NULL )
		   )
GROUP BY ct.ID ,
		bp.BrandName ,
		bp.Name ,
		ct.State ,
		ct.SalesManID ,
		ct.CreatedBy ,
		ct.SignedDate ,
		cmp.Name ,
		cmp.Code ,
		ct.CreatedOn ,
		ct.OrigionalContractID
ORDER BY Expr1 DESC ,
		bp.BrandName DESC</CommandText>
      </Query>
      <Fields>
        <Field Name="ID">
          <DataField>ID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="BrandName">
          <DataField>BrandName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Name">
          <DataField>Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="state">
          <DataField>state</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="value">
          <DataField>value</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="initialvalue">
          <DataField>initialvalue</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="channel">
          <DataField>channel</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Expr1">
          <DataField>Expr1</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Expr2">
          <DataField>Expr2</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Expr3">
          <DataField>Expr3</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="years">
      <Query>
        <DataSourceName>YPS</DataSourceName>
        <CommandText>with yearlist as 
(
    select 2012 as year
    union all
    select yl.year + 1 as year
    from yearlist yl
    where yl.year + 1 &lt;= YEAR(GetDate())
)

select year from yearlist order by year desc;</CommandText>
      </Query>
      <Fields>
        <Field Name="year">
          <DataField>year</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
