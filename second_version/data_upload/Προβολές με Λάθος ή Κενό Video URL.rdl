  <DataSets>
    <DataSet Name="InvalidVideoURLsDataSet">
      <Query>
        <DataSourceName>YPS</DataSourceName>
        <CommandText>WITH    ProductIds
          AS ( SELECT   id ,
                        Code ,
                        IsComposite
               FROM     dbo.product
               WHERE    Code LIKE '%VH%' OR
						Code LIKE '%VP%'
             ),
        YContracts
          AS ( SELECT distinct  fccv.*
               FROM     dbo.FullContractCavView fccv
		join Contract ct on ct.id = fccv.c_ID and ct.Type in (0,2,3,4)
		JOIN dbo.ContractProduct cp ON cp_ID = cp.id AND cp.State &lt;&gt; 3
               WHERE    C_State not in (3,5,10)
                        AND Adv_Productid IN ( SELECT   id
                                               FROM     ProductIds )
             )
    SELECT  DISTINCT
            bp.Code BPCode,
            bp.Name BPBrandName,
            bp.BrandName BPName,
            YC.C_ID ContractId,
            YC.C_Signdate ContractSignedDate,
			YC.Adv_BeginDate, 
			YC.Adv_EndDate,
            ProductCode = ( SELECT  pr.Code
                            FROM    dbo.Advertisement adv
                                    JOIN dbo.Product pr ON adv.ProductID = pr.ID
                            WHERE   yc.adv_id = adv.ID
                          ),
            Complete = (CASE WHEN YC.Adv_QA = 0 THEN 'Μη Ολοκληρωμένο' ELSE 'Ολοκληρωμένο' END), 
            vs.Description,
            YC.Cad_Name,
            YC.Cav_Value
    FROM    YContracts yc
            JOIN dbo.BusinessPoint bp ON YC.A_BPID = bp.ID
            JOIN dbo.VersionGroupItem vs ON YC.Adv_HeadingID = vs.id
    WHERE   YC.Cad_ID IN (47503,
							47906,
							47907,
							48770)
             AND  (YC.Cav_Value IS NULL OR
                     not (YC.Cav_Value  LIKE 'video.xo.gr/%' or
					  YC.Cav_Value LIKE 'http://video.xo.gr/%') )
             AND CAST(yc.Adv_EndDate AS DATE) &gt; CAST(GETDATE() AS DATE)
	     AND yc.Cav_SeqNo = 0</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
