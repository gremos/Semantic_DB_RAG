    <DataSet Name="campaigns">
      <Query>
        <DataSourceName>YPS</DataSourceName>
        <CommandText>select c.id,
('['+c.Code+'] '+c.Name) as Καμπάνια 
from  Campaign c </CommandText>
      </Query>
      <Fields>
        <Field Name="id">
          <DataField>id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Καμπάνια">
          <DataField>Καμπάνια</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="users">
      <Query>
        <DataSourceName>YPS</DataSourceName>
        <CommandText>select u.id, (u.salescode+' - '+ u.username) as name
from [User] u
where u.SalesCode is not null and u.IsActive = 1
order by 2 </CommandText>
      </Query>
      <Fields>
        <Field Name="id">
          <DataField>id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="name">
          <DataField>name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
