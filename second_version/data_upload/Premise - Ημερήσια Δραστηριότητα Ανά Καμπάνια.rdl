        </Field>
        <Field Name="CustomerIndicator">
          <DataField>CustomerIndicator</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CountOfContracts">
          <DataField>CountOfContracts</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="SumOfContractPrice">
          <DataField>SumOfContractPrice</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="SumPreprintedValue">
          <DataField>SumPreprintedValue</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Raised">
          <DataField>Raised</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Reduced">
          <DataField>Reduced</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
