  <DataSets>
    <DataSet Name="ContractsWebProductsLastStepDataSet">
      <Query>
        <DataSourceName>YPS</DataSourceName>
        <CommandText>--and ct.type in (0,2,4)


if object_id ('tempdb..#a') is not null drop table #a
 SELECT   C.*
                      , tt.Description
                      , t.ID AS TaskID
                      , t.CreatedOn AS TaskCreatedON
                      , adv.ID AS AdvID
                      , cp.BusinessPointID
                      , ContractId = ISNULL(tgiv2.ContractID, cp.RelatedContractID)
                      , tgiv2.AltOfferID
                      , pr.Code AS ProductCode
                      , t.Status AS TaskStatus
                      , AO.UserName AS ActualOwnerUserName
                      , R.Description AS PotentialOwnerRole
                      , ta.AssignmentType
                      , t.Outcome
                      , adv.VersionSectionID
                      , adv.AdvBeginDate
                      , ConcatContractId = COALESCE(tgiv2.ContractID, cp.RelatedContractID, AltOfferID)
					  ,cp.State cpstate
                      into #a
               FROM     Task t
                        JOIN dbo.TaskType tt ON t.TaskTypeID = tt.ID
                        JOIN dbo.[Case] c ON t.CaseID = c.ID
                        LEFT JOIN dbo.TargetGroupItemValue tgiv2 ON C.ContextEntityID = TGIV2.ID
                                                              AND C.ContextEntityType = 'TGIV'
                        LEFT JOIN dbo.Advertisement ADV ON C.ContextEntityID = ADV.ID
                                                           AND C.ContextEntityType = 'ADV'
                        LEFT JOIN dbo.Product pr ON adv.ProductID = pr.ID
                        LEFT JOIN dbo.Udac u ON pr.UdacID = u.ID
                        LEFT JOIN dbo.ContractProduct CP ON ADV.ContractProductID = CP.ID
                        LEFT JOIN dbo.Contract ct ON cp.RelatedContractID = ct.ID
                                                     AND ct.Type IN ( 0, 2, 3, 4 )
                        LEFT OUTER JOIN [User] AS AO ON T.ActualOwnerID = AO.ID
                        LEFT OUTER JOIN TaskAssignment AS TA ON T.ID = TA.TaskID
                                                              AND ta.AssignmentType = 0
                        LEFT OUTER JOIN [Role] AS R ON TA.RoleID = R.ID
               WHERE    TT.ActivityType = 'H'
                        AND EXISTS ( SELECT  wc.Code
                                        FROM    ItApps.dbo.udacwebsites wc
                                        WHERE wc.Code =  u.code )
   
   
if object_id ('tempdb..#b') is not null drop table #b
        SELECT   bp.ID bpid
                      , bp.code
                      , bp.Name
                      , c.ID
                      , c.[State]
					  --, cp.State
                      , c.SalesManID
                      , c.CreatedOn
                      , c.SignedDate
                      , SalesMan = usr.UserName
                      , ContractPrice = SUM(
					  CAST(CASE WHEN  cp.[STATE] in (3,4) AND cp.AlteredAmount IS NOT NULL 
							THEN cp.AlteredAmount 
							ELSE (cp.Price - cp.Discount - cp.RulesDiscount) END AS money)  )
                      into #b
               FROM     dbo.BusinessPoint bp
                        JOIN dbo.Contract c ON bp.ID = c.ActualBPOwnerID
                        LEFT JOIN dbo.[User] usr ON c.SalesManID = usr.ID
                        JOIN dbo.ContractProduct cp ON c.ID = cp.RelatedContractID
											
               WHERE    c.Type IN ( 0, 2, 3, 4 )
                        AND EXISTS ( SELECT  wc.Code
                                        FROM    ItApps.dbo.udacwebsites wc
											JOIN udac ud ON wc.Code =  ud.code
											JOIN dbo.Product pr ON pr.UdacID = ud.ID 
											JOIN dbo.Advertisement adv ON adv.ProductID = pr.ID
                                        WHERE adv.ContractProductID = cp.ID )
               GROUP BY bp.ID
                      , bp.code
                      , bp.Name
                      , c.ID
                      , c.[State]
                      , c.SalesManID
                      , c.CreatedOn
                      , c.SignedDate
                      , usr.UserName
           
             
        SELECT   wc.code AS [BranchCode]
                      , wc.name AS [Name]
                      , wc.id AS [ContractID]
                      , [Αξία_Συμβολαίου] = wc.ContractPrice
                      , A.ProductCode AS [UDAC]
                      , A.Description AS [LastStep]
                      , (CASE wc.[State] 
						   WHEN 0 THEN 'Pending'
						   WHEN 1 THEN 'Approved'
						   WHEN 2 THEN 'Rejected'
						   WHEN 3 THEN 'Cancelled'
						   WHEN 4 THEN 'Replaced'
						   WHEN 5 THEN 'CancelledOffer'
						   WHEN 6 THEN 'Modified'
						 END)
					  AS [ContractStatus]
					  , (CASE a.cpstate
						   WHEN 0 THEN 'Pending'
						   WHEN 1 THEN 'Accepted'
						   WHEN 2 THEN 'Rejected'
						   WHEN 3 THEN 'Cancelled'
						   WHEN 4 THEN 'Interrupted'
						   WHEN 5 THEN 'Completed'
						   WHEN 6 THEN 'Modified'
						   WHEN 7 THEN 'ModifiedForEBS'
						 END)
					  [ContractProductStatus]
                      , a.ActualOwnerUserName AS ActualOwner
                      , a.PotentialOwnerRole AS PotentialOwnerRole
                      , a.Outcome AS Outcome
                      , wc.SalesMan
                      , wc.CreatedOn
                      , wc.SignedDate
                      , a.AdvBeginDate
                      , ( SELECT    MAX(email)
                          FROM      PointEmail
                          WHERE     BusinessPointID = wc.bpid
                        ) AS Email
                      , ( SELECT    MAX(number)
                          FROM      PointPhone
                          WHERE     BusinessPointID = wc.bpid
                                    AND IsActive = 1
                                    AND Usage IN ( 7, 8 )
                        ) AS Fax
               FROM     #b wc
                        LEFT JOIN #a A on wc.id = a.ConcatContractId
               WHERE    (
                          a.TaskCreatedON = ( SELECT    MAX(TaskCreatedON)
                                              FROM      #a ain
                                              WHERE     a.ContractId =ain.ConcatContractId
                                            )
                          OR (
                               A.TaskCreatedON IS NULL
                             )
                        )
             
			ORDER BY wc.code
				  ,  wc.id</CommandText>
