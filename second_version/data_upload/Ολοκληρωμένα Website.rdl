if object_id('tempdb..#urls') is not null
drop table #urls

;WITH urlwebsites AS
(
SELECT DISTINCT fccv2.CP_ID, fccv2.adv_id, vgi2.Description, fccv2.cav_value
FROM dbo.FullContractCavView fccv2
JOIN dbo.Product p ON fccv2.Adv_ProductID = p.ID
JOIN dbo.VersionGroupItem vgi2 ON vgi2.ID = fccv2.Adv_HeadingID
WHERE cad_name in('URL','Domain')
),
urlwebsitesall AS
(
SELECT DISTINCT fccv2.CP_ID, vgi2.Description, fccv2.cav_value
FROM dbo.FullContractCavView fccv2
JOIN dbo.Product p ON fccv2.Adv_ProductID = p.ID
JOIN dbo.VersionGroupItem vgi2 ON vgi2.ID = fccv2.Adv_HeadingID
WHERE cad_name in('URL','Domain')
AND EXISTS (SELECT 1 FROM dbo.MediumVersion mv WHERE MediumID = 9 AND mv.id=p.MediumVersionID) 
),
urlall AS
(
--url online/website products
SELECT DISTINCT fccv2.CP_ID, vgi2.Description, fccv2.cav_value
FROM dbo.FullContractCavView fccv2
JOIN dbo.Product p ON fccv2.Adv_ProductID = p.ID
JOIN dbo.VersionGroupItem vgi2 ON vgi2.ID = fccv2.Adv_HeadingID
WHERE  cad_name in('URL','Domain')
)

SELECT x.*, 
domain1 = stuff((
        select ';' + uw.Cav_Value
        from urlwebsites uw
        where x.cpid = uw.CP_ID AND uw.Adv_ID = x.advid AND uw.Cav_Value IS NOT NULL
        order by uw.CP_ID, uw.Description, uw.cav_value
        for xml path('')
    ),1,1,''), 
domain2 =  stuff((
        select ';' + uw2.Cav_Value
        from urlwebsitesall uw2
        where x.cpid = uw2.CP_ID AND uw2.Description = x.Heading AND uw2.Cav_Value IS NOT NULL
        order by uw2.CP_ID, uw2.Description, uw2.cav_value
        for xml path('')
    ),1,1,''),
domain3 =  stuff((
        select ';' + ua.Cav_Value
        from urlall ua
        where x.cpid = ua.CP_ID AND ua.Description = x.Heading AND ua.Cav_Value IS NOT NULL
        order by ua.CP_ID, ua.Description, ua.cav_value
        for xml path('')
    ),1,1,'')
INTO #urls 
FROM #new x
  
if object_id('tempdb..#phones') is not null
drop table #phones

SELECT * , 	RankID = RANK() over ( PARTITION By aa.BusinessPointID order by aa.Number )
into #phones
from
(	select distinct pp.BusinessPointID , pp.Number	
	from #new bp
	join dbo.PointPhone pp on bp.bpID = pp.BusinessPointID  AND pp.IsActive=1 and Usage in (7,8)
)aa

if object_id('tempdb..#emails') is not null
drop table #emails

SELECT * , 	RankID = RANK() over ( PARTITION By aa.BusinessPointID order by aa.Email )
into #emails
from
(	select distinct pe.BusinessPointID ,pe.Email
	from #new bp
	join dbo.PointEmail pe on bp.bpID = pe.BusinessPointID 
)aa


	select bp.bpid
      , bp.[Κωδ. ΕΣ] 
      , bp.[Διακριτικός] 
      , bp.[Επωνυμία] 
      , bp.[Διεύθυνση] 
      , bp.[ΤΚ] 
      , bp.[Δήμος] 
      , bp.[Νομός] 
      , bp.[Πωλητής] 
      , domain=COALESCE(bp.domain1, bp.domain2, bp.domain3) 
      , bp.[Site Preview File] 
      , bp.[Ημερ. Ολοκλήρωσης] 
      , bp.completed_on
      , [Ημερ. Λήξης Προβολής] = CONVERT(NVARCHAR(15),MAX(bp.[Ημερ. Λήξης Προβολής]), 103)
	  ,e1.Email as email1,e2.Email as email2,e3.Email as email3, p1.Number as fax1,p2.Number as fax2,p3.Number as fax3

	from #urls bp
	 left join #emails e1 on bp.bpID = e1.BusinessPointID and e1.RankID =1 
	 left join #emails e2 on bp.bpID = e2.BusinessPointID and e2.RankID =2 
	 left join #emails e3 on bp.bpID = e3.BusinessPointID and e3.RankID =3 
	 left join #phones p1 on bp.bpID = p1.BusinessPointID and p1.RankID =1 
	 left join #phones p2 on bp.bpID = p2.BusinessPointID and p2.RankID =2 
	 left join #phones p3 on bp.bpID = p3.BusinessPointID and p3.RankID =3 
	
	GROUP BY bp.bpid
      , bp.[Κωδ. ΕΣ] 
      , bp.[Διακριτικός] 
      , bp.[Επωνυμία] 
      , bp.[Διεύθυνση] 
      , bp.[ΤΚ] 
      , bp.[Δήμος] 
      , bp.[Νομός] 
      , bp.[Πωλητής] 
      , COALESCE(bp.domain1, bp.domain2, bp.domain3) 
      , bp.[Site Preview File] 
      , bp.[Ημερ. Ολοκλήρωσης] 
      , bp.completed_on
	  ,e1.Email ,e2.Email ,e3.Email , p1.Number ,p2.Number ,p3.Number
	  
	order by cast(bp.[Κωδ. ΕΣ] as bigint)


