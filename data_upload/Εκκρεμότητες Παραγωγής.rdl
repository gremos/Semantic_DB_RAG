      <Query>
        <DataSourceName>YPS</DataSourceName>
        <CommandText>IF OBJECT_ID('tempdb..#MaxCompletedOn') IS NOT NULL 
	DROP TABLE #MaxCompletedOn
SELECT	ContractId = tgiv2.AltOfferID ,
		MAX(t.CompletedOn) CompletedOn
INTO	#MaxCompletedOn
FROM	dbo.Task t
		JOIN dbo.TaskType tt ON t.TaskTypeID = tt.ID
		JOIN dbo.[Case] c ON t.CaseID = c.ID
		JOIN dbo.TargetGroupItemValue tgiv2 ON C.ContextEntityID = TGIV2.ID
											   AND C.ContextEntityType = 'TGIV'
		JOIN dbo.contract ct ON ct.ID = tgiv2.AltOfferID
WHERE	tt.id IN ( 189 )
		AND ct.State &lt;&gt; 3
		AND ct.type IN ( 0, 2, 3, 4 )
		AND t.Status = 1
		AND t.Outcome IN ( 'Δεν αφορά σύνθετη καταχώρηση', 'Δεν υπάρχουν ελλείψεις' )
GROUP BY tgiv2.AltOfferID

IF OBJECT_ID('tempdb..#MinCreatedOn') IS NOT NULL 
	DROP TABLE #MinCreatedOn
SELECT	ContractId = tgiv2.AltOfferID ,
		MIN(t.CreatedOn) CreatedOn
INTO	#MinCreatedOn
FROM	dbo.Task t
		JOIN dbo.TaskType tt ON t.TaskTypeID = tt.ID
		JOIN dbo.[Case] c ON t.CaseID = c.ID
		JOIN dbo.TargetGroupItemValue tgiv2 ON C.ContextEntityID = TGIV2.ID
											   AND C.ContextEntityType = 'TGIV'
		JOIN dbo.contract ct ON ct.ID = tgiv2.AltOfferID
WHERE	tt.id IN ( 189 )
		AND ct.State &lt;&gt; 3
		AND ct.type IN ( 0, 2, 3, 4 )
GROUP BY tgiv2.AltOfferID

IF OBJECT_ID('tempdb..#OfferFlow') IS NOT NULL 
	DROP TABLE #OfferFlow
SELECT  DISTINCT
		ContractId = tgiv2.AltOfferID
INTO	#OfferFlow
FROM	dbo.Task t
		JOIN dbo.TaskType tt ON t.TaskTypeID = tt.ID
		JOIN dbo.[Case] c ON t.CaseID = c.ID
		JOIN dbo.TargetGroupItemValue tgiv2 ON C.ContextEntityID = TGIV2.ID
											   AND C.ContextEntityType = 'TGIV'
		JOIN dbo.contract ct ON ct.ID = tgiv2.AltOfferID
WHERE	tt.id IN ( 154, 313 )
		AND ct.State &lt;&gt; 3
		AND ct.type IN ( 0, 2, 3, 4 )
		AND t.Status = 1
		AND t.Outcome = 'ΠΡΟΣΦ'

IF OBJECT_ID('tempdb..#Supervisor') IS NOT NULL 
	DROP TABLE #Supervisor
SELECT	ContractId = tgiv2.AltOfferID ,
		MAX(t.CompletedOn) CompletedOn
INTO	#Supervisor
FROM	dbo.Task t
		JOIN dbo.TaskType tt ON t.TaskTypeID = tt.ID
		JOIN dbo.[Case] c ON t.CaseID = c.ID
		JOIN dbo.TargetGroupItemValue tgiv2 ON C.ContextEntityID = TGIV2.ID
											   AND C.ContextEntityType = 'TGIV'
		JOIN dbo.contract ct ON ct.ID = tgiv2.AltOfferID
WHERE	tt.id IN ( 164, 259, 261 )
		AND ct.State &lt;&gt; 3
		AND ct.type IN ( 0, 2, 3, 4 )
		AND t.Status = 1
		AND t.Outcome IN ( 'Δεν απαιτούνται διορθώσεις' )
GROUP BY tgiv2.AltOfferID

IF OBJECT_ID('tempdb..#TicketDate') IS NOT NULL 
	DROP TABLE #TicketDate
SELECT	min(ta.CreatedOn) as CreatedOn,
        a.id AS AdvId
INTO	#TicketDate
FROM	Ticket t
        JOIN Advertisement a on t.RelatedAdvertisementID = a.ID
		JOIN dbo.[Case] ic ON t.ID = ic.ContextEntityID
						  AND ic.ContextEntityType = 'TICKET'
		JOIN dbo.[Case] mc ON mc.MasterCaseID = ic.id
		JOIN dbo.Task ta ON ta.CaseID = mc.ID 		
		LEFT JOIN dbo.TaskAssignment tas ON ta.id = tas.TaskID
		LEFT JOIN dbo.Role r ON	tas.RoleID = r.ID
where r.ID = 72 
  and t.Status = 0
  --and t.ID = 634498 
group by a.id 

--select * from #TicketDate



SELECT DISTINCT
		t.id AS taskid ,
		t.CreatedOn AS taskCreatedOn ,
		t.Description AS taskDescr ,
		t.DynamicDescription taskcomments ,
		bp.Code AS bpCode ,
		bp.Brandname AS bpName ,
		ContractId = tgiv2.AltOfferID ,
		ct.CreatedOn AS CtCreatedDate ,
		ct.SignedDate ,
		srv.completedon srvCompletedon ,
		CASE WHEN ct.ChannelTypeID = 1 THEN 'Tele'
			 WHEN ct.ChannelTypeID = 2 THEN 'Premise'
			 ELSE 'Direct Mail'
		END AS ChannelType ,
		mint.CreatedOn ,
		mco.CompletedOn ,
		NULL AS AdvBeginDate ,
		NULL AS DesiredBeginDate,
		NULL AS prCode ,
		NULL AS prDescription ,
		NULL AS mvCode ,
		NULL AS mvDescr ,
		case when usr.UserName is null and R.Description is null then potusr.UserName 
		else usr.UserName end AS taskowner ,
		R.Description AS roleDescr ,
		paymentType = CASE WHEN EXISTS ( SELECT	1
										 FROM	InvoicePaymentMethod pm
										 WHERE	pm.ID = ct.ID ) THEN ( SELECT	pt.Description
																	   FROM		InvoicePaymentMethod pm
																				JOIN PaymentMethodType pt ON pm.PaymentMethodTypeID = pt.ID
																	   WHERE	pm.ID = ct.ID
																	 )
						   ELSE ( SELECT TOP 1
											pt.Description
								  FROM		PaymentMethod pm
											JOIN PaymentMethodType pt ON pm.PaymentMethodTypeID = pt.id
											JOIN ContractProduct cp2 ON cp2.PaymentMethodID = pm.id
								  WHERE		cp2.RelatedContractID = ct.id
								)
					  END ,
		IsContract = CASE WHEN flo.contractid IS NULL THEN 1
						  ELSE 0
					 END,
		tck.CreatedOn AS CrmDate			 
FROM	dbo.Task t
		JOIN dbo.TaskType tt ON t.TaskTypeID = tt.ID
		
		
		JOIN dbo.[Case] c ON t.CaseID = c.ID
		JOIN dbo.TargetGroupItemValue tgiv2 ON C.ContextEntityID = TGIV2.ID
											   AND C.ContextEntityType = 'TGIV'
		JOIN dbo.contract ct ON ct.ID = tgiv2.AltOfferID
		JOIN ContractProduct cp ON ct.ID = cp.RelatedContractID
		JOIN Advertisement a on a.ContractProductID = cp.id
		JOIN BusinessPoint bp ON ct.ActualBPOwnerID = bp.id
		LEFT JOIN dbo.[User] usr ON usr.ID = t.ActualOwnerID
		LEFT JOIN dbo.TaskAssignment ta ON t.ID = ta.TaskID
										   AND ta.AssignmentType = 0
	    LEFT JOIN dbo.[User] potusr ON ta.UserID = potusr.ID
		LEFT JOIN dbo.Role r ON ta.RoleID = r.ID
		LEFT JOIN #MaxCompletedOn mco ON mco.ContractId = ct.id
		LEFT JOIN #MinCreatedOn mint ON mint.ContractId = ct.id
		LEFT JOIN #OfferFlow flo ON flo.contractid = ct.id
		LEFT JOIN #Supervisor srv ON srv.ContractId = ct.id
		LEFT JOIN #TicketDate tck ON tck.AdvId = a.id
WHERE	tt.id IN ( 189, 195 )
		AND ct.State &lt;&gt; 3
		AND ct.type IN ( 0, 2, 3, 4 )
		AND t.Status = 0
		AND c.State = 0
		--and bp.Code = 305879
		--and a.ID = 4129348
		--and ct.id=1925123 

UNION ALL
SELECT DISTINCT
		t.id AS taskid ,
		t.CreatedOn AS taskCreatedOn ,
		t.Description AS taskDescr ,
		t.DynamicDescription taskcomments ,
		bp.Code AS bpCode ,
		bp.Brandname AS bpName ,
		ct.id AS ContractId ,
		ct.CreatedOn AS CtCreatedDate ,
		ct.SignedDate ,
		srv.completedon srvCompletedon ,
		CASE WHEN ct.ChannelTypeID = 1 THEN 'Tele'
			 WHEN ct.ChannelTypeID = 2 THEN 'Premise'
			 ELSE 'Direct Mail'
		END AS ChannelType ,
		mint.CreatedOn ,
		mco.CompletedOn ,
		adv.AdvBeginDate ,
		adv.DesiredBeginDate ,
		pr.Code AS prCode ,
		pr.Description AS prDescription ,
		mv.code AS mvCode ,
		mv.Description mvDescr ,
		case when usr.UserName is null and R.Description is null then potusr.UserName 
		else usr.UserName end AS taskowner ,
		R.Description AS roleDescr ,
		paymentType = CASE WHEN EXISTS ( SELECT	1
										 FROM	InvoicePaymentMethod pm
										 WHERE	pm.ID = ct.ID ) THEN ( SELECT	pt.Description
																	   FROM		InvoicePaymentMethod pm
																				JOIN PaymentMethodType pt ON pm.PaymentMethodTypeID = pt.ID
																	   WHERE	pm.ID = ct.ID
																	 )
						   ELSE ( SELECT	pt.Description
								  FROM		PaymentMethod pm
											JOIN PaymentMethodType pt ON pm.PaymentMethodTypeID = pt.id
											JOIN ContractProduct cp2 ON cp2.PaymentMethodID = pm.id
								  WHERE		cp2.ID = cp.ID
								)
					  END ,
		IsContract = CASE WHEN flo.contractid IS NULL THEN 1
						  ELSE 0
					 END,
		tck.CreatedOn AS CrmDate
FROM	dbo.Task t
		JOIN dbo.TaskType tt ON t.TaskTypeID = tt.ID	
		JOIN dbo.[Case] c ON t.CaseID = c.ID
		JOIN Advertisement adv ON C.ContextEntityID = ADV.ID
								  AND C.ContextEntityType = 'ADV'
		JOIN ContractProduct cp ON cp.ID = adv.ContractProductID
		JOIN dbo.contract ct ON ct.ID = cp.RelatedContractID
		JOIN BusinessPoint bp ON ct.ActualBPOwnerID = bp.id
		JOIN VersionSection vs ON adv.VersionSectionID = vs.ID
		JOIN MediumVersion mv ON mv.ID = vs.MediumVersionID
		LEFT JOIN Product pr ON pr.ID = adv.ProductID
		LEFT JOIN dbo.[User] usr ON usr.ID = t.ActualOwnerID
		LEFT JOIN dbo.TaskAssignment ta ON t.ID = ta.TaskID
										   AND ta.AssignmentType = 0
		LEFT JOIN dbo.[User] potusr ON ta.UserID = potusr.ID
		LEFT JOIN dbo.Role r ON ta.RoleID = r.ID
		LEFT JOIN #MaxCompletedOn mco ON mco.ContractId = ct.id
		LEFT JOIN #MinCreatedOn mint ON mint.ContractId = ct.id
		LEFT JOIN #OfferFlow flo ON flo.contractid = ct.id
		LEFT JOIN #Supervisor srv ON srv.ContractId = ct.id
		LEFT JOIN #TicketDate tck ON tck.AdvId = adv.id

WHERE	tt.id IN ( 192,193,194,  511,514,516,517,  520,522,523,  525,527,528,509,512,513,519,524,529)
		AND ct.State &lt;&gt; 3
		AND ct.type IN ( 0, 2, 3, 4 )
		AND t.Status = 0
		AND c.State = 0
		--and ct.id=1925123 
		--and adv.ID = 4129348
		--and bp.Code = 305879


		ORDER BY 1

	</CommandText>
