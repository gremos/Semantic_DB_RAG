  <DataSets>
    <DataSet Name="ContractsPerTaskTypeDashBoardDataSet">
      <Query>
        <DataSourceName>YPS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@CampaignType">
            <Value>=Parameters!CampaignType.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT	ContractId = ISNULL(ct.id, tgiv2.AltOfferID) ,
		CampaignId = ISNULL(tgi.campaignid, ct.campaignid) ,
		RoleDesc = r.Description ,
		TaskTypeID = tt.id ,
		TaskTypeDesc = tt.Description ,
		t.CreatedOn ,
		t.Status ,
		t.outcome ,
		t.ID AS taskid
INTO	#temp
FROM	Task t
		JOIN dbo.TaskType tt ON t.TaskTypeID = tt.ID
		JOIN dbo.[Case] c ON t.CaseID = c.ID
		LEFT JOIN dbo.TaskAssignment ta ON t.ID = ta.TaskID
										   AND ta.AssignmentType = 0
		LEFT JOIN dbo.Role r ON ta.RoleID = r.ID
		LEFT JOIN dbo.TargetGroupItemValue tgiv2 ON C.ContextEntityID = TGIV2.ID
													AND C.ContextEntityType = 'TGIV'
		LEFT JOIN dbo.TargetGroupItem tgi ON tgi.ID = tgiv2.TargetGroupItemID
		LEFT JOIN dbo.Advertisement ADV ON C.ContextEntityID = ADV.ID
										   AND C.ContextEntityType = 'ADV'
		LEFT JOIN dbo.ContractProduct CP ON ADV.ContractProductID = CP.ID
											AND cp.State &lt;&gt; 3
		LEFT JOIN dbo.Contract ct ON cp.RelatedContractID = ct.ID
									 AND ct.type IN ( 0, 2, 3, 4 )
WHERE	( ( tt.id &lt;&gt; 195
			AND t.status &lt;&gt; 189
			AND t.status = 0
			AND TT.ActivityType = 'H'
		  )
		  OR tt.id = 195
		  OR tt.id = 505
		  OR tt.id = 500
		  OR tt.id = 189
		)
		AND tt.id &lt;&gt; 175
		AND tt.id &lt;&gt; 170
		AND tt.id &lt;&gt; 266;
WITH	LastTaskPerContract
		  AS ( SELECT  DISTINCT
						ContractId ,
						CampaignId ,
						TaskTypeid ,
						STATUS ,
						outcome ,
						CreatedOn ,
						LastRole = CASE	WHEN ( TaskTypeid IN ( 146, 169, 252, 258, 262, 173, 154, 151, 260, 167, 299, 307, 300, 309, 311, 312, 313 ) )
											 OR ( TaskTypeid = 190
												  AND ISNULL(RoleDesc, '') = ''
												)
											 OR ( TaskTypeid IN ( 294, 295 )
												  AND ISNULL(RoleDesc, '') = ''
												) THEN 'Σε ΣΔ'
										WHEN ( TaskTypeid = 190
											   AND RoleDesc &lt;&gt; ''
											 )
											 OR ( TaskTypeID IN ( 179, 296, 291 ) )
											 OR ( TaskTypeid IN ( 294, 295 )
												  AND ISNULL(RoleDesc, '') &lt;&gt; ''
												) THEN 'Back Office'
										WHEN tasktypeid IN ( 259, 164, 261, 174, 178 ) THEN 'Σε S/V'
										WHEN tasktypeid IN ( 198, 264, 267, 268, 269, 270, 310, 308 ) THEN 'Σε S/V - Manager'
										WHEN tasktypeid IN ( 193, 192, 194, 511, 516, 517, 520, 522, 523, 525, 527, 528 ) THEN 'Παραγωγή'
										WHEN tasktypeid = 189
											 AND STATUS = 0 THEN 'Σε έλεγχο υλικού'
											--WHEN tasktypeid = 189 AND status = 1 AND isnull(outcome,'') &lt;&gt; 'Δεν αφορά σύνθετη καταχώρηση' THEN 'ForProduction'
										WHEN tasktypeid = 195
											 AND STATUS = 0 THEN 'Οριστικ/ση'
										WHEN ( tasktypeid = 195
											   AND STATUS = 1
											   AND outcome = 'Σελιδοποίηση'
											 )
											 OR ( tasktypeid = 505
												  AND STATUS = 1
												  AND outcome = 'Σελιδοποίηση'
												)
											 OR ( tasktypeid = 189
												  AND STATUS = 1
												  AND outcome = 'Δεν αφορά σύνθετη καταχώρηση'
												)
											 OR ( tasktypeid = 500
												  AND STATUS = 1
												  AND outcome = ''
												) THEN 'Ολοκλ/να'
								   END
			   FROM		#temp wrk
			   WHERE	taskid = ( SELECT	MAX(taskid)
								   FROM		#temp wrd
								   WHERE	wrd.ContractId = wrk.ContractId
								 )
			 )
	SELECT	Campaign = cmp.Name + '(' + cmp.Code + ')' ,
			cmp.Code AS CampaignCode ,
			cmp.PlannedStartOn ,
			cmp.PlannedCloseOn ,
			COUNT(Contract) AS CntContract ,
			LastRole ,
			GroupInd = CASE	WHEN LastRole IN ( 'Σε ΣΔ', 'Σε S/V', 'Σε S/V - Manager', 'Back Office' ) THEN 'Πωλήσεις'
							WHEN LastRole IN ( 'Σε έλεγχο υλικού', 'Παραγωγή', 'Οριστικ/ση' ) THEN 'Παραγωγή'
							ELSE ''
					   END ,
			CASE WHEN cmp.PlannedCloseOn &lt;= GETDATE() THEN 2
				 ELSE 1
			END AS cmpType
	FROM	( SELECT	Contract = ct.id ,
						ISNULL(ltc.CampaignId, ct.campaignid) AS CampaignId ,
						LastRole = CASE	WHEN LastRole IS NULL
											 AND ct.OrigionalContractID IS NOT NULL
											 AND OriginalContractType IS NOT NULL THEN 'Συνδρ/κά'
										ELSE LastRole
								   END
			  FROM		dbo.Contract ct
						LEFT JOIN LastTaskPerContract ltc ON ltc.ContractId = ct.id
			  WHERE		ct.State NOT IN ( 3, 5 )
						AND ct.type IN ( 0, 2, 3, 4 )
			) AS xx
			JOIN dbo.Campaign cmp ON cmp.ID = xx.CampaignId
									 AND cmp.State = 2
	WHERE	LastRole IS NOT NULL
			AND CASE WHEN cmp.PlannedCloseOn &lt;= GETDATE() THEN 2
					 ELSE 1
				END IN ( @CampaignType )
	GROUP BY LastRole ,
			cmp.Name ,
			cmp.Code ,
			cmp.PlannedStartOn ,
			cmp.PlannedCloseOn
	ORDER BY cmp.Code ,
			cmp.Name
  

DROP TABLE #temp</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
