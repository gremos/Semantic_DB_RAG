  <DataSources>
    <DataSource Name="DataSource1">
      <DataSourceReference>YPS</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>9404b0e7-3945-4560-ab6f-bafef8eb1cd2</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DS_Main">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@DateFrom">
            <Value>=Parameters!DateFrom.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DateTo">
            <Value>=Parameters!DateTo.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>IF OBJECT_ID('tempdb..#a') IS NOT NULL
    DROP TABLE #A;
    
IF OBJECT_ID('tempdb..#a1') IS NOT NULL
    DROP TABLE #A1;    
    
--DECLARE @DateFrom DATE,
--@DateTo DATE 
--SET @DateFrom='2017-03-01 00:00:00';
--SET @DateTo='2017-03-15 00:00:00';    

WITH a AS (
SELECT DISTINCT a.id NoOfCompletedProducts
      ,case      
		WHEN pr.Characterization = 1 THEN 'Print'
		WHEN pr.Characterization = 2 THEN 'Online'
		WHEN pr.Characterization = 3 THEN 'BundleMorePrint'
		WHEN pr.Characterization = 4 THEN 'BundleMoreOnline'
		WHEN pr.Characterization = 5 THEN 'BundleCore'
	   END AS ProductCharacterization	
      ,ISNULL(pt.Description,'(χωρίς κατηγορία)') AS ProductType
	  ,ts.id ts_id
	  ,cp.RelatedContractID
	  ,CONVERT(VARCHAR,TS.CompletedOn,102) AS CompletedOn
	  ,pr.Code AS pr_code
	  ,pr.Name AS pr_name
	  --,case 
   --      WHEN cp.[state] in (3,4)
   --      THEN ISNULL(a.AlteredAmount,0)
	  --   ELSE a.[Price] - a.[Discount] - a.[RulesDiscount]
	  -- END AS Amount

	  
  FROM dbo.Advertisement a
  JOIN dbo.ContractProduct cp ON cp.id = a.ContractProductID
  JOIN dbo.TargetGroupItemValue tgiv ON cp.RelatedContractID = ISNULL(tgiv.ContractID,tgiv.AltOfferID)
  JOIN dbo.[Case] cs ON tgiv.id = cs.ContextEntityID
					AND cs.ContextEntityType = 'TGIV'
  --JOIN dbo.[Case] cc ON cs.ID = cc.MasterCaseID					
  JOIN task ts ON cs.ID = ts.CaseID 
  JOIN dbo.TaskType tt ON ts.TaskTypeID = tt.ID
  JOIN dbo.Product pr ON a.ProductID = pr.ID
  JOIN udac u ON pr.UdacID = u.ID
  LEFT JOIN dbo.UdacProdRole upr ON u.ID = upr.UdacID
  LEFT JOIN dbo.ProductType pt ON pr.ProductTypeID = pt.ID
 WHERE a.qa = 1  
   AND pr.IsComposite = 0 
   AND tt.id = 189
   AND ( upr.RoleId IS NULL or u.ProdWorkflowID = 2 )
    AND cast(TS.CompletedOn as date) &gt;= @DateFrom AND 
   cast(TS.CompletedOn as date) &lt;= @DateTo
  -- AND case 
		--WHEN upr.RoleID IS NULL AND tt.id = 189 THEN ts.CompletedOn
		--WHEN upr.RoleID IS NOT NULL AND tt.id = 193 THEN ts.CompletedOn
	 --  END IS NOT NULL 
  -- AND upr.RoleID IS NOT NULL 	

UNION

SELECT DISTINCT a.id NoOfCompletedProducts
      ,case      
		WHEN pr.Characterization = 1 THEN 'Print'
		WHEN pr.Characterization = 2 THEN 'Online'
		WHEN pr.Characterization = 3 THEN 'BundleMorePrint'
		WHEN pr.Characterization = 4 THEN 'BundleMoreOnline'
		WHEN pr.Characterization = 5 THEN 'BundleCore'
	   END AS ProductCharacterization	
      ,ISNULL(pt.Description,'(χωρίς κατηγορία)') AS ProductType
	  ,ts.id ts_id
	  ,cp.RelatedContractID
	  ,CONVERT(VARCHAR,TS.CompletedOn,102) AS CompletedOn
	  ,pr.Code AS pr_code
	  ,pr.Name AS pr_name
	  --,case 
   --      WHEN cp.[state] in (3,4)
   --      THEN ISNULL(a.AlteredAmount,0)
	  --   ELSE a.[Price] - a.[Discount] - a.[RulesDiscount]
	  -- END AS Amount

	  
  FROM dbo.Advertisement a
  JOIN dbo.ContractProduct cp ON cp.id = a.ContractProductID
  JOIN dbo.[Case] cs ON a.id = cs.ContextEntityID
					AND cs.ContextEntityType = 'ADV'
  JOIN task ts ON cs.ID = ts.CaseID 
  JOIN dbo.TaskType tt ON ts.TaskTypeID = tt.ID
  JOIN dbo.Product pr ON a.ProductID = pr.ID
  JOIN udac u ON pr.UdacID = u.ID
  --LEFT JOIN dbo.UdacProdRole upr ON u.ID = upr.UdacID
  LEFT JOIN dbo.ProductType pt ON pr.ProductTypeID = pt.ID
 WHERE a.qa = 1  
   AND pr.IsComposite = 0 
   AND tt.id IN ( 193 ,516, 522, 527, 752, 688, 782, 803, 675, 824, 794, 833, 773, 740, 753, 690, 719, 720, 783, 692, 693, 701, 702, 710, 711, 716, 717, 806, 807, 676, 825, 795, 834, 774, 779, 780, 741)
   AND cast(TS.CompletedOn as date) &gt;= @DateFrom AND 
   cast(TS.CompletedOn as date) &lt;= @DateTo
  -- AND case 
		--WHEN upr.RoleID IS NULL AND tt.id = 189 THEN ts.CompletedOn
		--WHEN upr.RoleID IS NOT NULL AND tt.id = 193 THEN ts.CompletedOn
	 --  END IS NOT NULL 
  -- AND upr.RoleID IS NOT NULL 
)





SELECT a.NoOfCompletedProducts
      ,a.ProductCharacterization
      ,a.ProductType
      ,a.RelatedContractID
	  ,a.pr_code
      ,a.pr_name
      --,a.CompletedOn
      ,MAX(ts_id) AS ts_id
      --,a.amount
  INTO #a    
  FROM a 
 --WHERE  a.RelatedContractID = 1844097
  GROUP BY a.NoOfCompletedProducts
      ,a.ProductCharacterization
      ,a.ProductType
      ,a.RelatedContractID
	  ,a.pr_code
      ,a.pr_name
      --,a.CompletedOn 
      --,a.amount
      
      
SELECT NoOfCompletedProducts
      ,ProductCharacterization
      ,ProductType
      ,RelatedContractID
      ,ts_id 
      ,ts.CompletedOn
	  ,pr_code
      ,pr_name
  INTO #a1    
  FROM #a
  JOIN dbo.Task ts ON ts.id = #a.ts_id
   


SELECT COUNT(NoOfCompletedProducts) AS NoOfCompletedProducts
       ,ProductCharacterization
       ,ProductType
	   ,pr_code
       ,pr_name
       --,SUM(Amount) AS Amount
  FROM #a1 
  GROUP BY ProductCharacterization
          ,ProductType
		  ,pr_code
          ,pr_name
order by ProductCharacterization,ProductType</CommandText>
      </Query>
      <Fields>
        <Field Name="NoOfCompletedProducts">
          <DataField>NoOfCompletedProducts</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="ProductCharacterization">
          <DataField>ProductCharacterization</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ProductType">
          <DataField>ProductType</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="pr_code">
          <DataField>pr_code</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="pr_name">
          <DataField>pr_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
