    <DataSet Name="DS_Campaign">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>select DISTINCT cv.NAME+' '+cv.Code AS code
			   from dbo.Campaign cv

union select '(κενή)' as code </CommandText>
      </Query>
      <Fields>
