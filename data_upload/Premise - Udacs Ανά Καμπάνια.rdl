  <DataSets>
    <DataSet Name="UdacsDS">
      <Query>
        <DataSourceName>YPS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@CampaignID">
            <Value>=Parameters!CampaignID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Enddate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT  CampaignID = cm.id
      , CampaignDesc = '[' + cm.Code + '] ' + cm.Name
      , Product = '[' + p.Code + '] ' + p.name
      , CountOfProducts = COUNT(DISTINCT cp.id)
      , CountOfContracts = COUNT(DISTINCT ct.id)
FROM    Task t
        JOIN dbo.TaskType tt ON t.TaskTypeID = tt.ID
                                AND TT.ActivityType = 'H'
                                AND tt.ID = 154	---TaskType = Επίσκεψη σε πελάτη
        JOIN dbo.[Case] c ON t.CaseID = c.ID
        JOIN dbo.TargetGroupItemValue tgiv ON C.ContextEntityID = tgiv.ID
                                              AND C.ContextEntityType = 'TGIV'
        JOIN dbo.Contract ct ON tgiv.AltOfferID = ct.ID
        JOIN dbo.ContractProduct cp ON ct.ID = cp.RelatedContractID
        JOIN dbo.Product p ON cp.ProductID = p.ID
        JOIN dbo.TargetGroupItem tgi ON tgiv.TargetGroupItemID = tgi.ID
        JOIN dbo.Campaign cm ON tgi.CampaignID = cm.ID
WHERE   cm.ID = @CampaignID
        AND ct.State not in (3,5,10) 
		and ct.type in (0,2,3,4)
		AND cp.State &lt;&gt; 3
        AND ct.CreatedOn BETWEEN @StartDate AND DATEADD(DAY,1, @Enddate)
GROUP BY cm.id
      , cm.Code
      , cm.Name
      , p.Code
      , p.name</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="CampaignID">
          <DataField>CampaignID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="CampaignDesc">
          <DataField>CampaignDesc</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Product">
          <DataField>Product</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CountOfContracts">
          <DataField>CountOfContracts</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="CountOfProducts">
          <DataField>CountOfProducts</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
