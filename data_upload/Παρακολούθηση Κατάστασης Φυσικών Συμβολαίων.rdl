  <DataSets>
    <DataSet Name="MainDataSet">
      <Query>
        <DataSourceName>MainDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@DateFrom">
            <Value>=Parameters!DateFrom.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DateTo">
            <Value>=Parameters!DateTo.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Action">
            <Value>=Parameters!Action.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>--declare @DateFrom datetime = '2021-12-29';
--declare @DateTo datetime = '2021-12-29';
--declare @action int = 2;

IF OBJECT_ID('tempdb..#contractsSHT') IS NOT NULL drop table #contractsSHT;
  SELECT ctr.ContractId , tgiv.ID as tgivID, ct.CreatedOn, ct.State, ct.SignedDate, ct.SalesManID, ct.CampaignID, ct.ActualBPOwnerID, tgiv.HandlerUserID, tgiv.Supervisor
  INTO #contractsSHT
  FROM sht.ContractTracker ctr
  JOIN yps.dbo.TargetGroupItemValue tgiv ON ISNULL(tgiv.ContractID,tgiv.AltOfferID) = ctr.ContractId
  JOIN yps.dbo.[Contract] ct ON ct.ID = ctr.ContractId
  WHERE ctr.ReferenceDate &gt;= @DateFrom AND ctr.ReferenceDate &lt;= @DateTo AND ctr.[Action] = @Action
  GROUP BY ctr.ContractId , tgiv.ID, ct.CreatedOn, ct.State, ct.SignedDate, ct.SalesManID, ct.CampaignID, ct.ActualBPOwnerID, tgiv.HandlerUserID, tgiv.Supervisor


  IF OBJECT_ID('tempdb..#camps') IS NOT NULL drop table #camps;
SELECT cmp.ID, cmp.Name, cmp.Code
INTO #camps
FROM #contractsSHT tmp
JOIN yps.dbo.Campaign cmp ON cmp.ID = tmp.CampaignID
GROUP BY cmp.ID, cmp.Name, cmp.Code


  IF OBJECT_ID('tempdb..#tasks') IS NOT NULL drop table #tasks;
SELECT tmp.contractID,
t.id taskID,
isnull('['+CAST(t.id AS nvarchar(15))+'] '+t.Description,'-') tDescription,
t.CreatedOn
INTO #tasks
FROM #contractsSHT tmp
JOIN yps.dbo.[Case] mc ON mc.ContextEntityID = tmp.tgivID AND mc.ContextEntityType = 'TGIV'
JOIN yps.dbo.[case] c ON mc.ID = c.MasterCaseID
JOIN yps.dbo.Task t ON c.ID = t.CaseID
JOIN yps.dbo.TaskType tt ON t.TaskTypeID = tt.ID AND tt.ActivityType = 'H'
WHERE t.Status = 0 AND c.state = 0
GROUP BY tmp.contractID, t.id,
t.Description,
t.CreatedOn

 union all

select ci.ContractId,
t.id taskID, 
tDescription = isnull('['+CAST(t.id AS nvarchar(15))+'] '+t.Description,'-'),
t.CreatedOn 
from Itapps.cdm.[case] cs
join Itapps.cdm.caseinfo ci on ci.caseid = cs.Id
join Itapps.cdm.task t on t.CaseId = cs.Id
join Itapps.cdm.TaskInfo ti on ti.TaskId = t.Id
join #contractsSHT cts on cts.contractID = ci.ContractId
left join Itapps.cdm.AssignUsers au on au.id = ti.AssignedTo
left join Itapps.cdm.Outcome o on t.OutcomeId = o.Id
WHERE t.Closed = 0 


union all

select ci.ContractId,
t.id taskID, 
tDescription = isnull('['+CAST(t.id AS nvarchar(15))+'] '+t.Description,'-'),
t.CreatedOn 
from Itapps.cdm.[caseHistory] cs
join Itapps.cdm.caseinfoHistory ci on ci.caseid = cs.Id
join Itapps.cdm.taskHistory t on t.CaseId = cs.Id
join Itapps.cdm.TaskInfoHistory ti on ti.TaskId = t.Id
join #contractsSHT cts on cts.contractID = ci.ContractId
left join Itapps.cdm.AssignUsers au on au.id = ti.AssignedTo
left join Itapps.cdm.Outcome o on t.OutcomeId = o.Id
WHERE t.Closed = 0 

union all

-- docusign pseudo-task for DEV-14465
select e.ContractID,
0 taskID,
tDescription = 'Αναμονή Υπογραφής μέσω Docusign',
e.CreatedOn
FROM Itapps.docusign.Envelope e
	join #contractsSHT ct on ct.contractID = e.ContractID
WHERE e.LastStatus = 0
ORDER BY t.CreatedOn desc

IF OBJECT_ID('tempdb..#maxtasks') IS NOT NULL drop table #maxtasks;
select ta.ContractId, ta.taskId, ta.tDescription, ta.CreatedOn
into #maxtasks
from #tasks ta
JOIN (
	SELECT tmp1.contractID,
	max(tmp1.taskID) as taskID
	FROM #tasks tmp1
	group by tmp1.contractID ) maxedTID on ta.taskID = maxedTID.taskID
group by ta.ContractId,ta.tDescription, ta.CreatedOn, ta.taskID


if object_id('tempdb..#taskstatus') is not null
drop table #taskstatus

select cp.RelatedContractID AS [Contract], max(ts.id) AS Taskid 
into #taskstatus
from yps.dbo.[case] cs
join yps.dbo.task ts on cs.id = ts.CaseID

join yps.dbo.Advertisement  a on a.id = cs.ContextEntityID and cs.ContextEntityType = 'ADV'
join yps.dbo.[Case] cc on cc.id = cs.MasterCaseID AND cc.ContextEntityType = 'TGIV'
join yps.dbo.ContractProduct cp on cp.ID = a.ContractProductID
join #contractssht cts on cts.contractID= cp.RelatedContractID
join yps.dbo.Product pr on pr.id = cp.ProductID
join yps.dbo.Udac ud on ud.id = pr.UdacID
--where ud.ProdWorkflowID &lt;&gt; 2
group BY cp.RelatedContractID 



union all


-------------Μέχρι έλεγχο Πληρότητας ----------
select cp.RelatedContractID AS [Contract], max(ts.id) AS Taskid
--into #taskstatus
 from yps.dbo.[case] cs
join yps.dbo.task ts on cs.id = ts.CaseID
join yps.dbo.[Case] cc on cc.id = cs.MasterCaseID AND cc.ContextEntityType = 'TGIV'
join #contractssht cts on cts.tgivID= cs.ContextEntityID 
join yps.dbo.ContractProduct cp on cts.contractID = cp.RelatedContractID
join yps.dbo.Product pr on pr.id = cp.ProductID
join yps.dbo.Udac ud on ud.id = pr.UdacID 
where ts.Status = 1 and ts.TaskTypeID = 189 
and not exists ( select 1 from 
                           yps.dbo.udac iu 
						   join yps.dbo.Product ip on ip.UdacID = iu.id
						   join yps.dbo.ContractProduct icp on icp.ProductID = ip.id
						   where icp.RelatedContractID = cp.RelatedContractID and iu.ProdWorkflowID &lt;&gt; 2)




group BY cp.RelatedContractID

union all
select cp.RelatedContractID AS [Contract], max(ts.id) AS Taskid
--into #taskstatus
 from yps.dbo.[case] cs
join yps.dbo.task ts on cs.id = ts.CaseID
join yps.dbo.[Case] cc on cc.id = cs.MasterCaseID AND cc.ContextEntityType = 'TGIV'
join #contractssht cts on cts.tgivID= cs.ContextEntityID 
join yps.dbo.ContractProduct cp on cts.contractID = cp.RelatedContractID
join yps.dbo.Product pr on pr.id = cp.ProductID
join yps.dbo.Udac ud on ud.id = pr.UdacID 
where ts.Status = 1 and ts.TaskTypeID = 189 and cts.contractID in (2004625,2005595,2006379,2008295,2009114,2010632,2014811,2012808)
and not exists ( select 1 from 
                           yps.dbo.udac iu 
						   join yps.dbo.Product ip on ip.UdacID = iu.id
						   join yps.dbo.ContractProduct icp on icp.ProductID = ip.id
						   where icp.RelatedContractID = cp.RelatedContractID and iu.ProdWorkflowID &lt;&gt; 4)




          group BY cp.RelatedContractID


          order by 1
--select * from #taskstatus

--medium
if object_id('tempdb..#tmpMediums') is not null
          drop table #tmpMediums
          select  distinct '[' + mv.Code + ']-'+m.name as medium
          , t.contractID
          into #tmpMediums
          from #contractsSHT t
          join yps.dbo.[Contract] ct on t.contractID = ct.ID
          join yps.dbo.ContractProduct cp on cp.RelatedContractID = ct.ID
          join yps.dbo.Advertisement adv on cp.ID = adv.ContractProductID
          join yps.dbo.Product pr on adv.ProductID = pr.id
          join yps.dbo.VersionSection vs on adv.VersionSectionID = vs.ID
          join yps.dbo.MediumVersion mv on mv.ID = vs.MediumVersionID
          join yps.dbo.Medium m on m.ID = mv.MediumID
          where pr.IsComposite = 0

 if object_id('tempdb..#medium') is not null
          drop table #medium
          select t1.contractID ,
          ( SELECT t2.medium + ','
          FROM #tmpMediums t2
          WHERE t2.contractID = t1.contractID
          ORDER BY t2.medium
          FOR XML PATH('') ) AS Mediums
          into #medium
          from #tmpMediums t1
          group by contractID

		  
		  --ContractPrice
		IF OBJECT_ID('tempdb..#contractprice') IS NOT NULL drop table #contractprice
		select ct.id contractid,sum(CASE WHEN cp.[STATE] IN ( 3, 4 )
          AND cp.AlteredAmount IS NOT NULL THEN cp.AlteredAmount
          ELSE cp.price - cp.discount - cp.rulesdiscount
          END) price,
          sum(cp.Discount) Disc,
          sum(isnull(cp.PaymentDiscount,0)) pDisc,
          SUM(CASE WHEN ct.State = 6 AND cp.state IN (3,4) THEN cp.AlteredAmount WHEN ct.State = 6 AND cp.State NOT IN (3,4) THEN cp.Price-cp.RulesDiscount-cp.Discount ELSE null END) altAmount
		  into #contractprice
		  from yps.dbo.[Contract] ct
          join yps.dbo.ContractProduct cp on cp.RelatedContractID = ct.id
		  join #contractsSHT ctrSHT on ctrSHT.ContractId = ct.Id
          GROUP BY ct.id

SELECT  
  [Καμπάνια] = '[' + cmp.code+'] '+cmp.Name,
	[Area Manager] = '[' +cast(area.SalesCode as nvarchar(100))+'] '+area.LastName+' '+area.FirstName,
  [Supervisor] = '[' +supervisor.SalesCode+'] ' + cast(supervisor.LastName as nvarchar(50)) + ' ' + cast(supervisor.FirstName as nvarchar(50)) ,
  [SalesMan] = '[' +cast(u.SalesCode as nvarchar(100))+'] '+u.LastName+' '+u.FirstName,
  [Κωδ. ΕΣ] = bp.code,
  [Επωνυμία] = bp.name,
  [Κωδ. Συμβολαίου] = ctr.contractID,
  [Τελική Αξία Συμβολαίου] = ctp.price,
  [Ημερ. Υπογραφής Συμβ.] = ctr.SignedDate,
  [Ημερ. Δημιουργίας Συμβ.] = ctr.CreatedOn,
  [Κατάσταση Συμβολαίου] = CASE ctr.[State]
          WHEN 0 THEN 'Pending'
          WHEN 1 THEN 'Approved'
          WHEN 2 THEN 'Rejected'
          WHEN 3 THEN 'Cancelled'
          WHEN 4 THEN 'Replaced'
          WHEN 5 THEN 'CancelledOffer'
          WHEN 6 THEN 'Modified'
          END,
  [Ολοκληρωμένα] = CASE ts.Status
          WHEN 1 THEN 'Completed' ELSE '-' 
		  END,
  [Τελευταίο Ανοιχτό Βήμα] = isnull(tasks.tDescription,'-'),
  [Μέσα/Εκδόσεις Συμβ.] = SUBSTRING(mdm.Mediums,1,LEN(mdm.Mediums)-1)
  FROM #contractsSHT ctr
  JOIN #camps cmp ON cmp.ID = ctr.CampaignID
  JOIN yps.dbo.[User] supervisor on ctr.Supervisor = supervisor.ID
  JOIN yps.dbo.[User] u ON ctr.HandlerUserID = u.id
	LEFT JOIN yps.dbo.SalesTreeHistory sth on sth.SalesmanID = u.id and sth.[Year] = YEAR(ctr.CreatedOn) and sth.SegmentID = MONTH(ctr.CreatedOn) and sth.IsDeleted=0
	LEFT JOIN yps.dbo.[User] area on sth.ManagerID = area.ID
  JOIN yps.dbo.Businesspoint bp ON bp.id = ctr.ActualBPOwnerID
  LEFT JOIN #contractprice ctp ON ctp.contractid = ctr.contractID
  LEFT JOIN #taskstatus tst on tst.Contract = ctr.contractID
  LEFT JOIN yps.dbo.Task ts on ts.id = tst.Taskid
  LEFT JOIN #maxtasks tasks ON tasks.contractID = ctr.ContractId
  LEFT JOIN #medium mdm ON mdm.ContractId = ctr.ContractId
  ORDER BY [Καμπάνια]</CommandText>
      </Query>
