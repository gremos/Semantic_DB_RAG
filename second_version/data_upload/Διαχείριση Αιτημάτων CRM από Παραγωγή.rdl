--set @Medium = '[10] XO.GR' 

IF OBJECT_ID('tempdb..#alldata') IS NOT NULL DROP TABLE #alldata
select * into #alldata
from (
select  t.id as ticketcode,
		t.TicketTypeID,
		tt.Description ticketType,
		t.RelatedContractID contractid , 
		ic.ID as caseid, mc.ID as mastercaseid,
        taskid = ta.id ,
		TaskTypeId = ta.TaskTypeID ,
		usr.UserName,ta.Status TskSt, 
		ta.Description Task, 
        ta.DynamicDescription TaskDescription,
		ta.CreatedOn TaskCreatedOn, 
		ta.CompletedOn TaskCompletedOn,  
		ta.Outcome,
        bp.Code as bpcode,
		bp.BrandName as bpName,
		t.RelatedAdvertisementID,
		pr.code as productcode,
		tp.[Description] as tktDescription
from   dbo.Ticket t
       join dbo.TicketType tt on tt.ID=t.TicketTypeID
	   left join dbo.TicketPriority tp on tp.id = t.PriorityID --- added
	   JOIN dbo.[Case] ic ON t.ID = ic.ContextEntityID AND ic.ContextEntityType = 'TICKET'
	   JOIN dbo.[Case] mc ON mc.MasterCaseID = ic.id
	   JOIN dbo.CaseType ct ON mc.CaseTypeID = ct.ID
	   JOIN dbo.Task ta ON ta.CaseID = mc.ID
	   join dbo.BusinessPoint bp on bp.ID=t.BusinessPointID
	   join #CRMProdTaskTypes cptt on cptt.taskID = ta.ID
	   left join dbo.[User] usr on usr.id=ta.ActualOwnerID
	  join Advertisement adv on mc.ContextEntityID = adv.id and mc.ContextEntityType = 'ADV' 
	  join dbo.Product pr on pr.ID=adv.ProductID
	  JOIN VersionSection vs ON vs.ID = adv.VersionSectionID
	  JOIN MediumVersion mv ON mv.ID = vs.MediumVersionID
	  JOIN Medium m ON m.ID = mv.MediumID
	
where  ('[' + m.code + '] ' + m.Name) in (@Medium)
--order by ta.ID desc
union 

select  t.id as ticketcode,
		t.TicketTypeID,
		tt.Description ticketType,
		t.RelatedContractID contractid,
		ic.ID as caseid,
		mc.ID as mastercaseid,
        taskid = ta.id,
		TaskTypeId = ta.TaskTypeID,
		usr.UserName,
		ta.Status TskSt,
		ta.Description Task, 
        ta.DynamicDescription TaskDescription,
		ta.CreatedOn TaskCreatedOn,
		ta.CompletedOn TaskCompletedOn,
		ta.Outcome,
        bp.Code as bpcode,
		bp.BrandName as bpName ,
		t.RelatedAdvertisementID,
		NULL as productcode,
		tp.[Description] as tktDescription
from   dbo.Ticket t
       join dbo.TicketType tt on tt.ID=t.TicketTypeID
	   left JOIN dbo.TicketPriority tp on tp.id = t.PriorityID--added
	   JOIN dbo.[Case] ic ON t.ID = ic.ContextEntityID AND ic.ContextEntityType = 'TICKET'
	   JOIN dbo.[Case] mc ON mc.MasterCaseID = ic.id
	   JOIN dbo.CaseType ct ON mc.CaseTypeID = ct.ID
	   JOIN dbo.Task ta ON ta.CaseID = mc.ID 
	   JOIN dbo.TaskAssignment tas ON ta.id = tas.TaskID
	   JOIN dbo.Role r ON	tas.RoleID = r.ID
	   join dbo.BusinessPoint bp on bp.ID=t.BusinessPointID
	   left join dbo.[User] usr on usr.id=ta.ActualOwnerID
where r.ID in (114,655) --tas.roleid in (114,655)  'Προϊστάμενος Οργανωτικής Μονάδας Production' ,'Προϊστάμενος Οργανωτικής Μονάδας Website Constr.'
and ta.TaskTypeID in (227)

)xx

IF OBJECT_ID('tempdb..#users') IS NOT NULL DROP TABLE #users
Select Main.ticketcode,
       case when len(main.users)&gt;0 then Left(Main.users,Len(Main.users)-1) else main.users end As [users]
into #users
From
    (
        Select distinct ST2.ticketcode, 
            (
                Select distinct ST1.UserName + '/' AS [text()]
                From #alldata ST1
                Where ST1.ticketcode= ST2.ticketcode
                For XML PATH ('')
            ) [users]
        From #alldata ST2
    ) [Main]





--DECLARE @DateFrom DATETIME ;
--DECLARE @DateTo DATETIME ;



--set @DateFrom = '2017-03-01'
--set @DateTo ='2017-03-09'
   
    
;with main as (    
select 
		a.bpcode,
		bpName,
		ticketcode, 
		ticketType, 
		contractid,
		productcode,
		MIN(taskcreatedon) taskCreatedon,
        PCCompletedon= (select Min(t.completedon) 
						from [case] c 
						join task t on t.CaseID = c.id  
						where t.TaskTypeID in (537,543) and t.Outcome&lt;&gt;'ΕΠΙΣΤΡΟΦΗ' 
						and t.Status =1 and c.ContextEntityType = 'TICKET'  
						and c.ContextEntityID = a.ticketcode),
        maxtaskid=(select MAX(taskid) from #alldata b where b.ticketcode=a.ticketcode),
	    tktDescription ,
		max(TaskCompletedOn) taskcompletedOn
       
from #alldata a
group by a.bpcode , bpName, ticketcode, ticketType, contractid,productcode,tktDescription--,TaskCompletedOn

)



select 
		[Κωδ. ΕΣ] =m.bpcode,
		[Επωνυμία] = m.bpName,
		[Κωδ. Αιτήματος] = m.ticketcode, 
		[Τύπος Aιτήματος] = m.ticketType,
		[Προτεραιότητα Aιτήματος] = m.tktDescription,--added
		[Κωδ. Συμβολαίου] = m.contractid,
		[Προϊόν] = m.productcode,
		[Ημ. Προώθησης από CRM] = taskCreatedon ,
		[Ημερομ. Ολοκλήρ. από τον Έλεγχο] = m.PCCompletedon,
		[Ημερομ. Ολοκλήρ. από Παραγωγή] = (select taskcompletedon from #alldata  a where a.taskid=m.maxtaskid),
		[Ημέρες σε Παραγωγή] =  case when(select taskcompletedon from #alldata  a where a.taskid=m.maxtaskid) is not null 
								then DATEDIFF(dd,taskCreatedon,(select taskcompletedon from #alldata  a where a.taskid=m.maxtaskid)) 
								else DATEDIFF(dd,taskCreatedon,getdate()) end ,--ημ/νία ολοκλήρωσης από έλεχγο μέχρι την ημ ολοκλήρωσης. Αν δεν είναι ολοκληρωμένο, τότε θα είναι η ημ/νία ολοκλήρωσης από έλεχγο μέχρι την τρέχουσα ημέρα
		[Κατάσταση] = case when (Case when (select taskcompletedon from #alldata  a where a.taskid=m.maxtaskid) IS not null 
			then DATEDIFF(dd,taskCreatedon,(select taskcompletedon from #alldata  a where a.taskid=m.maxtaskid)) end)  is not null then 'Ολοκληρωμένο' else 'Σε εκκρεμότητα' end ,
		[Ολοκλήρωση εντός 1 εβδομάδας] = case when (Case when (select taskcompletedon from #alldata  a where a.taskid=m.maxtaskid) IS not null 
			then DATEDIFF(dd,taskCreatedon,(select taskcompletedon from #alldata  a where a.taskid=m.maxtaskid)) end) &lt; 7 then 'ΝΑΙ' else null end,
		[Ολοκλήρωση 1-2 εβδομάδες] = case when (Case when (select taskcompletedon from #alldata  a where a.taskid=m.maxtaskid) IS not null 
			then DATEDIFF(dd,taskCreatedon,(select taskcompletedon from #alldata  a where a.taskid=m.maxtaskid)) end) between 7 and 14 then 'ΝΑΙ' else null end,
		[Ολοκλήρωση 3-4 εβδομάδες] = case when (Case when (select taskcompletedon from #alldata  a where a.taskid=m.maxtaskid) IS not null 
			then DATEDIFF(dd,taskCreatedon,(select taskcompletedon from #alldata  a where a.taskid=m.maxtaskid)) end) between 21 and 28 then 'ΝΑΙ' else null end,
		[Ολοκλήρωση &gt; 4 εβδομάδες] = case when (Case when (select taskcompletedon from #alldata  a where a.taskid=m.maxtaskid) IS not null 
			then DATEDIFF(dd,taskCreatedon,(select taskcompletedon from #alldata  a where a.taskid=m.maxtaskid)) end) &gt; 28 then 'ΝΑΙ' else null end,
		[Βήμα]=(select task from #alldata  a where a.taskid=m.maxtaskid),
		[Ημέρες Ολοκλήρωσης] = Case when (select taskcompletedon from #alldata  a where a.taskid=m.maxtaskid) IS not null 
			then DATEDIFF(dd,taskCreatedon,(select taskcompletedon from #alldata  a where a.taskid=m.maxtaskid)) end, 
		[Όλοι οι Χρήστες] =(select  UserS from #users usr where usr.ticketcode=m.ticketcode)
from main m
where taskCreatedon &gt;= @datefrom
and taskCreatedon &lt; @dateto+1
--and m.ticketcode = 758329

order by taskCreatedon


