            <Value>=Parameters!ID_REFERENCEDATE.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>

	SET NOCOUNT ON;
	SET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED;


IF OBJECT_ID('tempdb..#detailed') IS NOT NULL
    DROP TABLE #detailed;
IF OBJECT_ID('tempdb..#CloseAmountPTY') IS NOT NULL
    DROP TABLE #CloseAmountPTY;
IF OBJECT_ID('tempdb..#CloseAmount') IS NOT NULL
    DROP TABLE #CloseAmount;
IF OBJECT_ID('tempdb..#CloseAmountDates') IS NOT NULL
    DROP TABLE #CloseAmountDates;
IF OBJECT_ID('tempdb..#indicator') IS NOT NULL
    DROP TABLE #indicator;	
IF OBJECT_ID('tempdb..#NotPaid') IS NOT NULL
    DROP TABLE #NotPaid;	
IF OBJECT_ID('tempdb..#CloseAmountSum') IS NOT NULL
    DROP TABLE #CloseAmountSum;	
IF OBJECT_ID('tempdb..#NotPaidSum') IS NOT NULL
    DROP TABLE #NotPaidSum;	
IF OBJECT_ID('tempdb..#NotExpired') IS NOT NULL
    DROP TABLE #NotExpired;		
IF OBJECT_ID('tempdb..#NotExpiredSum') IS NOT NULL
    DROP TABLE #NotExpiredSum;				
IF OBJECT_ID('tempdb..#a') IS NOT NULL
    DROP TABLE #a;		
	



--------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
-- Υπολογίζει όλα τα παραστατικά που κλείνουν με 'ΠΤΥ' ή 'ΧΡΠ'--------------------------------------------------------------------------------------------------------------------------------------------
--------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------


SELECT ESFILiquidityAccountEntryMatching.fOpenEntryGID AS fOpenEntryGID, 
       sum(ESFILiquidityAccountEntryMatching.CloseCurrencyAmount )AS CloseCurrencyAmount
into #CloseAmountPTY
FROM ESFILiquidityAccountEntryMatching AS ESFILiquidityAccountEntryMatching
     INNER JOIN ESFILiquidityAccountEntry AS FK_ESFILiquidityAccountEntry_ESFILiquidityAccountEntryMatching
       ON ESFILiquidityAccountEntryMatching.fCloseEntryGID = FK_ESFILiquidityAccountEntry_ESFILiquidityAccountEntryMatching.GID
     INNER JOIN ESFILiquidityAccountEntry AS FK_ESFILiquidityAccountEntry_ESFILiquidityAccountEntryMatching1
       ON ESFILiquidityAccountEntryMatching.fOpenEntryGID = FK_ESFILiquidityAccountEntry_ESFILiquidityAccountEntryMatching1.GID
     LEFT JOIN ESFILiquidityAccountEntry AS FK_ESFILiquidityAccountEntryMatching_ESFILiquidityAccountEntry_OI
       ON ESFILiquidityAccountEntryMatching.fOpenEntryGID = FK_ESFILiquidityAccountEntryMatching_ESFILiquidityAccountEntry_OI.GID
     LEFT JOIN ESFILiquidityAccountEntry AS FK_ESFILiquidityAccountEntryMatching_ESFILiquidityAccountEntry_CI
       ON ESFILiquidityAccountEntryMatching.fCloseEntryGID = FK_ESFILiquidityAccountEntryMatching_ESFILiquidityAccountEntry_CI.GID
   where left(FK_ESFILiquidityAccountEntryMatching_ESFILiquidityAccountEntry_CI.DocumentCode,3) in ('ΠΤΥ','ΧΡΠ') 
group by fOpenEntryGID
  --select * from #CloseAmountPTY ca where ca.DocumentCode1 ='ΤΠΥ-3-52701'
  

--------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
--Υπολογίζει τα παραστατικά που έχουν αντιστοίχιση------------------------------------------------------------------------------------------------------------------------------------------------
--------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------

SELECT ESFILiquidityAccountEntryMatching.fOpenEntryGID AS fOpenEntryGID, 
       sum(ESFILiquidityAccountEntryMatching.CloseCurrencyAmount )AS CloseCurrencyAmount 
into #CloseAmount
FROM ESFILiquidityAccountEntryMatching AS ESFILiquidityAccountEntryMatching
     INNER JOIN ESFILiquidityAccountEntry AS FK_ESFILiquidityAccountEntry_ESFILiquidityAccountEntryMatching
       ON ESFILiquidityAccountEntryMatching.fCloseEntryGID = FK_ESFILiquidityAccountEntry_ESFILiquidityAccountEntryMatching.GID
     INNER JOIN ESFILiquidityAccountEntry AS FK_ESFILiquidityAccountEntry_ESFILiquidityAccountEntryMatching1
       ON ESFILiquidityAccountEntryMatching.fOpenEntryGID = FK_ESFILiquidityAccountEntry_ESFILiquidityAccountEntryMatching1.GID
     LEFT JOIN ESFILiquidityAccountEntry AS FK_ESFILiquidityAccountEntryMatching_ESFILiquidityAccountEntry_OI
       ON ESFILiquidityAccountEntryMatching.fOpenEntryGID = FK_ESFILiquidityAccountEntryMatching_ESFILiquidityAccountEntry_OI.GID
     LEFT JOIN ESFILiquidityAccountEntry AS FK_ESFILiquidityAccountEntryMatching_ESFILiquidityAccountEntry_CI
       ON ESFILiquidityAccountEntryMatching.fCloseEntryGID = FK_ESFILiquidityAccountEntryMatching_ESFILiquidityAccountEntry_CI.GID
group by fOpenEntryGID



SELECT  distinct ESFILiquidityAccountEntryMatching.fOpenEntryGID AS fOpenEntryGID, 
       ESFILiquidityAccountEntryMatching.GID AS GID, 
       trade.code,
       FK_ESFILiquidityAccountEntry_ESFILiquidityAccountEntryMatching1.DocumentCode AS DocumentCode1, 
       FK_ESFILiquidityAccountEntry_ESFILiquidityAccountEntryMatching1.ValeurDate as XrewshValeur,
       FK_ESFILiquidityAccountEntry_ESFILiquidityAccountEntryMatching1.RegistrationDate as XrewshRegistration,
	    ESFILiquidityAccountEntryMatching.CloseCurrencyAmount,
	   FK_ESFILiquidityAccountEntryMatching_ESFILiquidityAccountEntry_CI.DocumentCode,
       FK_ESFILiquidityAccountEntryMatching_ESFILiquidityAccountEntry_CI.RegistrationDate as PistwshRegistration,
	   FK_ESFILiquidityAccountEntry_ESFILiquidityAccountEntryMatching1.fDimension2Code
into #CloseAmountDates
FROM ESFILiquidityAccountEntryMatching AS ESFILiquidityAccountEntryMatching
     INNER JOIN ESFILiquidityAccountEntry AS FK_ESFILiquidityAccountEntry_ESFILiquidityAccountEntryMatching
       ON ESFILiquidityAccountEntryMatching.fCloseEntryGID = FK_ESFILiquidityAccountEntry_ESFILiquidityAccountEntryMatching.GID
     INNER JOIN ESFILiquidityAccountEntry AS FK_ESFILiquidityAccountEntry_ESFILiquidityAccountEntryMatching1
       ON ESFILiquidityAccountEntryMatching.fOpenEntryGID = FK_ESFILiquidityAccountEntry_ESFILiquidityAccountEntryMatching1.GID
	   join ESFITradeAccount trade on FK_ESFILiquidityAccountEntry_ESFILiquidityAccountEntryMatching1.fTradeAccountGID = trade.gid and trade.Type = 0
     LEFT JOIN ESFILiquidityAccountEntry AS FK_ESFILiquidityAccountEntryMatching_ESFILiquidityAccountEntry_OI
       ON ESFILiquidityAccountEntryMatching.fOpenEntryGID = FK_ESFILiquidityAccountEntryMatching_ESFILiquidityAccountEntry_OI.GID
     LEFT JOIN ESFILiquidityAccountEntry AS FK_ESFILiquidityAccountEntryMatching_ESFILiquidityAccountEntry_CI
       ON ESFILiquidityAccountEntryMatching.fCloseEntryGID = FK_ESFILiquidityAccountEntryMatching_ESFILiquidityAccountEntry_CI.GID
where FK_ESFILiquidityAccountEntry_ESFILiquidityAccountEntryMatching1.ValeurDate  between @_FROM_DATECRITERIO and @_TO_DATECRITERIO 
 and left(FK_ESFILiquidityAccountEntry_ESFILiquidityAccountEntryMatching1.DocumentCode,3) in  ('ΤΠΥ','ΔΧΧ','ΜΕΤ','ΧΡΠ') 
 and left(FK_ESFILiquidityAccountEntryMatching_ESFILiquidityAccountEntry_CI.DocumentCode,3) not in  ('ΠΤΥ','ΧΡΠ')
 and left(FK_ESFILiquidityAccountEntryMatching_ESFILiquidityAccountEntry_CI.DocumentCode,4) not in  ('ΠΤΥ','ΑΧΡΠ')
 and isnull(FK_ESFILiquidityAccountEntry_ESFILiquidityAccountEntryMatching1.fDimension2Code,'ΜΗ ΥΠΑΡΚΤΟΣ') in (@DocumentType)
 

select sum(c.CloseCurrencyAmount) as CloseCurrencyAmount
INTO #CloseAmountSum 
 from #CloseAmountDates c  



SELECT distinct  ESFILiquidityAccountEntry.GID AS ISUDGID, 
       FK_ESFILiquidityAccountEntry_ESFITradeAccount.Code + ' - ' + FK_ESFILiquidityAccountEntry_ESFITradeAccount.Name  AS Συναλλασσόμενος, 
       ESFILiquidityAccountEntry.DocumentCode AS DocumentCode, 
       ESFILiquidityAccountEntry.ValeurDate AS ValeurDate,
       ESFILiquidityAccountEntry.RegistrationDate AS RegistrationDate, 
PayableAmount = case when pty.CloseCurrencyAmount is not null  
                     then (case when left(ESFILiquidityAccountEntry.DocumentCode,3)in ('ΠΛΛ')
                               then 0 
					           else case  ESFILiquidityAccountEntry.MatchingSign 
										when 0 then ESFILiquidityAccountEntry.CurrencyValue 
										else 0
						            end 
				           end) - pty.CloseCurrencyAmount 
                     else 
				 (case when left(ESFILiquidityAccountEntry.DocumentCode,3)in ('ΠΛΛ')
                                then 0 
					            else case  ESFILiquidityAccountEntry.MatchingSign 
										when 0 then ESFILiquidityAccountEntry.CurrencyValue 
										else 0
						             end 
				            end) 
				 end
				 ,

PaidAmount =case when pty.CloseCurrencyAmount is not null then oa.CloseCurrencyAmount - pty.CloseCurrencyAmount else oa.CloseCurrencyAmount end,
LateAmount = case when ( case when ESFILiquidityAccountEntry.ValeurDate &lt;  @_REFERENCEDATE
	                                   then(ESFILiquidityAccountEntry.CurrencyValue - ESFILiquidityAccountEntry.CurrencyClosedAmount) 
			                           else 0
				         end)&gt;0
						 and (case when left(ESFILiquidityAccountEntry.DocumentCode,3)='ΠΛΛ'
                                   then 0 
					               else case  ESFILiquidityAccountEntry.MatchingSign 
								          when 0 then ESFILiquidityAccountEntry.CurrencyValue 
								          else 0
						                end 
				               end)= 0
					then 0 
					else (case when ESFILiquidityAccountEntry.ValeurDate &lt; cast(getdate() as datetime)   
		                     then case when ESFILiquidityAccountEntry.ValeurDate &lt;  @_REFERENCEDATE
	                                   then(ESFILiquidityAccountEntry.CurrencyValue - ESFILiquidityAccountEntry.CurrencyClosedAmount) 
			                           else 0
		                          end 
					         else 0
				         end)
                end, -- Αν το lateAmount είναι μεγαλύτερο του 0 και το Payable είναι 0 τότε Late  = 0






NotExpiredAmount = (     case when ESFILiquidityAccountEntry.ValeurDate &gt;=  @_REFERENCEDATE
	                               then(ESFILiquidityAccountEntry.CurrencyValue - ESFILiquidityAccountEntry.CurrencyClosedAmount) 
			                       else 0
					      end),	   				  	

       ESFILiquidityAccountEntry.fDocumentGID AS fDocumentGID, 
		 ESFILiquidityAccountEntry.fTradeAccountGID,
		 ESFILiquidityAccountEntry.fDimension2Code
into #Detailed
FROM ESFILiquidityAccountEntry AS ESFILiquidityAccountEntry
     join ESFITradeAccount on ESFITradeAccount.GID = ESFILiquidityAccountEntry.fTradeAccountGID
	   left join #CloseAmount oa on oa.fOpenEntryGID = ESFILiquidityAccountEntry.GID
	 left join #CloseAmountPTY pty on pty.fOpenEntryGID = ESFILiquidityAccountEntry.GID
     LEFT JOIN ESFITradeAccount AS FK_ESFILiquidityAccountEntry_ESFITradeAccount
       ON ESFILiquidityAccountEntry.fTradeAccountGID = FK_ESFILiquidityAccountEntry_ESFITradeAccount.GID
     LEFT JOIN ESFINote AS FK_ESFILiquidityAccountEntry_ESFINote
       ON ESFILiquidityAccountEntry.fNoteGID = FK_ESFILiquidityAccountEntry_ESFINote.GID
     LEFT JOIN ESFILineItem AS AdjustmentLine
       ON ESFILiquidityAccountEntry.fDocumentLineGID = AdjustmentLine.GID AND 3 = AdjustmentLine.DocumentClass
     LEFT JOIN ESFIDocumentTrade AS TradeDocument
       ON ESFILiquidityAccountEntry.fDocumentGID = TradeDocument.GID
     LEFT JOIN ESFIDocumentCash AS CashDocument
       ON ESFILiquidityAccountEntry.fDocumentGID = CashDocument.GID
     LEFT JOIN ESGOSites AS FK_ESFILineItem_ESGOSites
       ON AdjustmentLine.fSiteGID = FK_ESFILineItem_ESGOSites.GID
     LEFT JOIN ESGOSites AS FK_ESGOSites_ESFIDocumentTrade_TradeAccountSite
       ON TradeDocument.fTradeAccountSiteGID = FK_ESGOSites_ESFIDocumentTrade_TradeAccountSite.GID
     LEFT JOIN ESGOSites AS FK_ESGOSites_ESFIDocumentCash_TradeAccountSite
       ON CashDocument.fTradeAccountSiteGID = FK_ESGOSites_ESFIDocumentCash_TradeAccountSite.GID
     
WHERE ESFILiquidityAccountEntry.fCompanyCode = '001'
       AND ESFILiquidityAccountEntry.MatchingSign &lt;&gt; 2
       AND ESFILiquidityAccountEntry.CancelState &lt;&gt; 1
       AND ESFILiquidityAccountEntry.CancelState &lt;&gt; 2
	   and ESFITradeAccount.Type = 0 -- Returns customers and no suppliers
       AND (case 
                  when IsNull(FK_ESFILiquidityAccountEntry_ESFINote.Code,'A') &lt;&gt; 'A' then 1
                  when IsNull(FK_ESFILiquidityAccountEntry_ESFINote.Code,'A')= 'A'  then 2
             end = 2/*@Criterio*/
			)
       AND (Case 1 
                 when 0 then ESFILiquidityAccountEntry.RegistrationDate
                 when 1 then ESFILiquidityAccountEntry.ValeurDate
              end BETWEEN @_FROM_DATECRITERIO AND @_TO_DATECRITERIO)
       AND ((ESFILiquidityAccountEntry.Fullfilled IN (0/*@FULLFILLED_ITEM1*/,1/* @FULLFILLED_ITEM2*/)))
	   ---- Λαμβάνουμε μόνο πελάτες που έχουν τιμολόγιο στην δεδομένη περίοδο
	   and exists (select 1 
	                 from ESFILiquidityAccountEntry ie 
					where ie.ValeurDate BETWEEN @_FROM_DATECRITERIO AND @_TO_DATECRITERIO 
					  and ie.fTradeAccountGID = ESFILiquidityAccountEntry.fTradeAccountGID
					  and left(ie.DocumentCode,3)='ΤΠΥ')
	   and left(ESFILiquidityAccountEntry.DocumentCode,3) in ('ΤΠΥ','ΔΧΧ','ΜΕΤ')
	   --and not exists (select 1 from )
	   and isnull(ESFILiquidityAccountEntry.fDimension2Code,'ΜΗ ΥΠΑΡΚΤΟΣ') in (@DocumentType)
--ΕΔΩ ΘΑ ΜΠΕΙ Η ΣΥΝΘΗΚΗ ΠΟΥ ΘΑ ΒΡΙΣΚΕΙ ΤΑ ΑΝΕΞΟΦΛΗΤΑ

select d.* 
  into #NotPaid
  from #Detailed d
  where  isnull(d.LateAmount,0) &gt;0 and
     d.ValeurDate &lt; getdate()
	and d.[PayableAmount] &gt; 0 -- μόνο τα παραστατικά που έχουν απαιτητό ποσό

  



select sum(NotExpiredAmount) as NotExpiredAmount
into #NotExpiredSum
  from  #Detailed




select sum(LateAmount) as  LateAmount
into #NotPaidSum
from #NotPaid n 


	   select  isnull(cl.CloseCurrencyAmount,0)+isnull(np.LateAmount,0)+isnull(ne.NotExpiredAmount,0) as 'Απαιτητό Ποσό',-- Απαιτητό μείον πιστωτικά
	           isnull(cl.CloseCurrencyAmount,0) as 'Εξοφλημένο',
			   isnull(np.LateAmount,0) as 'Ανεξόφλητο',
			   isnull(ne.NotExpiredAmount,0) as 'Άληκτο'--,

		 into #indicator
	     from #detailed d 
		 cross join #NotPaidSum np 
		 cross join #CloseAmountSum cl
         cross join #NotExpiredSum ne




select a.* 
into #a 
from (
--Εξοφλημένα
select   i.[Απαιτητό Ποσό],
         i.[Εξοφλημένο],
		 i.[Ανεξόφλητο],
		 i.[Άληκτο],
		  case when isnull(i.Εξοφλημένο,0)  = 0 then  0 else (datediff(day,XrewshValeur,PistwshRegistration) * CloseCurrencyAmount)/i.Εξοφλημένο end as 'Μέσος Χρόνος Εξόφλησης/Ημ.Λήξης',
		 case when isnull(i.Εξοφλημένο,0)  = 0 then  0 else (datediff(day,xrewshRegistration,PistwshRegistration) * CloseCurrencyAmount)/i.Εξοφλημένο end as 'Μέσος Χρόνος Εξόφλησης/Ημ.Έκδοσης'
		 ,NULL as 'Μέσος Χρόνος Καθηστέρησης'
		 ,XrewshValeur,PistwshRegistration,xrewshRegistration,CloseCurrencyAmount
		 ,ca.DocumentCode1,ca.DocumentCode,ca.code,ca.fDimension2Code as 'Τύπος Παραστατικού',
		 ca.fOpenEntryGID,ca.GID
		 
 from #CloseAmountDates ca
 cross join #indicator i 



 union 

-- Ανεξόφλητα

select   i.[Απαιτητό Ποσό],
         i.[Εξοφλημένο],
		 i.[Ανεξόφλητο],
		 i.[Άληκτο],
		 null,
		 null,
		 case when i.Ανεξόφλητο  = 0   then 0 else (datediff(day,ValeurDate,@_REFERENCEDATE)*d.LateAmount/i.Ανεξόφλητο) end as 'Μέσος Χρόνος Καθηστέρησης'
	     ,ValeurDate,null,null,d.LateAmount
		 ,d.DocumentCode,NULL,d.Συναλλασσόμενος,d.fDimension2Code,null,null 
		 
 from #NotPaid d 
 cross join #indicator i 


)a




select a.[Απαιτητό Ποσό],
       a.Εξοφλημένο,
	   a.Ανεξόφλητο,
	   a.Άληκτο,
	   sum(a.[Μέσος Χρόνος Εξόφλησης/Ημ.Λήξης]) as 'Μέσος Χρόνος Εξόφλησης/Ημ.Λήξης',  
	   sum(a.[Μέσος Χρόνος Εξόφλησης/Ημ.Έκδοσης]) as 'Μέσος Χρόνος Εξόφλησης/Ημ.Έκδοσης',
	   sum(a.[Μέσος Χρόνος Καθηστέρησης]) as 'Μέσος Χρόνος Καθηστέρησης'
	   from #a a
	   group by  a.[Απαιτητό Ποσό],
       a.Εξοφλημένο,
	   a.Ανεξόφλητο,
	   a.Άληκτο
</CommandText>
      </Query>
