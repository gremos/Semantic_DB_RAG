  <DataSets>
    <DataSet Name="DS_Main">
      <Query>
        <DataSourceName>YPS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@dateFrom">
            <Value>=Parameters!DateFrom.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@dateTo">
            <Value>=Parameters!DateTo.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ContractLineStatus">
            <Value>=Parameters!ContractLineStatus.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@FinancialFlag">
            <Value>=Parameters!FinancialFlag.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@MarketingFlag">
            <Value>=Parameters!MarketingFlag.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@PrefectureParam">
            <Value>=Parameters!PrefectureParam.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>DECLARE	@Rpt TABLE
	(
	  BranchCode NVARCHAR(50) ,
	  a_id INT ,
	  CustomerName NVARCHAR(150) ,
	  BrandName NVARCHAR(150) ,
	  ContractCode INT ,
	  SalesCode NVARCHAR(20) ,
	  SignDate DATETIME ,
	  ContractProduct NVARCHAR(100) ,
	  AdvertisementProduct NVARCHAR(100) ,
	  AdvertisementStatus NVARCHAR(40) ,
	  AdvertisementPrice DECIMAL(18, 3) ,
	  FinancialDptFlag NVARCHAR(40) ,
	  MarketingDptFlag NVARCHAR(40) ,
	  PaymentMethod NVARCHAR(100) ,
	  Campaign NVARCHAR(254) ,
	  Channel NVARCHAR(20) ,
	  heading NVARCHAR(MAX) ,
	  Profession NVARCHAR(150) ,
	  Prefecture NVARCHAR(250)
	);
 /*SET @DateTo = '2013-01-30'*/
INSERT	INTO @Rpt
		SELECT DISTINCT
				bp.Code AS BranchCode ,
				a.id a_id ,
				bp.BrandName AS CustomerName ,
				bp.NAME AS BrandName ,
				c.ID AS ContractCode ,
				u.SalesCode ,
				c.SignedDate AS SignDate ,
				p.NAME AS ContractProduct ,
				CASE WHEN p.IsComposite = 0 THEN ''
					 ELSE ap.Name
				END AS AdvertisementProduct ,
				CASE WHEN a.qa = 0 THEN 'Σε εξέλιξη'
					 ELSE 'Ολοκληρωμένο'
				END AS AdvertisementStatus ,
				AdvertisementPrice = a.Price - a.Discount - a.RulesDiscount ,
				CASE WHEN ( pmt.PaymentWay NOT IN ( 2, 5 )
							OR
                                                                                                                                                                                                                /*OTE-TAXYPL*/ pm.AdvancedPayment &gt; 0
						  ) THEN 'ΑΜΕΣΗ ΤΙΜΟΛΟΓΗΣΗ'
					 ELSE 'ΔΕΝ ΧΡΗΖΕΙ ΑΜΕΣΗΣ ΤΙΜ.'
				END AS FinancialDptFlag ,
				CASE WHEN pmt.id = 269 THEN 'ΑΝΤΑΠΟΔΟΤΙΚΗ ΣΥΝΕΡΓΑΣΙΑ'
					 ELSE ''
				END AS MarketingDptFlag ,
				pmt.DESCRIPTION AS PaymentMethod ,
				Campaign = ( SELECT TOP 1
									cv.NAME + ' ' + cv.Code AS code
							 FROM	dbo.TargetGroupItemValue tgiv
									JOIN dbo.TargetGroupItem tgi ON tgiv.TargetGroupItemID = tgi.ID
									JOIN dbo.Campaign cv ON cv.ID = tgi.CampaignID
							 WHERE	C.ID = tgiv.ContractID
						   ) ,
				ISNULL(ct.Description, srct.Description) AS Channel ,
				Heading = CASE WHEN CHARINDEX('.',
											  SUBSTRING(Hvgi.Description, 1,
														CHARINDEX('-',
															  Hvgi.Description))) &lt;&gt; 0
							   THEN HvgiC.Description
							   ELSE Hvgi.[Description]
						  END , 
                                                                                                                                                                                                                /*hvgi.Description AS heading ,*/
				bp.Profession ,
				adr.Prefecture
		FROM	Contract AS c
				JOIN dbo.[User] u ON c.SalesManID = u.ID
				LEFT JOIN dbo.Unit unt ON u.UnitID = unt.ID
				LEFT JOIN dbo.ChannelType srct ON unt.ChannelTypeID = srct.ID /*LEFT JOIN dbo.SubChannel sc ON c.SubChannelID = sc.ID*/
				LEFT
                                                                                                                                                                                                                 JOIN dbo.ChannelType ct ON c.ChannelTypeID = ct.id /*sc.ChannelTypeID = ct.ID*/
				JOIN dbo.ContractProduct AS cp ON c.ID = cp.RelatedContractID
				JOIN dbo.Advertisement a ON cp.ID = a.ContractProductID
				JOIN dbo.Product p ON cp.ProductID = p.ID
				JOIN dbo.Product ap ON a.ProductID = ap.id
				JOIN dbo.BusinessPoint AS bp ON bp.ID = c.ActualBPOwnerID
				JOIN FullAddressViewV2 adr ON adr.id = bp.LocationAddressID
				JOIN dbo.PaymentMethod AS pm ON cp.PaymentMethodID = pm.ID
				JOIN dbo.PaymentMethodType AS pmt ON pm.PaymentMethodTypeID = pmt.ID
				LEFT 
                                                                                                                                                                                                                JOIN dbo.VersionGroupItem Hvgi ON hvgi.ID = a.HeadingID
				LEFT JOIN dbo.VersionGroupItem Hvgic ON Hvgi.ParentID = HvgiC.ID
		WHERE	c.[State] NOT IN ( 3, 5 ,10)
				AND ( c.type IN ( 0, 2, 3, 4 ) )
				AND cp.State &lt;&gt; 3
				AND ap.IsComposite = 0
				AND ( ( c.SignedDate BETWEEN @dateFrom
									 AND	 DATEADD(day, 1, @dateTo)
						AND c.OrigionalContractID IS NULL
					  )
					  OR ( c.CreatedOn BETWEEN @dateFrom
									   AND	   DATEADD(day, 1, @dateTo)
						   AND c.OrigionalContractID IS NOT NULL
						 )
					)
				AND a.qa IN ( @ContractLineStatus )
INSERT	INTO @Rpt
		SELECT DISTINCT
				bp.Code AS BranchCode ,
				a.id a_id ,
				bp.BrandName AS CustomerName ,
				bp.NAME AS BrandName ,
				c.ID AS ContractCode ,
				u.SalesCode ,
				c.SignedDate AS SignDate ,
				p.NAME AS ContractProduct ,
				CASE WHEN p.IsComposite = 0 THEN ''
					 ELSE ap.Name
				END AS AdvertisementProduct ,
				CASE WHEN a.qa = 0 THEN 'Σε εξέλιξη'
					 ELSE 'Ολοκληρωμένο'
				END AS AdvertisementStatus ,
				AdvertisementPrice = a.Price - a.Discount - a.RulesDiscount ,
				CASE WHEN ( pmt.PaymentWay NOT IN ( 2, 5 )
							OR apm.Amount &gt; 0
						  ) THEN 'ΑΜΕΣΗ ΤΙΜΟΛΟΓΗΣΗ'
					 ELSE 'ΔΕΝ ΧΡΗΖΕΙ ΑΜΕΣΗΣ ΤΙΜ.'
				END AS FinancialDptFlag ,
				CASE WHEN pmt.id = 269 THEN 'ΑΝΤΑΠΟΔΟΤΙΚΗ ΣΥΝΕΡΓΑΣΙΑ'
					 ELSE ''
				END AS MarketingDptFlag ,
				pmt.DESCRIPTION AS PaymentMethod ,
				Campaign = ( SELECT TOP 1
									cv.NAME + ' ' + cv.Code AS code
							 FROM	dbo.TargetGroupItemValue tgiv
									JOIN dbo.TargetGroupItem tgi ON tgiv.TargetGroupItemID = tgi.ID
									JOIN dbo.Campaign cv ON cv.ID = tgi.CampaignID
							 WHERE	C.ID = tgiv.ContractID
						   ) ,
				ISNULL(ct.Description, srct.Description) AS Channel ,
				Heading = CASE WHEN CHARINDEX('.',
											  SUBSTRING(Hvgi.Description, 1,
														CHARINDEX('-',
															  Hvgi.Description))) &lt;&gt; 0
							   THEN HvgiC.Description
							   ELSE Hvgi.[Description]
						  END , 
                                                                                                                                                                                                                                                                            /*hvgi.Description AS heading ,*/
				bp.Profession ,
				adr.Prefecture
		FROM	Contract AS c
				JOIN dbo.[User] u ON c.SalesManID = u.ID
				LEFT JOIN dbo.Unit unt ON u.UnitID = unt.ID
				LEFT JOIN dbo.ChannelType srct ON unt.ChannelTypeID = srct.ID /*LEFT JOIN dbo.SubChannel sc ON c.SubChannelID = sc.ID*/
				LEFT
                                                                                                                                                                                                                                                                             JOIN dbo.ChannelType ct ON c.ChannelTypeID = ct.id /*sc.ChannelTypeID = ct.ID*/
				JOIN dbo.ContractProduct AS cp ON c.ID = cp.RelatedContractID
				JOIN dbo.Advertisement a ON cp.ID = a.ContractProductID
				JOIN dbo.Product p ON cp.ProductID = p.ID
				JOIN dbo.Product ap ON a.ProductID = ap.id
				JOIN dbo.BusinessPoint AS bp ON bp.ID = c.ActualBPOwnerID
				JOIN FullAddressViewV2 adr ON adr.id = bp.LocationAddressID
				JOIN dbo.InvoicePaymentMethod AS ipm ON c.id = ipm.ID
				LEFT JOIN dbo.advancedPaymentMethod AS apm ON c.id = apm.ID
				JOIN dbo.PaymentMethodType AS pmt ON ipm.PaymentMethodTypeID = pmt.ID
				LEFT JOIN dbo.VersionGroupItem Hvgi ON hvgi.ID = a.HeadingID
				LEFT JOIN dbo.VersionGroupItem Hvgic ON Hvgi.ParentID = HvgiC.ID
		WHERE	c.[State] NOT IN ( 3, 5 , 10)
				AND ( c.type IN ( 0, 2, 3, 4 ) )
				AND cp.State &lt;&gt; 3
				AND ap.IsComposite = 0
				AND ( ( c.SignedDate BETWEEN @dateFrom
									 AND	 DATEADD(day, 1, @dateTo)
						AND c.OrigionalContractID IS NULL
					  )
					  OR ( c.CreatedOn BETWEEN @dateFrom
									   AND	   DATEADD(day, 1, @dateTo)
						   AND c.OrigionalContractID IS NOT NULL
						 )
					)
				AND a.qa IN ( @ContractLineStatus )

SELECT	BranchCode ,
		a_id ,
		CustomerName ,
		BrandName ,
		ContractCode ,
		SalesCode ,
		CONVERT(NVARCHAR(15), SignDate, 103) AS SignDate ,
		ContractProduct ,
		AdvertisementProduct ,
		AdvertisementStatus ,
		AdvertisementPrice ,
		FinancialDptFlag ,
		MarketingDptFlag ,
		PaymentMethod ,
		Campaign ,
		Channel ,
		heading ,
		Profession ,
		Prefecture
FROM	@Rpt
WHERE	FinancialDptFlag IN ( @FinancialFlag )
		AND MarketingDptFlag IN ( @MarketingFlag )
		AND Prefecture IN ( @PrefectureParam )</CommandText>
      </Query>
