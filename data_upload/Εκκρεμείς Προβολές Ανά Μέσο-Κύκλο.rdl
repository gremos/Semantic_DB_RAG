    <DataSet Name="MediumsDataSet">
      <Query>
        <DataSourceName>YPS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Inactive">
            <Value>=Parameters!Inactive.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT  m.Name + '( ' + mv.Code + '-' + m.Code +  ' )' AS MName, mv.ID
FROM    dbo.Medium m
        JOIN dbo.MediumVersion mv ON m.ID = mv.MediumID
WHERE   (m.IsActive = 1
        AND mv.IsActive = 1
	and @Inactive = 0)
or @Inactive = 1
        --AND MediumTypeID = 3
ORDER BY m.Name</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="ID">
          <DataField>ID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="MName">
          <DataField>MName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
