        <DataSourceName>YPS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@mediumid">
            <Value>=Parameters!mediumid.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@versioncode">
            <Value>=Parameters!versioncode.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT	mv.name AS medium ,
		mv.MediumID AS mediumCode ,
		mv.code AS mediumVersion ,
		vs.Name AS mediumSection ,
		usr.LastName ,
		usr.SalesCode ,
		pr.Code AS prCode ,
		pr.Name AS prName ,
		COUNT(*) AS CntofAdv
FROM	contract ct
		JOIN contractproduct cp ON cp.relatedcontractid = ct.id
		JOIN advertisement ad ON cp.id = ad.contractproductid
		JOIN versionsection vs ON ad.versionsectionid = vs.id
		JOIN product pr ON pr.id = ad.productid
		JOIN MediumVersion mv ON mv.ID = vs.MediumVersionID
		JOIN [User] usr ON usr.ID = ct.SalesManID
WHERE	mv.MediumID = @mediumid
		AND ( ct.type IN ( 0, 2, 3, 4 ) )
		AND mv.Code = @versioncode
		AND pr.iscomposite = 0
		AND ct.State NOT IN ( 3, 5, 10 )
		AND cp.State &lt;&gt; 3
GROUP BY mv.name ,
		mv.MediumID ,
		mv.code ,
		vs.Name ,
		usr.LastName ,
		usr.SalesCode ,
		pr.Code ,
		pr.Name
ORDER BY 4 ,
		5</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="medium">
          <DataField>medium</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="mediumCode">
          <DataField>mediumCode</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="mediumVersion">
          <DataField>mediumVersion</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="mediumSection">
          <DataField>mediumSection</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="LastName">
          <DataField>LastName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="SalesCode">
          <DataField>SalesCode</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="prCode">
          <DataField>prCode</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="prName">
          <DataField>prName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CntofAdv">
          <DataField>CntofAdv</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
