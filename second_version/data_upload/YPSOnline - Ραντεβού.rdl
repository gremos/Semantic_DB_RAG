    <DataSet Name="Activity">
      <Query>
        <DataSourceName>YPS</DataSourceName>
        <CommandText>	select ISNULL(ActivityName,'') as ActivityName from(
	select ActivityName from 
 [YPS].[dbo].[ViewActivityHeader]  union

 select distinct MainActivity as ActivityName  from [avocet\avocet].XODB.ypsonline.appointment
 ) a

 order by ActivityName </CommandText>
      </Query>
