  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>YPS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@datefrom">
            <Value>=Parameters!datefrom.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@dateto">
            <Value>=Parameters!dateto.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@channeltypeid">
            <Value>=Parameters!channeltypeid.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>
;with released as (
select cmp.Description as campaign,ct.ID as ctid,ct.ChannelTypeID ,
    --   compid=(select MAX( M.CompanyID)
			 --from  ContractProduct AS CP 
			 --INNER JOIN Product AS P ON CP.ProductID = P.ID
			 --INNER JOIN MediumVersion AS MV ON P.MediumVersionID = MV.ID
			 --INNER JOIN Medium AS M ON MV.MediumID = M.ID
			 --where cp.relatedcontractid=ct.id
			 --and cp.State&lt;&gt;3)  ,
       value=(select cast(sum(CASE WHEN cp.state = 4 THEN cp.AlteredAmount ELSE  cp.Price - cp.Discount - cp.RulesDiscount	END) as float)  
			       from yps.dbo.ContractProduct cp where cp.RelatedContractID=ct.id and cp.State&lt;&gt;3)   ,
	   adv_flg=Case when adv.ID IS not null then 1 else 0 end,
	   cash_flg=(Case when pt.id  in(414,435,431,272,437,438,429)  then  1 else 0 end) ,
       cast(adv.Amount as float)as adv_value ,pt.id ptid
from dbo.Contract ct
join dbo.Campaign cmp on ct.CampaignID=cmp.id
left join dbo.AdvancedPaymentMethod adv on adv.ID=ct.id
join dbo.InvoicePaymentMethod pm on pm.ID=ct.id
join dbo.PaymentMethodType pt on pt.ID=pm.PaymentMethodTypeID
where ct.OrigionalContractID is null
and ct.type not IN( 1,10)
and ct.State not IN (3,5)
and ct.CampaignID not IN (25,26,30,32,38,42,43,44,45,47,23,39) 
and ct.createdon&gt;='20130411'
and ct.ReleaseDate&gt;=@datefrom
and ct.ReleaseDate&lt;@dateto+1
and ct.ChannelTypeID in (@channeltypeid)
 
)

,alldata as (
select * ,
      est_value=Case  when channeltypeid=2 then  value*0.3
               when ChannelTypeID=1 and value&lt;=300 then 50 
               when ChannelTypeID=1 and value&gt;300 then 100
               end
               
from released
where value&lt;&gt;0
--and compid=1
)

select campaign,
        --released--
        COUNT(ctid) released_Contracts, SUM(value) released_valueContracts, SUM(est_value) released_estimatedAdvanceValue, 
        --cash---
        sum(case when cash_flg=1 then 1 else 0 end) as cash_Contracts,
        sum(case when cash_flg=1 then value else 0 end) as cash_valueContracts,
        --advance---
        sum(case when adv_flg=1 and cash_flg=0 then 1 else 0 end) as adv_Contracts,
        sum(case when adv_flg=1 and cash_flg=0 then value else 0 end) as adv_valueContracts,
        sum(case when adv_flg=1 and cash_flg=0 then est_value else 0 end) as adv_estimatedAdvanceValue,
        sum(case when adv_flg=1 and cash_flg=0 then adv_value else 0 end) as adv_advancevalue,
        --no advance--
        sum(case when cash_flg=0 and adv_flg=0 then 1 else 0 end) as noadvance_Contracts,
        sum(case when cash_flg=0 and adv_flg=0 then value else 0 end) as noadvance_valueContracts,
        sum(case when cash_flg=0 and adv_flg=0 then est_value else 0 end) as noadvance_estimatedAdvanceValue
from alldata a
group by campaign</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
