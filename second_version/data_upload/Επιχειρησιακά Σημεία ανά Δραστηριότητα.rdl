--SET @BpUpdateTo = '2017-09-30';--
--SET @BpName = 'an';--                              
--SET @BpBrandName = 'an';--
--SET @xolisting = 1;
--SET @activity = 1;
--SET @MODE = 1;

IF @MODE = 0

BEGIN

if object_id('tempdb..#activities') is not null drop table #activities
SELECT items
into #activities
FROM itapps.dbo.SplitToInt(@allActivities, ',')

if object_id('tempdb..#csids') is not null drop table #csids
SELECT items
into #csids
FROM itapps.dbo.SplitToInt(@allCS, ',')

if object_id('tempdb..#cuids') is not null drop table #cuids
SELECT items
into #cuids
FROM itapps.dbo.SplitToInt(@allCU, ',')

if object_id('tempdb..#pref') is not null drop table #pref
SELECT items
into #pref
FROM itapps.dbo.Split(@allPrefectures, ',')

if object_id('tempdb..#mun') is not null drop table #mun
SELECT items
into #mun
FROM itapps.dbo.Split(@allMunicipality, ',')


if object_id('tempdb..#ids') is not null drop table #ids
select distinct bp.id id, ISNULL(bp.BillingAddressID, bilbp.LocationAddressID ) as biladrid, 
CustomerSource=cs.Description, CustomerUpdate=cu.Description, CustomerSolvency = solv.Description,
LocationAddId = bp.LocationAddressID
INTO #ids
from dbo.BusinessPoint bp 
LEFT JOIN dbo.CustomerSolvency solv ON solv.ID = bp.CustomerSolvencyID
left JOIN dbo.CustomerSource cs ON cs.id = bp.CustomerSourceID
left JOIN dbo.CustomerUpdate cu ON cu.id = bp.CustomerUpdateID
JOIN dbo.Classification cl ON bp.id = cl.BusinessPointID AND cl.ClassifierID IN (3,4)
LEFT JOIN dbo.PointRelationship pr WITH (NOLOCK) ON pr.BusinessPointID = bp.id AND pr.type = 10
LEFT JOIN dbo.BusinessPoint bilbp WITH (NOLOCK) ON pr.TargetBussinessPointID = bilbp.id 
join #activities act on cl.ClassifierNodeID = act.items
join #csids csi on csi.items =  isnull(bp.CustomerSourceID,-1)
join #cuids cui on cui.items =  isnull(bp.CustomerUpdateID ,-1)

where bp.IsActive = 1 
and cast(bp.CreatedOn as date) &gt;= @DateFrom--
and cast(bp.CreatedOn as date) &lt;= @DateTo--
and (bp.name like '%'+@BpName+'%' or bp.BrandName like '%'+@BpName+'%' or @BpName is null ) --
AND cast(bp.CustomerUpdateModifiedOn as date) &gt;= @BpUpdateFrom--
AND cast(bp.CustomerUpdateModifiedOn as date) &lt;= @BpUpdateTo--
--AND cl.ClassifierNodeID IN (@ids)
--AND bp.CustomerSourceID IN (@csid) 
--AND bp.CustomerUpdateID IN (@cuid)


if object_id('tempdb..#CustIndic') is not null drop table #CustIndic
SELECT clf.BusinessPointID, CustomerIndicator=cn.Name	
INTO #CustIndic
FROM #ids id
JOIN dbo.Classification clf ON clf.BusinessPointID = id.id
JOIN dbo.ClassifierNode cn ON cn.ID = clf.ClassifierNodeID
WHERE ClassifierID=5

if object_id('tempdb..#CustMainActivity') is not null drop table #CustMainActivity
SELECT clf.BusinessPointID, MainActivity=cn.Name	
INTO #CustMainActivity
FROM #ids id
JOIN dbo.Classification clf ON clf.BusinessPointID = id.id
JOIN dbo.ClassifierNode cn ON cn.ID = clf.ClassifierNodeID
WHERE ClassifierID=3



if object_id('tempdb..#avct') is not null drop table #avct
select count(distinct ct.id) AS ctid, MaxEndDate=MAX(adv.AdvEndDate), MaxSignDate=MAX(ct.SignedDate), ct.ActualBPOwnerID
INTO #avct
from #ids id
JOIN Contract ct ON ct.ActualBPOwnerID = id.id
join ContractProduct cp ON cp.RelatedContractID = ct.id and ct.State NOT IN (3,5) and ct.Type not in (1,10)
join Advertisement adv ON adv.ContractProductID = cp.id and (adv.AdvEndDate &gt; GETDATE())
group by ct.ID, ct.ActualBPOwnerID

