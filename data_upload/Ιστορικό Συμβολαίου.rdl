    <DataSet Name="ContractValueDataset">
      <Query>
        <DataSourceName>YPS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ContractID">
            <Value>=Parameters!ContractID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select ContractID = ct.id,
Price = sum(cp.price),
SDDiscount = sum(cp.discount),
RulesDiscount = sum(cp.rulesdiscount),
ProductDiscount=sum(cp.ProductDiscount),
PaymentDiscount=sum(cp.PaymentDiscount),
BundleDiscount= sum(cp.BundleDiscount),
AlteredDiffAmount = sum(case when cp.state in (3,4) then cp.price - cp.discount - cp.rulesdiscount - isnull(cp.AlteredAmount,0) else 0 end),
PriceAfterDiscount = sum(case when cp.state in (3,4) then isnull(AlteredAmount,0) else cp.Price - cp.Discount - cp.RulesDiscount end)

from Contract ct
join ContractProduct cp on ct.id = cp.RelatedContractID
where ct.id =  @ContractID
and ct.type in (2,3,4,5)
group by ct.id</CommandText>
      </Query>
