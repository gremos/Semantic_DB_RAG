  <DataSources>
    <DataSource Name="YPS">
      <DataSourceReference>YPS</DataSourceReference>
      <rd:SecurityType>Integrated</rd:SecurityType>
      <rd:DataSourceID>04a28ea6-b7a3-48ad-8a44-8dad0a917d10</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>YPS</DataSourceName>
        <CommandText> with xx as (
 select distinct  2 as xx,cmp.id cm_id, CampaignDesc = '['+cmp.Code +'] ' + cmp.Name , TaskDesc = t.Description, t.TaskTypeID  ,
       Case when t.tasktypeid in (192,509,194) then 'Graphic Designer' when t.tasktypeid in (514,517,519) then 'Web Designer' 
             when t.tasktypeid in (520,523,524) then 'Google Ads Expert' when t.tasktypeid in (525,528,529) then 'SEO Expert' 
             when t.tasktypeid in (511,513) then 'Copywriter' when t.tasktypeid in (193,512) then 'Directories QC'  
             when t.tasktypeid in (516, 522, 527) then 'Web Services QC' end as [RoleDesc]
      , taskid = t.id , cp.RelatedContractID contractid, adv.ID as advid
from  yps.dbo.[Case] c with (nolock)
        JOIN yps.dbo.[Case] cc with (nolock) ON cc.MasterCaseID = c.ID
        JOIN yps.dbo.Task t with (nolock) ON t.CaseID = cc.ID
        join yps.dbo.Advertisement adv with (nolock) on adv.ID=cc.ContextEntityID and cc.ContextEntityType='adv' 
        join yps.dbo.ContractProduct cp  with (nolock)on cp.id= adv.ContractProductID
		JOIN yps.dbo.Product p with (nolock) ON cp.ProductID = p.ID
		join yps.dbo.Contract ct with (nolock) on ct.ID=cp.RelatedContractID
		join yps.dbo.Campaign cmp with (nolock) on cmp.ID=ct.CampaignID
		where t.TaskTypeID in (192,193,194,509,511,512,513,514,516,517,519,520,522,523,524,525,527,528,529)
		and (t.Status=0  and cc.state=0) 
		and ((adv.productid = cp.productid AND p.IsComposite = 0) OR (adv.productid &lt;&gt; cp.productid AND p.IsComposite = 1))
union
		
		
select distinct 	1 as xx, cmp.id cm_id, CampaignDesc = '['+cmp.Code +'] ' + cmp.Name , TaskDesc = t.Description, t.TaskTypeID  ,
       'Production Controller' as [RoleDesc] ,  taskid = t.id , cp.RelatedContractID contractid, adv.ID as advid
from  yps.dbo.TargetGroupItemValue tgiv with (nolock)
        JOIN yps.dbo.[Case] c with (nolock) ON c.ContextEntityID = tgiv.ID AND c.ContextEntityType = 'TGIV'
        JOIN yps.dbo.[Case] cc with (nolock)ON cc.MasterCaseID = c.ID
        JOIN yps.dbo.Task t with (nolock)ON t.CaseID = cc.ID
		join yps.dbo.ContractProduct cp  with (nolock)on cp.RelatedContractID= tgiv.ContractID
		join yps.dbo.Advertisement adv with (nolock) on adv.ContractProductID=cp.id
		JOIN yps.dbo.Product p with (nolock) ON cp.ProductID = p.ID
		join yps.dbo.Contract ct with (nolock) on ct.ID=cp.RelatedContractID
		join yps.dbo.Campaign cmp with (nolock) on cmp.ID=ct.CampaignID
		where t.TaskTypeID=189
		and (t.Status=0  and cc.state=0) 
		and ((adv.productid = cp.productid AND p.IsComposite = 0) OR (adv.productid &lt;&gt; cp.productid AND p.IsComposite = 1))
		
)

select distinct xx,cm_id,CampaignDesc,TaskDesc,TaskTypeID,RoleDesc,
       TaskCnt = COUNT(distinct taskid) ,
       ContractCnt = COUNT(DISTINCT contractid),
       AdvSum = COUNT(distinct advid)
from xx 
	
group by 	xx,cm_id,CampaignDesc,TaskDesc,TaskTypeID,RoleDesc
order by xx,RoleDesc,TaskDesc
		</CommandText>
      </Query>
      <Fields>
        <Field Name="cm_id">
          <DataField>cm_id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="xx">
          <DataField>xx</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="CampaignDesc">
          <DataField>CampaignDesc</DataField>
