    <DataSet Name="Prefectures">
      <Query>
        <DataSourceName>YPS</DataSourceName>
        <CommandText>select    distinct     Prefecture = case --bal.AreaName
                          WHEN bal.BroaderAreaID ='E2' THEN 'Αττικής'
                          WHEN bal.BroaderAreaID = 'E5' THEN 'Αττικής'
                          WHEN bal.BroaderAreaID = 'E20' THEN 'Αττικής'
                          WHEN bal.BroaderAreaID = 'E4' THEN 'Αττικής'
                          ELSE bal.AreaName
                      END from BroaderAreaLookUp bal 
where bal.BroaderAreaID like 'E%'</CommandText>
      </Query>
      <Fields>
        <Field Name="Prefecture">
          <DataField>Prefecture</DataField>
          <rd:TypeName>System.String</rd:TypeName>
