Select pe.BusinessPointID, pe.Email
INTO #emails 
FROM #ids id
JOIN dbo.PointEmail pe ON pe.BusinessPointID = id.id
 where pe.ID = (SELECT min(id)
					FROM dbo.PointEmail pein 
					WHERE pe.BusinessPointID = pein.BusinessPointID
					)

if object_id('tempdb..#phones') is not null drop table #phones 
Select pe.BusinessPointID, pe.Number
INTO #phones 
FROM #ids id
JOIN dbo.PointPhone pe ON pe.BusinessPointID = id.id
 WHERE
pe.Usage = 1
and
  pe.ID = (SELECT min(id)
					FROM dbo.PointPhone pein 
					WHERE pe.BusinessPointID = pein.BusinessPointID
					AND pein.Usage = 1
					)

 
 if object_id('tempdb..#basicPoint') is not null drop table #basicPoint
select distinct 
bp.id ID
,[Live σε XO]=  case when tixo.CustomerCode is null then 'False' else 'True' end
,[Προέλευση] = i.CustomerSource
,[Ημερομηνία Δημιουργίας] = cast(bp.CreatedOn as date)
,[Κατάσταση Επικαιροποίησης] = cu.[Description]
,[Ημερομηνία Επικαιροποίησης] = bp.CustomerUpdateModifiedOn
,[Κωδικός ΕΣ]= cast(bp.code AS BIGINT)
,[Διακριτικός] = bp.name 
,[Επωνυμία] = bp.BrandName
,[Επάγγελμα] = bp.profession
,[Φερεγγυότητα] = i.CustomerSolvency
--,[CusIndicator]= indi.CustomerIndicator

---locationaddress
,[Διεύθυνση] = Ad.[Line1]        
--,[Περιοχή]=	Ad.[Line3] 
,[Πόλη]=bal.AreaName
,[Νομός] = ad.GeoModelDistrict

--,[Πηγή] = i.CustomerSource
--,[Επικαιροποίηση] = i.CustomerUpdate
,[Τηλέφωνα] = (SELECT TOP 1 ph.Number FROM yps.[dbo].[PointPhone] ph WHERE ph.BusinessPointID=bp.ID AND ph.isactive=1 AND ph.Usage!=5 ORDER BY ph.ID DESC)        -----CSV
,[Url] = case when  bp.Url is null then 'False' else 'True' end
,[Email] = case when (SELECT TOP 1 pe.Email FROM yps.[dbo].[PointEmail] pe WHERE pe.BusinessPointID=bp.ID ORDER BY pe.ID DESC) is null then 'False' else 'True' end
,[ΑΦΜ] = case when  tc.TaxCode is null then 'False' else 'True' end
,[Κύρια Δραστηριότητα]= act.MainActivity
--,[Δευτερεύουσα(ες) Δραστηριότητες] = ''
,[Ενεργά Συμβόλαια] = avct.ctid  -----CSV
-----billing address
--,[Διεύθυνση Τιμ.] = adbil.[Line1]
--,[Περιοχή Τιμ.]= adbil.[Line3] 
--,[Δήμος Τιμ.]=balbil.AreaName
--,[Νομός Τιμ.] = adbil.GeoModelDistrict

--,[Ημερ.Λήξης ΓΓΠΣ] = g.stopDate 
--,[ActiveContracts] = avct.ctid
--,[Ημερ. Λήξης Προβολής]= avct.MaxEndDate
--,[LastCreatedContract] = MaxSignDate
--,[LastComment] = comm.CommentText
--,[LastCommentDate] = comm.CommentDate
--,[email1] =em.Email
--,[Phone1] = ph.Number

into #basicPoint

from #ids i
JOIN #CustIndic indi ON indi.BusinessPointID = i.id
JOIN #CustMainActivity act ON act.BusinessPointID = i.id
LEFT join BusinessPoint bp ON i.id = bp.id
left join #avct avct on avct.ActualBPOwnerID = bp.id
LEFT JOIN CustomerUpdate cu on cu.id = bp.CustomerUpdateID
left join TaxCode tc ON tc.id =bp.id
left join Itapps.xoupdate.XoListing tixo on tixo.CustomerCode = bp.Code
LEFT JOIN itapps.dbo.[gsis_Data] g ON g.afm = tc.taxcode
--LEFT join #comments comm ON comm.BusinessPointID = bp.ID
LEFT JOIN #emails em ON em.BusinessPointID = i.id
LEFT JOIN #phones ph ON ph.BusinessPointID = i.id

---location address
JOIN dbo.Address ad ON ad.ID = i.LocationAddId
JOIN dbo.BroaderArea bap ON bap.AddressID = ad.ID JOIN #pref p ON bap.BroaderAreaID = p.items
JOIN dbo.BroaderArea bam ON bam.AddressID = ad.ID JOIN #mun m ON  bam.BroaderAreaID = m.items
JOIN dbo.BroaderAreaLookUp bal ON bal.BroaderAreaID = bam.BroaderAreaID


---billing  
--LEFT JOIN dbo.Address adbil ON adbil.ID = i.biladrid
--LEFT JOIN dbo.BroaderArea bambil ON bambil.AddressID = adbil.ID AND bambil.BroaderAreaID LIKE 'HB%'
--LEFT JOIN dbo.BroaderAreaLookUp balbil ON balbil.BroaderAreaID = bambil.BroaderAreaID

where ( tixo.CustomerCode is null AND @xolisting = 0) OR 
	  ( tixo.CustomerCode is not null AND @xolisting = 1) OR
	  ( @xolisting = 2)	



	
if object_id('tempdb..#SecondaryActivities') is not null drop table #secondaryActivities
 select cl.BusinessPointID,cn.Name
	,RankID = RANK() over ( PARTITION By cl.BusinessPointID order by cn.Name, cl.ID )
	into #secondaryActivities
	from #basicPoint bp
	join dbo.Classification cl ON bp.ID=cl.BusinessPointID and cl.ClassifierID=4
	join dbo.ClassifierNode cn ON cn.ID=cl.ClassifierNodeID	
	
	
if object_id('tempdb..#result') is not null drop table #result
 select 
 ID
,[Live σε XO]
,[Προέλευση]
,[Ημερομηνία Δημιουργίας]
,[Κατάσταση Επικαιροποίησης]
,[Ημερομηνία Επικαιροποίησης]
,[Κωδικός ΕΣ]
,[Διακριτικός]
,[Επωνυμία]
,[Επάγγελμα]
,[Φερεγγυότητα]
,[Διεύθυνση] 
,[Πόλη]
,[Νομός]
,[Τηλέφωνα] = substring(
						( Select ','+ST1.Τηλέφωνα  AS [text()]
							From #basicPoint ST1
							Where ST1.ID = bp.id
							For XML PATH ('')
						 ), 2, 1000
					   )
,[Url]
,[Email]
,[ΑΦΜ]
,[Κύρια Δραστηριότητα]
,[Δευτερεύουσα(ες) Δραστηριότητες] = 
									substring(
									          ( Select ','+ST1.Name  AS [text()]
									            From #secondaryActivities ST1
									            Where ST1.BusinessPointID = bp.id
									            ORDER BY ST1.RankID
									            For XML PATH ('')
									           ), 2, 1000
											 )
,[Ενεργά Συμβόλαια]
--,SecActivity2= a2.Name,SecActivity3= a3.Name,SecActivity4= a4.Name,SecActivity5= a5.Name
--,SecActivity6= a6.Name,SecActivity7= a7.Name,SecActivity8= a8.Name,SecActivity9= a9.Name,SecActivity10= a10.Name

 into #result
 from #basicPoint bp
left join #secondaryActivities sa on bp.ID = sa.BusinessPointID --and a1.RankID =1 
--left join #secondaryActivities a2 on bp.ID = a2.BusinessPointID and a2.RankID =2 
--left join #secondaryActivities a3 on bp.ID = a3.BusinessPointID and a3.RankID =3
--left join #secondaryActivities a4 on bp.ID = a4.BusinessPointID and a4.RankID =4
--left join #secondaryActivities a5 on bp.ID = a5.BusinessPointID and a5.RankID =5
--left join #secondaryActivities a6 on bp.ID = a6.BusinessPointID and a6.RankID =6
--left join #secondaryActivities a7 on bp.ID = a7.BusinessPointID and a7.RankID =7
--left join #secondaryActivities a8 on bp.ID = a8.BusinessPointID and a8.RankID =8
--left join #secondaryActivities a9 on bp.ID = a9.BusinessPointID and a9.RankID =9
--left join #secondaryActivities a10 on bp.ID = a10.BusinessPointID and a10.RankID =10
where (
	   @activity = 1 and sa.BusinessPointID is null 
	   --and a2.BusinessPointID is null and a3.BusinessPointID is null 
       --and a4.BusinessPointID is null and a5.BusinessPointID is null and a6.BusinessPointID is null and a7.BusinessPointID is null 
	   --and a8.BusinessPointID is null and a9.BusinessPointID is null and a10.BusinessPointID is null
	  )
      OR
	  (
	   @activity = 0 and sa.BusinessPointID is not null 
	   --and a2.BusinessPointID is not null and a3.BusinessPointID is not null 
       --and a4.BusinessPointID is not null and a5.BusinessPointID is not null and a6.BusinessPointID is not null and a7.BusinessPointID is not null 
	   --and a8.BusinessPointID is not null and a9.BusinessPointID is not null and a10.BusinessPointID is not null
	  )


SELECT 
[Αριθμός Εγγραφών] = count(distinct [Κωδικός ΕΣ])
FROM #result 			 
WHERE id IS NOT null	  
--ORDER BY 1,2			 
						 
END 

ELSE 

