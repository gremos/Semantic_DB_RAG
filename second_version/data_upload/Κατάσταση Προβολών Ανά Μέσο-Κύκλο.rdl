-------------------------NEW Version---------------


	SELECT 
	       [Product] = '['+P.Code+'] '+ P.Name,
		   [Template] = case when temp.id is not null and (temp.Template='' or temp.Template is null) then 0 
						     when temp.id is null and ( utemp.id is null or utemp.Template='' or utemp.Template is null) then 0  
							 else 1 
					    end,
		   [Κατάσταση Προβολών] = case when ad.qa = 0 then 'Μη Ολοκληρωμένες' else 'Ολοκληρωμένες' end ,
		   [ADVID] = ad.id
		  
	FROM Advertisement ad
	  JOIN dbo.Product p ON ad.ProductID = p.ID AND p.IsComposite = 0
	  JOIN dbo.Udac ud ON ud.ID=p.UdacID
	  JOIN ContractProduct cp ON cp.id = ad.ContractProductID
	  JOIN [Contract] ct ON ct.id = cp.RelatedContractID 
	  JOIN VersionSection vs ON vs.id = ad.VersionSectionID
	  JOIN MediumVersion mv ON mv.id = vs.MediumVersionID
	  JOIN Medium m ON m.id = mv.MediumID
	  LEFT JOIN ProductExportTemplate temp ON p.id = temp.ID
      LEFT JOIN UdacExportTemplate utemp ON ud.id = utemp.ID
	WHERE mv.id = @MediumCode
	  AND ct.State not in (3,5) and ct.Type not in (1,10)
	  AND cp.State &lt;&gt; 3

