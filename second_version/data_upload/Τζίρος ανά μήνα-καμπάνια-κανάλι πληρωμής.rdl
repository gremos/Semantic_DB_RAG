--SET @dateTo = '20130601'
--SET @w_sign = 1 ;

SELECT	ISNULL(camp, '(κενή)') AS Καμπάνια ,
		creMonth AS μήνας ,
		PaymentChannel ,/* COUNT( DISTINCT ctid) AS πλήθος,*/
		SUM(CPamount) AS Τζίρος
FROM	( SELECT	[ctid] = ct.id ,
					[cpid] = cp.id ,
					[camp] = ( '[' + c.Code + '] ' + c.Name ) ,
					[creMonth] = CASE WHEN ct.OrigionalContractID IS NULL
										   AND @w_sign = 1
									  THEN CAST(MONTH(ct.CreatedOn) AS NVARCHAR(5))
										   + '/'
										   + CAST(YEAR(ct.SignedDate) AS NVARCHAR(5))
									  WHEN ct.OrigionalContractID IS NULL
										   AND @w_sign = 0
									  THEN CAST(MONTH(ct.CreatedOn) AS NVARCHAR(5))
										   + '/'
										   + CAST(YEAR(ct.CreatedOn) AS NVARCHAR(5))
									  WHEN ct.OrigionalContractID IS NOT NULL
									  THEN CAST(MONTH(ct.CreatedOn) AS NVARCHAR(5))
										   + '/'
										   + CAST(YEAR(ct.CreatedOn) AS NVARCHAR(5))
								 END ,
					[PaymentChannel] = CASE pmt.PaymentWay
							--WHEN 1 THEN 'ΠΙΣΤΩΤΙΚΗ ΚΑΡΤΑ'
										 WHEN 2 THEN 'ΟΤΕ'
							--WHEN 3 THEN 'ΜΕΤΡΗΤΑ'
							--WHEN 4 THEN 'ΕΠΙΤΑΓΗ'
							--WHEN 5 THEN 'ΤΑΧΥΠΛΗΡΩΜΗ'
							--WHEN 6 THEN 'ΑΝΤΑΛΛΑΚΤΙΚΗ ΠΩΛΗΣΗ'
							--WHEN 7 THEN 'ONLINE ΠΙΣΤΩΤΙΚΗ'
							--WHEN 8 THEN 'OFFLINE ΠΙΣΤΩΤΙΚΗ'
							--WHEN 9 THEN 'ΚΑΤΑΘΕΣΗ ΣΕ ΤΡΑΠΕΖΑ'
							--WHEN 10 THEN 'ΕΠΙ ΠΙΣΤΩΣΕΙ'
										 ELSE 'Non OTE'
									   END ,
					[CPamount] = CAST(CASE WHEN cp.[STATE] in (3,4)
												AND cp.AlteredAmount IS NOT NULL
										   THEN cp.AlteredAmount
										   ELSE ( cp.Price - cp.Discount
												  - cp.RulesDiscount )
									  END AS MONEY)  
	--,[date] =CASE	WHEN ct.OrigionalContractID IS NULL AND @w_sign = 1 THEN ct.SignedDate 
	--					when ct.OrigionalContractID IS NULL AND @w_sign = 0 THEN ct.CreatedOn 
	--					when ct.OrigionalContractID IS NOT NULL  THEN ct.CreatedOn
	--			END
	--select *
		  FROM		dbo.Contract ct
					LEFT JOIN dbo.Campaign c ON ct.CampaignID = c.id
					JOIN dbo.ContractProduct cp ON cp.RelatedContractID = ct.id
					LEFT JOIN dbo.PaymentMethod pm ON cp.PaymentMethodID = pm.id
					LEFT JOIN dbo.InvoicePaymentMethod ipm ON ipm.id = ct.id
					LEFT JOIN dbo.PaymentMethodType pmt ON ISNULL(pm.PaymentMethodTypeID,
															  ipm.PaymentMethodTypeID) = pmt.id
		  WHERE		( ( @w_sign = 1
						AND ct.SignedDate BETWEEN @dateFrom
										  AND	  DATEADD(day, 1, @dateTo)
						AND ct.OrigionalContractID IS NULL
					  )
					  OR ( @w_sign = 0
						   AND ct.CreatedOn BETWEEN @dateFrom
											AND		DATEADD(day, 1, @dateTo)
						   AND ct.OrigionalContractID IS NULL
						 )
					  OR ( ct.CreatedOn BETWEEN @dateFrom
										AND		DATEADD(day, 1, @dateTo)
						   AND ct.OrigionalContractID IS NOT NULL
						 )
					)
					AND ct.type IN ( 0, 2, 3, 4 )
					AND ct.state NOT IN ( 3, 5, 10 )
					AND cp.State &lt;&gt; 3
		) aa
GROUP BY camp ,
		creMonth ,
		PaymentChannel
ORDER BY 1 ,
		RIGHT(creMonth, 4) ,
		RIGHT('0' + creMonth, 7)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Καμπάνια">
          <DataField>Καμπάνια</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="μήνας">
          <DataField>μήνας</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="PaymentChannel">
          <DataField>PaymentChannel</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Τζίρος">
          <DataField>Τζίρος</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
  <ReportParameters>
    <ReportParameter Name="dateFrom">
      <DataType>DateTime</DataType>
      <DefaultValue>
        <Values>
          <Value>=DateAdd("d",(-1 * Day(Today)) + 1, Today).AddMonths(-1)</Value>
        </Values>
      </DefaultValue>
      <Prompt>Ημερομηνία από :</Prompt>
    </ReportParameter>
    <ReportParameter Name="dateTo">
      <DataType>DateTime</DataType>
      <DefaultValue>
        <Values>
          <Value>= DateAdd("d", -1, DateAdd("d",(-1 * Day(Today) + 1), Today))</Value>
        </Values>
      </DefaultValue>
      <Prompt>Ημερομηνία εώς :</Prompt>
    </ReportParameter>
    <ReportParameter Name="w_sign">
      <DataType>Boolean</DataType>
      <Prompt>Με ημερομηνία:</Prompt>
      <ValidValues>
        <ParameterValues>
          <ParameterValue>
            <Value>true</Value>
            <Label>Υπογραφής</Label>
          </ParameterValue>
          <ParameterValue>
            <Value>false</Value>
            <Label>Δημιουργίας</Label>
          </ParameterValue>
        </ParameterValues>
      </ValidValues>
    </ReportParameter>
  </ReportParameters>
