  <DataSets>
    <DataSet Name="ReportDS">
      <Query>
        <DataSourceName>YPS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@CampaignId">
            <Value>=Parameters!CampaignId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>;WITH 
SourceTable AS
(                    
SELECT CampaignCode = cm.Code 
		, CampaignName = cm.Name
		, ActionDate = case when BPIdent.BPIdentId is null and tgiv.BPIdentityConfirmed = 1 then 'ΠΡΟ ΑΝΑΒΑΘΜΙΣΗΣ' 
					else cast(Month(BPIdent.ActionDate) as varchar(2)) + '/' + cast(Year(BPIdent.ActionDate) as varchar(4)) end
		, LossPrintedNew = CASE LossPrintedNew WHEN 0 THEN 'ΝΕΟΙ'
				WHEN 1 THEN 'ΠΡΟΤΥΠ/ΝΟΙ'
				WHEN 2 THEN 'ΑΠΩΛΕΙΑ'
				END	
		, ContractIndicator = CASE WHEN ContractID IS NULL THEN 'ΧΩΡΙΣ ΣΥΜΒΟΛΑΙΟ'
				ELSE 'ΜΕ ΣΥΜΒΟΛΑΙΟ' END
		, IdentityConfirmed = CASE WHEN BPIdent.BPIdentId IS NULL AND tgiv.BPIdentityConfirmed = 0 THEN 'ΧΩΡΙΣ ΕΠΙΚΑΙΡΟΠΟΙΗΣΗ'
									WHEN BPIdent.BPIdentId IS NULL AND tgiv.BPIdentityConfirmed = 1 THEN 'ΜΕ ΕΠΙΚΑΙΡΟΠΟΙΗΣΗ'
									ELSE 'ΜΕ ΕΠΙΚΑΙΡΟΠΟΙΗΣΗ' END
		, DateSorting = case when BPIdent.BPIdentId is null and tgiv.BPIdentityConfirmed = 1 then 0 
					else 1 end
					
FROM dbo.Campaign cm
JOIN dbo.TargetGroupItem tgi ON cm.ID = tgi.CampaignID
JOIN dbo.TargetGroupItemValue tgiv ON tgi.ID = tgiv.TargetGroupItemID AND cm.SelectedAllocationPlanID = tgiv.CampaignAllocationPlanID
LEFT JOIN dbo.Contract ct ON tgiv.ContractID = ct.ID AND ct.State in (1,6) and ct.type in (0,2,4)
LEFT JOIN (SELECT BPIdentId = bpch.ID,
					ActionDate = bpch.TIMESTAMP,
					TgivId = cs.ContextEntityID
			FROM dbo.Task t  
					JOIN dbo.[Case] cs ON t.CaseID = cs.ID
					JOIN BusinessPointConfirmationHistory bpch ON t.ID = bpch.TaskID
			WHERE cs.ContextEntityType = 'tgiv'
			) BPIdent ON BPIdent.TgivId = tgiv.ID
			                     
WHERE cm.ID IN (@CampaignId)                 
        --AND   ContractID IS NOT NULL        
        --AND bpch.ID IS NULL
        --AND tgiv.BPIdentityConfirmed = 1      
)

SELECT CampaignCode,
		CampaignName,
		ActionDate,
		IdentityConfirmed,
		DateSorting,
		LossPrintedNew,
		ContractIndicator,
		IdentityConfCounts = COUNT(*)
FROM SourceTable
GROUP BY CampaignCode,
		CampaignName,
		ActionDate,
		LossPrintedNew,
		ContractIndicator,
		IdentityConfirmed,
		DateSorting 
ORDER BY 1, 2, 5, 6, 7</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="CampaignCode">
          <DataField>CampaignCode</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CampaignName">
          <DataField>CampaignName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="IdentityConfirmed">
          <DataField>IdentityConfirmed</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ActionDate">
          <DataField>ActionDate</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="LossPrintedNew">
          <DataField>LossPrintedNew</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DateSorting">
          <DataField>DateSorting</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="ContractIndicator">
          <DataField>ContractIndicator</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="IdentityConfCounts">
          <DataField>IdentityConfCounts</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
