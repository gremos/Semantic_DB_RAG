        </Field>
        <Field Name="Άληκτο">
          <DataField>Άληκτο</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Μέσος_Χρόνος_Εξόφλησης_Ημ_Λήξης">
          <DataField>Μέσος Χρόνος Εξόφλησης/Ημ.Λήξης</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Μέσος_Χρόνος_Εξόφλησης_Ημ_Έκδοσης">
