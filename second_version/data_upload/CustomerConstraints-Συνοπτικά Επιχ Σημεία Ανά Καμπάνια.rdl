  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@CampaignID">
            <Value>=Parameters!CampaignID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>--συγκεντρωτικό

select   count(distinct bp.id) as cnt--'Πελάτες με Customer Constraints'
	  ,a.CampaignId
	  ,'['+cm.code+'] '+cm.name as 'Καμπάνια'
	  , case when cast(cm.ClosedOn as date) &gt; cast(getdate() as date) then 'Ενεργή' else 'Ανενεργή' end as CStatus
  from
  BusinessPoint bp --on tgi.BusinessPointID = bp.id
  join dbo.Classification cl on cl.BusinessPointID = bp.ID
  JOIN dbo.ClassifierNode cln ON cl.ClassifierNodeID = cln.ID
  JOIN dbo.Classifier clr ON cl.ClassifierID = clr.ID
  join (select max(tgi.CampaignID) as campaignId,bp.id as bpId
		from BusinessPoint bp
		join TargetGroupItem tgi on bp.id = tgi.BusinessPointID
		where bp.IsActive = 1
		group by bp.id 
	    )a on a.bpId = bp.id
  join Campaign cm on cm.id = a.campaignId 
 where clr.id = 6
   --and cast(cm.ClosedOn as date) &gt; cast(getdate() as date) 
      and cm.id in (@CampaignID)
   group by a.campaignId,
	  '['+cm.code+'] '+cm.name,
	  case when cast(cm.ClosedOn as date) &gt; cast(getdate() as date) then 'Ενεργή' else 'Ανενεργή' end
 order by 4 desc
 </CommandText>
      </Query>
      <Fields>
        <Field Name="cnt">
          <DataField>cnt</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="CampaignId">
          <DataField>CampaignId</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Καμπάνια">
          <DataField>Καμπάνια</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CStatus">
          <DataField>CStatus</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DSCampaignParam">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>--συγκεντρωτικό

select   distinct a.CampaignId
	  ,'['+cm.code+'] '+cm.name as 'Καμπάνια'

  from
  BusinessPoint bp --on tgi.BusinessPointID = bp.id
  join dbo.Classification cl on cl.BusinessPointID = bp.ID
  JOIN dbo.ClassifierNode cln ON cl.ClassifierNodeID = cln.ID
  JOIN dbo.Classifier clr ON cl.ClassifierID = clr.ID
  join (select max(tgi.CampaignID) as campaignId,bp.id as bpId
		from BusinessPoint bp
		join TargetGroupItem tgi on bp.id = tgi.BusinessPointID
		where bp.IsActive = 1
		group by bp.id 
	    )a on a.bpId = bp.id
  join Campaign cm on cm.id = a.campaignId 
 where clr.id = 6
order by 1 desc

 </CommandText>
      </Query>
      <Fields>
        <Field Name="CampaignId">
          <DataField>CampaignId</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Καμπάνια">
          <DataField>Καμπάνια</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
