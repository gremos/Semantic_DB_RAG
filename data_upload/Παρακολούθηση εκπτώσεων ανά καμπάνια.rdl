  <DataSets>
    <DataSet Name="DiscountDS">
      <Query>
        <DataSourceName>YPS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@fromdate">
            <Value>=Parameters!fromdate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@todate">
            <Value>=Parameters!todate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select campaign='[' +cm.Code +  '] ' + cm.Name, 
contractid = ct.id,
contractsigned=ct.signeddate,
Saleman='['+u.SalesCode+'] ' + isnull(u.LastName,'') + ' ' + isnull(u.FirstName,''),
cttotalamount = sum(cp.price),
ctamountafterdiscount = sum(case when cp.State = 4 then cp.AlteredAmount  else cp.price-cp.discount -cp.rulesdiscount end),
ctSDdiscount=sum(cp.Discount),
ctbundlediscount = sum(cp.BundleDiscount),
ctproductdiscount = sum(cp.ProductDiscount),
ctpaymentdiscount = sum(cp.PaymentDiscount),
ctrulesdiscount = sum(cp.rulesdiscount)
from dbo.Contract ct
left join dbo.[User] u on u.id = ct.salesmanid
left join dbo.Campaign cm on cm.id = ct.CampaignID
join ContractProduct cp on cp.RelatedContractID = ct.ID and cp.State &lt;&gt; 3
where ct.state in (1,6)
and ct.type  not in (1,10)
and ct.SignedDate between cast(@fromdate as date) and cast(@todate as date)
group by '[' +cm.Code +  '] ' + cm.Name,  ct.id, ct.signeddate, '['+u.SalesCode+'] ' + isnull(u.LastName,'')+ ' '  + isnull(u.FirstName,'')</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
