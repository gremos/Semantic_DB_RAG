    <DataSet Name="DataSet2">
      <Query>
        <DataSourceName>YPS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@datefrom">
            <Value>=Parameters!datefrom.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@dateto">
            <Value>=Parameters!dateto.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>--declare @datefrom datetime
--set @datefrom='20150420'
--declare @dateto datetime
--set @dateto='20150423'




SELECT DISTINCT 'a' as type, ContractId = tgiv2.AltOfferID , cast(t.CreatedOn  as date) AS taskCreatedOn ,
       value=(select cast(sum(CASE WHEN cp.[state]  IN (3,4) THEN cp.AlteredAmount ELSE  cp.[Price] - cp.[Discount] - cp.[RulesDiscount]	END ) as float)
                from dbo.ContractProduct cp where cp.RelatedContractID=ct.ID)   
FROM	dbo.Task t
		JOIN dbo.[Case] c ON t.CaseID = c.ID
		JOIN dbo.TargetGroupItemValue tgiv2 ON C.ContextEntityID = TGIV2.ID AND C.ContextEntityType = 'TGIV'
		JOIN dbo.contract ct ON ct.ID = tgiv2.AltOfferID
WHERE	t.TaskTypeID=189 
		AND t.CreatedOn &gt;= @datefrom
		AND t.CreatedOn &lt; @dateto+1
		
union all 


SELECT DISTINCT 'c' as type, ContractId = tgiv2.AltOfferID , cast( t.CreatedOn as DATE) AS taskCreatedOn ,
       value=(select cast(sum(CASE WHEN cp.[state]  IN (3,4) THEN isnull(cp.AlteredAmount,0) ELSE  cp.[Price] - cp.[Discount] - cp.[RulesDiscount]	END )as float)
                from dbo.ContractProduct cp where cp.RelatedContractID=ct.ID) 
FROM	dbo.Task t
		JOIN dbo.[Case] c ON t.CaseID = c.ID
		JOIN dbo.TargetGroupItemValue tgiv2 ON C.ContextEntityID = TGIV2.ID AND C.ContextEntityType = 'TGIV'
		JOIN dbo.contract ct ON ct.ID = tgiv2.AltOfferID
WHERE	t.TaskTypeID=190
		AND t.CreatedOn &gt;= @datefrom
		AND t.CreatedOn &lt; @dateto+1

		
union all	

select *,
        value=(select CAST( sum(CASE WHEN cp.[state] IN (3,4) THEN isnull(cp.AlteredAmount,0) ELSE  cp.[Price] - cp.[Discount] - cp.[RulesDiscount]	END ) as float)
                from dbo.ContractProduct cp where cp.RelatedContractID=x.contractid) 
from(		
select distinct 'd' as type,cp.RelatedContractID as contractid, cast(t.CreatedOn  as date) AS taskCreatedOn
from yps.dbo.[Case] c
        JOIN yps.dbo.[Case] cc ON cc.MasterCaseID = c.ID
        JOIN yps.dbo.Task t  ON t.CaseID = cc.ID
        join yps.dbo.Advertisement adv  on adv.ID=cc.ContextEntityID and cc.ContextEntityType='adv' 
       	join yps.dbo.ContractProduct cp  on cp.id= adv.ContractProductID
 WHERE	t.TaskTypeID=508
		AND t.CreatedOn &gt;= @datefrom
		AND t.CreatedOn &lt; @dateto+1
)x

union all

	
SELECT DISTINCT 'b' as type, ContractId = tgiv2.AltOfferID , cast(t.CreatedOn  as date) AS taskCreatedOn ,
       value=(select CAST( sum(CASE WHEN cp.[state] IN (3,4) THEN cp.AlteredAmount ELSE  cp.[Price] - cp.[Discount] - cp.[RulesDiscount]	END ) as float)
                from dbo.ContractProduct cp where cp.RelatedContractID=ct.ID) 
FROM	dbo.Task t
		JOIN dbo.[Case] c ON t.CaseID = c.ID
		JOIN dbo.TargetGroupItemValue tgiv2 ON C.ContextEntityID = TGIV2.ID AND C.ContextEntityType = 'TGIV'
		JOIN dbo.contract ct ON ct.ID = tgiv2.AltOfferID
WHERE	t.TaskTypeID=189 
and  (not exists (Select 1 from dbo.Advertisement adv join ContractProduct cp on cp.ID=adv.ContractProductID where cp.RelatedContractID=ct.ID and adv.QA=0)
		or 
	  exists (Select 1 from dbo.Advertisement adv join ContractProduct cp on cp.ID=adv.ContractProductID where cp.RelatedContractID=ct.ID and adv.QA=0and cp.State in (3,6,7) )
	  )
	 	AND t.CreatedOn &gt;= @datefrom
		AND t.CreatedOn &lt; @dateto+1
			
			order by 2</CommandText>
      </Query>
