    <DataSet Name="ReportDS">
      <Query>
        <DataSourceName>YPS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@allcampaigns">
            <Value>=Parameters!allcampaigns.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@dateFrom">
            <Value>=Parameters!dateFrom.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@dateTo">
            <Value>=Parameters!dateTo.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>
IF OBJECT_ID('tempdb..#campaigns') IS NOT NULL DROP TABLE #campaigns
SELECT *
into #campaigns
FROM itapps.dbo.SplitTostring(@allcampaigns, ',')

CREATE NONCLUSTERED INDEX ix_camp ON #campaigns ([value]);

IF OBJECT_ID('tempdb..#selectedcontracts') IS NOT NULL DROP TABLE #selectedcontracts
select ContractId = ct.id
into #selectedcontracts
from contract ct
join #campaigns cmp on cmp.value = isnull(ct.CampaignID,-1)
where ( ( ct.SignedDate BETWEEN @dateFrom AND DATEADD(day, 1, @dateTo)
				AND ct.OrigionalContractID IS NULL
			  )
			  OR ( ct.CreatedOn BETWEEN @dateFrom AND DATEADD(day, 1, @dateTo)
				   AND ct.OrigionalContractID IS NOT NULL
				 )
			  OR ( @dateFrom IS NULL
				   AND @dateTo IS NULL
				 )
			)

CREATE NONCLUSTERED INDEX ix_RltCt ON #selectedcontracts (ContractId);

IF OBJECT_ID('tempdb..#temp') IS NOT NULL DROP TABLE #temp
SELECT	DISTINCT
		ContractId = ct.id ,
		TaskTypeID = tt.id , 
		t.CreatedOn ,
		t.CompletedOn,
		t.Status ,
		t.outcome , 
		t.ID AS taskid
		,tgi.CampaignID
INTO	#temp
FROM	#campaigns cpn 
		join dbo.TargetGroupItem tgi WITH ( NOLOCK ) ON tgi.CampaignID = cpn.value 
		join dbo.TargetGroupItemValue tgiv WITH ( NOLOCK ) ON tgi.ID = tgiv.TargetGroupItemID
		JOIN dbo.[Case] mc WITH ( NOLOCK ) ON mc.ContextEntityID = tgiv.id AND mc.ContextEntityType = 'TGIV'
		JOIN dbo.[Case] c WITH ( NOLOCK ) ON mc.MasterCaseID = c.MasterCaseID
		JOIN dbo.Task t WITH ( NOLOCK ) ON c.ID = t.CaseID
		LEFT JOIN dbo.TaskAssignment ta WITH ( NOLOCK ) ON t.ID = ta.TaskID AND ta.AssignmentType = 0
		left join [Role] r WITH ( NOLOCK ) on r.id = t.ActualRoleID 
		JOIN dbo.TaskType tt WITH ( NOLOCK ) ON t.TaskTypeID = tt.ID 
		JOIN dbo.Contract ct WITH ( NOLOCK ) ON ct.id = COALESCE(tgiv.ContractID, tgiv.AltOfferID, tgiv.FixOfferID)
		
WHERE    (
			(ActivityType = 'H' AND t.status = 0)
			  OR (	tt.id IN (648) AND (t.Outcome = 'CONTINUE')
					OR ( tt.id = 934 AND t.Outcome is null )
					OR ( TaskTypeID = 189 )
					OR ( tt.id = 500 AND t.Outcome = '')
					OR ( tt.id = 956 AND t.Outcome is null ) --Added for DEV-14497 (used for DocuSign)
				 ) 
		 )

		AND tt.id &lt;&gt; 175 --sumplhrwsh kwdikou fusikou fakelou
		AND tt.id &lt;&gt; 170 --sumplhrwsh kwdikou fusikou fakelou
		AND tt.id &lt;&gt; 266 --eksetash metaforas stoxou
		AND ct.id IS NOT NULL
ORDER BY ct.id ,t.id

--select * from #temp where ContractId in  (2014155,2015691,2015194,2015832)

CREATE NONCLUSTERED INDEX ix_temp ON #temp ([taskid]);
CREATE NONCLUSTERED INDEX ix_temp2 ON #temp ([TaskTypeID]);


IF OBJECT_ID('tempdb..#MaxTask') IS NOT NULL DROP TABLE #MaxTask
SELECT	max(t.id) Taskid,
		tmp.ContractId
INTO #MaxTask
from #temp tmp WITH ( NOLOCK )
join [task] t WITH ( NOLOCK ) on t.id = tmp.taskid
join [Contract] ct WITH ( NOLOCK ) on ct.id = tmp.ContractId

group by tmp.ContractId


CREATE NONCLUSTERED INDEX ix_maxtsk ON #MaxTask ([taskid]);
CREATE NONCLUSTERED INDEX ix_maxtsk2 ON #MaxTask ([ContractId]);

--select * from #MaxTask where ContractId = 2013957


IF OBJECT_ID('tempdb..#CtPrice') IS NOT NULL DROP TABLE #CtPrice
 SELECT	ContractID =cpp.RelatedContractID, 
		ContractPrice=	SUM(CASE WHEN cpp.[STATE] IN ( 3, 4 ) AND advv.AlteredAmount IS NOT NULL THEN advv.AlteredAmount
					ELSE advv.price - advv.discount - advv.rulesdiscount
					END) 
INTO #CtPrice
FROM #selectedContracts ct 
join dbo.ContractProduct cpp WITH ( NOLOCK ) on ct.ContractId = cpp.RelatedContractID
JOIN dbo.Advertisement advv WITH ( NOLOCK ) ON cpp.ID = advv.ContractProductID
	 
WHERE  advv.QA = 0
group by cpp.RelatedContractID


CREATE NONCLUSTERED INDEX ix_CtPrice ON #CtPrice ([ContractId]);


IF OBJECT_ID('tempdb..#RltCt') IS NOT NULL DROP TABLE #RltCt
SELECT	RelatedContractId = cpp.RelatedContractID ,
		ContractPrice = SUM(CASE WHEN cpp.[STATE] IN ( 3, 4 ) AND cpp.AlteredAmount IS NOT NULL THEN cpp.AlteredAmount
								ELSE cpp.price - cpp.discount - cpp.rulesdiscount
								END) ,
				PPC_xo_amount = SUM (case when cpp.state in (3,4) then ExtraFeeAmountAfterCC + isnull( cpp.OneTimeAmountAfterDiscount, cpp.OneTimeAmount)
						else isnull(ExtraFeeAmountAfterDiscount, ExtraFeeAmount) + isnull( cpp.OneTimeAmountAfterDiscount, cpp.OneTimeAmount)
						end )

INTO #RltCt
FROM #selectedcontracts ct
join dbo.ContractProduct cpp WITH ( NOLOCK ) on ct.ContractId = cpp.RelatedContractID
WHERE cpp.State &lt;&gt; 3 
GROUP BY	cpp.RelatedContractID

CREATE NONCLUSTERED INDEX ix_RltCt ON #RltCt ([RelatedContractId]);

 
IF OBJECT_ID('tempdb..#advp') IS NOT NULL DROP TABLE #advp
SELECT DISTINCT
   cp.RelatedContractID ,
	pm.id pmid ,
	pm.AdvancedPayment
INTO #advp
FROM #selectedcontracts ct
join dbo.ContractProduct cp WITH ( NOLOCK ) on ct.ContractId = cp.RelatedContractID
JOIN dbo.PaymentMethod pm WITH ( NOLOCK ) ON cp.PaymentMethodID = pm.ID
WHERE	cp.State &lt;&gt; 3

CREATE NONCLUSTERED INDEX ix_advp ON #advp ([RelatedContractId]);

  
    
IF OBJECT_ID('tempdb..#MinTaskProduction') IS NOT NULL DROP TABLE #MinTaskProduction
SELECT	ContractId ,
		MIN(createdon) createdon
INTO	#MinTaskProduction
FROM	#temp WITH ( NOLOCK )
WHERE	TaskTypeID = 189
GROUP BY ContractId



CREATE NONCLUSTERED INDEX ix_mintsk ON #MinTaskProduction([ContractId]);


IF OBJECT_ID('tempdb..#MaxTaskProduction') IS NOT NULL DROP TABLE #MaxTaskProduction
SELECT	ContractId ,
		MAX(CompletedOn) CompletedOn
INTO	#MaxTaskProduction
FROM	#temp WITH ( NOLOCK )
WHERE	((TaskTypeID =648 AND  Outcome = 'CONTINUE') 
     OR (TaskTypeID = 504 AND  Outcome = 'CONTINUE') ) and status = 1
GROUP BY ContractId

CREATE NONCLUSTERED INDEX ix_maxtskp ON #MaxTaskProduction([ContractId]);


IF OBJECT_ID('tempdb..#MinTaskBackOffice') IS NOT NULL DROP TABLE #MinTaskBackOffice
SELECT	ContractId ,
		MIN(createdon) createdon
INTO	#MinTaskBackOffice
FROM	#temp WITH ( NOLOCK )
WHERE	TaskTypeID IN (894,870,876) 
GROUP BY ContractId

CREATE NONCLUSTERED INDEX ix_mintskbo ON #MinTaskBackOffice([ContractId]);

IF OBJECT_ID('tempdb..#MinAdvDate') IS NOT NULL DROP TABLE #MinAdvDate
SELECT	ContractId ,
		MIN(a.AdvBeginDate) AdvBeginDate
INTO	#MinAdvDate
FROM	#selectedcontracts ct  WITH ( NOLOCK )
join    ContractProduct cp WITH ( NOLOCK ) on ct.ContractId = cp.RelatedContractID
join    Advertisement a WITH ( NOLOCK ) on cp.ID = a.ContractProductID
GROUP BY ContractId

CREATE NONCLUSTERED INDEX ix_mintskadv ON #MinAdvDate([ContractId]);

IF OBJECT_ID('tempdb..#WebSiteAllProducts') IS NOT NULL DROP TABLE #WebSiteAllProducts

SELECT DISTINCT
		contractid ,
		pr.code AS prCode
INTO	#WebSiteAllProducts
FROM	#selectedcontracts ct 
		JOIN ContractProduct cp WITH ( NOLOCK ) ON cp.RelatedContractID = ct.contractid
		JOIN Advertisement adv WITH ( NOLOCK ) ON adv.ContractProductID = cp.ID
		JOIN Product pr WITH ( NOLOCK ) ON pr.ID = adv.ProductID
		JOIN VersionSection vs WITH ( NOLOCK ) ON vs.ID = adv.VersionSectionID
WHERE	vs.MediumVersionID = 34
		
CREATE NONCLUSTERED INDEX ix_webs ON #WebSiteAllProducts([ContractId]);


IF OBJECT_ID('tempdb..#WebSiteConcatProducts') IS NOT NULL DROP TABLE #WebSiteConcatProducts
SELECT DISTINCT
		contractid ,
		SUBSTRING(( SELECT	',' + prCode AS [text()]
					FROM	#WebSiteAllProducts ins
					WHERE	ins.contractid = ext.contractid
					ORDER BY prCode
				  FOR
					XML	PATH('')
				  ), 2, 100) AS produdts
INTO	#WebSiteConcatProducts
FROM	#WebSiteAllProducts ext WITH ( NOLOCK )


CREATE NONCLUSTERED INDEX ix_webscn ON #WebSiteConcatProducts([ContractId]);

IF OBJECT_ID('tempdb..#oldpaymentmethod') IS NOT NULL DROP TABLE #oldpaymentmethod
SELECT ContractId,
		paymentDescription = max(pt.Description)
into #oldpaymentmethod
FROM	#selectedcontracts ct
		join Contract c on ct.ContractId = c.id
		JOIN contractproduct cp ON c.id = cp.RelatedContractID
		JOIN PaymentMethod pm ON pm.ID = cp.PaymentMethodID
		JOIN PaymentMethodType pt ON pt.ID = pm.PaymentMethodTypeID
group by ContractId


CREATE NONCLUSTERED INDEX ix_woldpm ON #oldpaymentmethod([ContractId]);


IF OBJECT_ID('tempdb..#results') IS NOT NULL DROP TABLE #results
SELECT	*
INTO	#results
FROM	( SELECT    [BP Κωδικός] = bp.code ,
					[BP Επωνυμία] = bp.BrandName ,
					[BP Διακριτικός Τίτλος] = bp.Name ,
					[Πωλητής] = LTRIM(RTRIM('[' + u.SalesCode + ']' + u.LastName + ' ' + u.FirstName)) ,
					[Κανάλι Πώλησης] = ISNULL(cht.DESCRIPTION, '') ,
					[Κωδ. Συμβολαίου] = ct.id ,
					[Συνδρομητικό Ν/Ο] = case 
											when ct.IsSubscriptionBased = 0 then 'OXI'
											else 'ΝΑΙ'
											end,
					[Ανανέωση Συνδρομητικών] = CASE	WHEN ct.OrigionalContractID IS NOT NULL
														 AND ct.OriginalContractType = 1 THEN 1
													ELSE 0
											   END ,
					[Κατάσταση Συμβολαίου] = ( CASE Ct.State
												 WHEN 0 THEN 'Pending'
												 WHEN 1 THEN 'Approved'
												 WHEN 2 THEN 'Rejected'
												 WHEN 3 THEN 'Cancelled'
												 WHEN 4 THEN 'Replaced'
												 WHEN 5 THEN 'CancelledOffer'
												 WHEN 6 THEN 'Modified'
											   END ) ,
					[Ποσό Συμβολαίου] = CAST(ctpr.ContractPrice AS MONEY) ,
					[Καθαρό Ποσό PPC] = CAST(ctpr.PPC_xo_amount AS MONEY) ,
					[Ποσό Εκκρεμών Προβολών] = CAST(ctp.ContractPrice AS MONEY) ,
					[Προκαταβολή] = CAST(SUM(advp.AdvancedPayment) AS MONEY),
					[Ρόλος] =  CASE	WHEN  ts.status = 1 AND ts.TaskTypeID = 934 AND ts.Outcome is null THEN 'ACS/Collection'
									WHEN  ts.status = 1 AND ts.TaskTypeID = 956 AND ts.Outcome is null THEN 'DocuSign'
									WHEN  ts.status = 1 THEN 'Completed'
									when ts.status is null then 'Συνδρομητικό'
													ELSE rld.Description
											   END ,
					[Ημ/νια Υπογραφής Συμβολαίου] = CONVERT(NVARCHAR(15), ct.SignedDate, 103) ,
					[Ημ/νια Δημιουργίας Συμβολαίου] = CONVERT(NVARCHAR(15), ct.CreatedOn, 103) ,
					[Πρώτη Ημ/νια Παραγωγής] = CONVERT(NVARCHAR(15), mtp.createdon, 103) ,
					[Τελευταία Ημ/νια Παραγωγής] = CONVERT(NVARCHAR(15), mtp2.CompletedOn, 103) ,
					[Διαφορά Ημερομηνιών] = DATEDIFF(dd, ct.CreatedOn, mtp.createdon) ,
					[Ημ/νία Έναρξης Τιμολόγησης σε EBS] = CONVERT(NVARCHAR(15), ct.EBSFirstInvoiceDate, 103) ,
					[Τρόπος Πληρωμής] = ctpm.paymentDescription  ,
					[Διαφορά Ημερομηνιών2] = DATEDIFF(dd, ct.SignedDate, GETDATE()),
					[Προϊόντα WebSite] = wpr.produdts ,
					[Πρώτη Ημ/νια BackOffice] = CONVERT(NVARCHAR(15), mtb.CreatedOn, 103) ,
					ISNULL(ct.CampaignID, r.CampaignID) AS CampaignID

		  FROM		#selectedcontracts selct 
					join dbo.Contract ct WITH ( NOLOCK )	on selct.ContractId = ct.ID
					LEFT JOIN #MinTaskProduction mtp WITH ( NOLOCK ) ON mtp.ContractId = ct.id
					LEFT JOIN #MaxTaskProduction mtp2 WITH ( NOLOCK ) ON mtp2.ContractId = ct.id
					LEFT JOIN #MinTaskBackOffice mtb WITH ( NOLOCK ) ON mtb.ContractId = ct.id
					LEFT JOIN #WebSiteConcatProducts wpr WITH ( NOLOCK ) ON wpr.contractid = ct.id
					LEFT JOIN #temp r WITH ( NOLOCK ) ON r.ContractId=ct.id 
					left JOIN #MaxTask mxo WITH ( NOLOCK ) ON mxo.ContractId=ct.id
					left join Task ts WITH ( NOLOCK )on  ts.id = mxo.Taskid
					left join [Role] rld WITH ( NOLOCK )on rld.id = ts.ActualRoleID 
					left JOIN #CtPrice ctp WITH ( NOLOCK ) ON ctp.ContractID = ct.id
					----------------------left join #temp tmp WITH ( NOLOCK ) ON tmp.CampaignID =  ct.CampaignID
					JOIN #RltCt ctpr WITH ( NOLOCK ) ON ctpr.RelatedContractId = ct.id --
					JOIN #advp advp WITH ( NOLOCK ) ON ct.ID = advp.RelatedContractID --
					JOIN dbo.[User] u WITH ( NOLOCK ) ON ct.SalesManID = u.ID
					LEFT JOIN dbo.ChannelType cht WITH ( NOLOCK ) ON ct.channeltypeid = cht.id--un.ChannelTypeID = cht.ID
					JOIN dbo.BusinessPoint bp WITH ( NOLOCK ) ON ct.ActualBPOwnerID = bp.ID

					join #campaigns cmp on ISNULL(ISNULL(ct.CampaignID, r.CampaignID), -1) = cmp.value
					left join #oldpaymentmethod ctpm on ctpm.ContractId = ct.ID
		  
		  WHERE		
					ct.State &lt;&gt; 3
					AND ct.type IN ( 0, 2, 3, 4 )
				
		  GROUP BY	bp.code ,
					bp.Name ,
					bp.BrandName ,
					cht.Description ,
					ctp.ContractPrice,
					ct.id ,
					ct.State ,
					rld.Description,
					ts.Status,
					ts.Outcome,
					ts.TaskTypeID,
					ctpr.ContractPrice ,
					ct.OrigionalContractID ,
					ct.OriginalContractType ,
					u.SalesCode ,
					u.LastName ,
					u.FirstName ,
					ct.CreatedOn ,
					ct.SignedDate ,
					mtp.createdon ,
					mtp2.CompletedOn ,
					mtb.CreatedOn ,
					wpr.produdts ,
					ct.CampaignID ,
					r.CampaignID ,
					ct.EBSFirstInvoiceDate ,
					ctpr.PPC_xo_amount, 
					ctpm.paymentDescription,
					ct.IsSubscriptionBased
		  UNION
		  SELECT	[BP Κωδικός] = bp.code ,
					[BP Επωνυμία] = bp.BrandName ,
					[BP Διακριτικός Τίτλος] = bp.Name ,
					[Πωλητής] = LTRIM(RTRIM('[' + u.SalesCode + ']' + u.LastName + ' ' + u.FirstName)) ,
					[Κανάλι Πώλησης] = ISNULL(cht.DESCRIPTION, '') ,
					[Κωδ. Συμβολαίου] = ct.id ,
					[Συνδρομητικό Ν/Ο] = case 
										when ct.IsSubscriptionBased = 0 then 'OXI'
										else 'ΝΑΙ'
										end,
					[Ανανέωση Συνδρομητικών] = CASE	WHEN ct.OrigionalContractID IS NOT NULL
														 AND ct.OriginalContractType = 1 THEN 1
													ELSE 0
											   END ,
					[Κατάσταση Συμβολαίου] = ( CASE    Ct.State
												 WHEN 0 THEN 'Pending'
												 WHEN 1 THEN 'Approved'
												 WHEN 2 THEN 'Rejected'
												 WHEN 3 THEN 'Cancelled'
												 WHEN 4 THEN 'Replaced'
												 WHEN 5 THEN 'CancelledOffer'
												 WHEN 6 THEN 'Modified'
											   END ) ,
					[Ποσό Συμβολαίου] = CAST(ctpr.ContractPrice AS MONEY) ,
					[Καθαρό Ποσό PPC] = CAST(ctpr.PPC_xo_amount AS MONEY) ,
					[Ποσό Εκκρεμών Προβολών] = CAST(ctp.ContractPrice AS MONEY) ,
					[Προκαταβολή] = CAST (advp.Amount AS MONEY),
					[Ρόλος] =  CASE	WHEN  ts.status = 1 AND ts.TaskTypeID = 934 AND ts.Outcome is null THEN 'ACS/Collection'
									WHEN  ts.status = 1 AND ts.TaskTypeID = 956 AND ts.Outcome is null THEN 'DocuSign' --Added for DEV-14497 (used for DocuSign)
									WHEN  ts.status = 1 THEN 'Completed'
									WHEN  ts.status is null THEN 'Συνδρομητικό'
													ELSE rld.Description
							   END ,
					[Ημ/νια Υπογραφής Συμβολαίου] = CONVERT(NVARCHAR(15), ct.SignedDate, 103) ,
					[Ημ/νια Δημιουργίας Συμβολαίου] = CONVERT(NVARCHAR(15), ct.CreatedOn, 103) ,
					[Πρώτη Ημ/νια Παραγωγής] = CONVERT(NVARCHAR(15), mtp.createdon, 103) ,
					[Τελευταία Ημ/νια Παραγωγής] = CONVERT(NVARCHAR(15), mtp2.CompletedOn, 103) ,
					[Διαφορά Ημερομηνιών] = DATEDIFF(dd, ct.CreatedOn, mtp.createdon) ,
					[Ημ/νία Έναρξης Τιμολόγησης σε EBS] = CONVERT(NVARCHAR(15), ct.EBSFirstInvoiceDate, 103) ,
					[Τρόπος Πληρωμής] =  pt.Description ,
					[Διαφορά Ημερομηνιών2] = DATEDIFF(dd, ct.SignedDate, GETDATE()),
					[Προϊόντα WebSite] = wpr.produdts ,
					[Πρώτη Ημ/νια BackOffice] = CONVERT(NVARCHAR(15), mtb.CreatedOn, 103) ,
					ISNULL(ct.CampaignID, r.CampaignID) AS CampaignID
		  FROM		#selectedcontracts selct 
					join dbo.Contract ct WITH ( NOLOCK ) on selct.ContractId = ct.ID
					LEFT JOIN #MinTaskProduction mtp WITH ( NOLOCK ) ON mtp.ContractId = ct.id
					LEFT JOIN #MaxTaskProduction mtp2 WITH ( NOLOCK ) ON mtp2.ContractId = ct.id
					LEFT JOIN #MinTaskBackOffice mtb WITH ( NOLOCK ) ON mtb.ContractId = ct.id
					LEFT JOIN #WebSiteConcatProducts wpr WITH ( NOLOCK ) ON wpr.contractid = ct.id
					LEFT JOIN #temp r ON r.ContractId=ct.id 
					left JOIN #MaxTask mxo WITH ( NOLOCK ) ON mxo.ContractId=ct.id
					left join Task ts WITH ( NOLOCK ) on ts.id = mxo.Taskid
					left join [Role] rld WITH ( NOLOCK ) on rld.id = ts.ActualRoleID 
					left JOIN #CtPrice ctp WITH ( NOLOCK ) ON ctp.ContractID = ct.id
					-----------------left join #temp tmp WITH ( NOLOCK ) ON tmp.CampaignID =   ct.CampaignID
					JOIN #RltCt ctpr WITH ( NOLOCK ) ON ctpr.RelatedContractId = ct.id---
					JOIN dbo.InvoicePaymentMethod ipm WITH ( NOLOCK )ON ct.ID = ipm.id---
					LEFT JOIN dbo.AdvancedPaymentMethod advp WITH ( NOLOCK )ON ct.id = advp.id
					JOIN dbo.[User] u ON ct.SalesManID = u.ID
					LEFT JOIN dbo.ChannelType cht WITH ( NOLOCK )ON ct.channeltypeid = cht.id--un.ChannelTypeID = cht.ID
					JOIN dbo.BusinessPoint bp WITH ( NOLOCK )ON ct.ActualBPOwnerID = bp.ID

					join #campaigns cmp on ISNULL(ISNULL(ct.CampaignID, r.CampaignID), -1) = cmp.value

					left join PaymentMethodType pt ON pt.ID = ipm.PaymentMethodTypeID

		  WHERE		ct.State NOT IN (3,10,11)
					AND ct.type IN ( 0, 2, 3, 4 )
					
		  GROUP BY	bp.code ,
					bp.Name ,
					bp.BrandName ,
					cht.Description ,
					ctp.ContractPrice,
					ct.id ,
					ct.State ,
					rld.Description,
					ts.Status,
					ts.Outcome,
					ts.TaskTypeID,
					ctpr.ContractPrice ,
					advp.Amount ,
					ct.OrigionalContractID ,
					ct.OriginalContractType ,
					u.SalesCode ,
					u.LastName ,
					u.FirstName ,
					ct.CreatedOn ,
					ct.SignedDate ,
					mtp.createdon ,
					mtp2.Completedon ,
					mtb.CreatedOn ,
					wpr.produdts ,
					ct.CampaignID ,
					r.CampaignID ,
					ct.EBSFirstInvoiceDate ,
					ctpr.PPC_xo_amount,
					pt.Description,
					ct.IsSubscriptionBased
		) aa

CREATE NONCLUSTERED INDEX ix_reslt ON #results([Κωδ. Συμβολαίου]);

SELECT	( CASE WHEN ( '[' + Code + '] ' + Name ) IS NULL THEN '(κενή)'
			   ELSE '[' + Code + '] ' + Name
		  END ) AS Καμπάνια ,
		t.* ,
		convert(nvarchar(20),ma.AdvBeginDate,103) as AdvBeginDate
--into #aaa
FROM	#results t
             join #MinAdvDate ma on t.[Κωδ. Συμβολαίου] = ma.ContractId
		LEFT JOIN dbo.Campaign a ON a.id = t.CampaignID
		--WHERE [Κωδ. Συμβολαίου] IN (2067624,2067406,2067330,2067603)
		--where t.Ρόλος = 'Συνδρομητικό'
ORDER BY 1,2

--select distinct a.[Ρόλος] from #aaa a

