  <DataSets>
    <DataSet Name="ContractsDS">
      <Query>
        <DataSourceName>YPS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@AutoCreated">
            <Value>=Parameters!AutoCreated.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>WITH reportdata AS (
SELECT  [ΚΩΔΙΚΟΣ ΕΣ] = bp.Code,
        [ΚΩΔΙΚΟΣ ΣΥΜΒΟΛΑΙΟΥ] = c.ID,
        [ΚΑΜΠΑΝΙΑ] = ISNULL('[' + cm.Code + '] ' + cm.Name,''),
        [ΠΩΛΗΤΗΣ] = RTRIM('[' + u.salescode + '] ' + u.LastName + ' ' + u.FirstName),
        [ΗΜ. ΥΠΟΓΡΑΦΗΣ] = CAST(C.SignedDate AS DATE),
        [ΚΑΤΑΣΤΑΣΗ ΣΥΜΒΟΛΑΙΟΥ] = CASE C.State
									   WHEN 0 THEN 'Pending'
									   WHEN 1 THEN 'Approved'
									   WHEN 2 THEN 'Rejected'
									   WHEN 3 THEN 'Cancelled'
									   WHEN 4 THEN 'Replaced'
									   WHEN 5 THEN 'CancelledOffer'
									   WHEN 6 THEN 'Modified'
                                 END,
        [ΜΙΝ START DATE] = CAST(AllSub.CBeginDate AS DATE),
        [MAX END DATE] = CAST(AllSub.CEndDate AS DATE),
        [ΣΥΝΔΡΟΜΗΤΙΚΗ ΑΞΙΑ ΣΥΜΒΟΛΑΙΟΥ] = AllSub.SubAdvPrice,
        [ΑΝΑΝΕΩΣΙΜΟ - ΜΙΝ START DATE] = CAST(Renewable.CBeginDate AS DATE),
        [ΑΝΑΝΕΩΣΙΜΟ - MAX END DATE] = CAST(Renewable.CEndDate AS DATE),
        [ΑΝΑΝΕΩΣΙΜΟ - ΣΥΝΔΡΟΜΗΤΙΚΗ ΑΞΙΑ ΣΥΜΒΟΛΑΙΟΥ] = Renewable.SubAdvPrice,
        --APO AUTOMATH ANANEWSH        
        [ΗΜ. ΔΗΜΙΟΥΡΓΙΑΣ] = CAST(C.CreatedOn AS DATE),
        [ΣΥΜΒΟΛΑΙΟ-ΠΗΓΗ] = CASE WHEN c.OrigionalContractID IS NULL AND c.OriginalContractType IS NULL THEN '' ELSE c.OrigionalContractID END ,
        [ΠΩΛΗΤΗΣ-ΠΗΓΗ] = CASE WHEN c.OrigionalContractID IS NULL AND c.OriginalContractType IS NULL THEN '' 
							ELSE RTRIM('[' + sourceu.salescode + '] ' + sourceu.LastName + ' ' + sourceu.FirstName) END,
        [AutoCreated] = CASE WHEN c.OrigionalContractID IS NOT NULL AND c.OriginalContractType  = 1 THEN 1 ELSE 0 END
        
                              
FROM    dbo.Contract c
		LEFT JOIN dbo.[User] U ON c.SalesManID = U.ID
		JOIN dbo.BusinessPoint bp ON c.ActualBPOwnerID = bp.ID
		JOIN ( SELECT   RelatedContractID,
                        MIN(ADV.AdvBeginDate) CBeginDate,
                        MAX(ISNULL(cp.InterruptionDate,ADV.AdvEndDate)) CEndDate,
                        SUM(CASE WHEN cp.state IN (3,4) AND adv.AlteredAmount IS NOT NULL  THEN adv.AlteredAmount else adv.Price - adv.RulesDiscount - adv.Discount end) SubAdvPrice
               FROM     dbo.Advertisement adv
                        JOIN dbo.ContractProduct cp ON adv.ContractProductID = cp.ID
               WHERE adv.IsSubscriptionBased = 1 
               GROUP BY RelatedContractID
             ) AllSub ON AllSub.RelatedContractID = c.id
        LEFT JOIN ( SELECT   RelatedContractID,
                        MIN(ADV.AdvBeginDate) CBeginDate,
                        MAX(ISNULL(cp.InterruptionDate,ADV.AdvEndDate)) CEndDate,
                        SUM(CASE WHEN cp.state IN (3,4) AND adv.AlteredAmount IS NOT NULL THEN adv.AlteredAmount else adv.Price - adv.RulesDiscount - adv.Discount end) SubAdvPrice
               FROM     dbo.Advertisement adv
                        JOIN dbo.ContractProduct cp ON adv.ContractProductID = cp.ID
               WHERE adv.IsSubscriptionBased = 1 AND cp.IsRenewable = 1
               GROUP BY RelatedContractID
             ) Renewable ON Renewable.RelatedContractID = c.id
        LEFT JOIN dbo.Campaign CM ON C.CampaignID = CM.ID
        LEFT JOIN dbo.Contract sourcec ON sourcec.ID = c.OrigionalContractID
        LEFT JOIN dbo.[User] sourceu ON sourcec.SalesManID = sourceu.ID
	where (c.type in (0,2,3,4)) 
)

SELECT * FROM reportdata
WHERE AutoCreated = @AutoCreated
ORDER BY [ΚΩΔΙΚΟΣ ΣΥΜΒΟΛΑΙΟΥ]</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
