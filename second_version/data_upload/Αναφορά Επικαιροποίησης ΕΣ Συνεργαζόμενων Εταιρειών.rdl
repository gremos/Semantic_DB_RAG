      <Query>
        <DataSourceName>YPS</DataSourceName>
        <CommandText>SELECT u.id, u.LastName 
FROM dbo.UserRole ur
JOIN dbo.[User] u ON ur.UserID = u.ID
WHERE RoleID = 580 --Συνεργαζόμενες Εταιρείες</CommandText>
      </Query>
      <Fields>
        <Field Name="id">
          <DataField>id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="LastName">
          <DataField>LastName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="BroaderAreas">
      <Query>
        <DataSourceName>YPS</DataSourceName>
        <CommandText>SELECT CAST('0' AS VARCHAR(10)) BroaderAreaID, 'Xωρίς Νομό' AreaName
UNION ALL
SELECT cast(BroaderAreaID as nvarchar(10)) BroaderAreaID, AreaName
FROM dbo.BroaderAreaLookUp 
WHERE BroaderAreaID LIKE 'E%'
order by 2</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="BroaderAreaID">
          <DataField>BroaderAreaID</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="AreaName">
          <DataField>AreaName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="ReportDS">
      <Query>
        <DataSourceName>YPS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Users">
            <Value>=Parameters!Users.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@BroaderAreas">
            <Value>=Parameters!BroaderAreas.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT CreatedUserId 
		, CreatedUser 
		, BPId 			
		, ActionType 
        , ActionDate
		, BPCode = bp.Code
		, CustomerIndicator = cnci.Name
		, BPName = bp.BrandName
		, BPBrandName = bp.NAME
		--Report2
		, CompanyPhone = ISNULL((SELECT TOP 1 Number 
						FROM dbo.PointPhone pp 
						WHERE pp.BusinessPointID = bp.ID AND pp.Usage = 1 --etairiko
						ORDER BY pp.ID
							), '')
		--Report2
		, CompanyPhone2 = ISNULL((SELECT TOP 1 Number 
						FROM dbo.PointPhone pp 
						WHERE pp.BusinessPointID = bp.ID AND pp.Usage = 2 --etairiko 2
						ORDER BY pp.ID
							), '')
		--Report2
		, WorkPhone = ISNULL((SELECT TOP 1 Number 
						FROM dbo.PointPhone pp 
						WHERE pp.BusinessPointID = bp.ID AND pp.Usage = 3 --ergasias
						ORDER BY pp.ID
							), '')
												
		, MainActivity = ISNULL(cn.NAME, '')
		
		--Report2
		, SecondaryActivity = ISNULL(scn.name , '')
		
		, StreetNameAndNumber = ad.[Line1] 
		, City = ISNULL( (select top 1 bal.AreaName
						 from dbo.BroaderArea ba with (nolock)
						 join dbo.BroaderAreaLookUp bal with (nolock) on ba.BroaderAreaID = bal.BroaderAreaID
						 where bp.LocationAddressID = ba.AddressID and ba.[BroaderAreaID] LIKE 'L%') , '')
		, ZipCode = ad.NumericZip
		, Area = ISNULL( (select top 1 bal.AreaName
						 from dbo.BroaderArea ba with (nolock)
						 join dbo.BroaderAreaLookUp bal with (nolock) on ba.BroaderAreaID = bal.BroaderAreaID
						 where bp.LocationAddressID = ba.AddressID and ba.[BroaderAreaID] LIKE 'M%' ), '')
        , Prefecture = ISNULL(  (select top 1 bal.AreaName
						 from dbo.BroaderArea ba with (nolock)
						 join dbo.BroaderAreaLookUp bal with (nolock) on ba.BroaderAreaID = bal.BroaderAreaID
						 where bp.LocationAddressID = ba.AddressID and ba.[BroaderAreaID] LIKE 'E%') , '')
        
        ---Report2
        , BPemail = stuff((
						select '; ' + pe.Email
						from dbo.PointEmail pe
						where pe.BusinessPointID = bp.ID
						order by pe.ID
						for xml path('')
					),1,1,'') 
        
        , Url = ISNULL(bp.url, '')
		, BPVatCode = ISNULL(tc.TaxCode, '')
		
		--Report2
		, TaxOfficeName = ISNULL('[' + txo.[Code] + '] ' + txo.[Description] , '')
		
		--Report2
		, Profession = ISNULL(bp.Profession, '')
		
		, ActiveContract = ISNULL( (SELECT top 1 'ΝΑΙ' FROM dbo.Contract c 
												JOIN dbo.ContractProduct cp ON c.ID = cp.RelatedContractID
												JOIN dbo.Advertisement adv ON adv.ContractProductID = cp.id
											WHERE C.State not in (3,5,10) AND cp.State &lt;&gt; 3 and c.type in (0,2,4) AND cp.state &lt;&gt; 3
													AND C.ActualBPOwnerID = a.BPID
													AND DATEDIFF(DAY, GETDATE(), ISNULL(cp.InterruptionDate,isnull(AdvEndDate, DATEADD(year,1,advbegindate))) )  &gt; 0 
														), 'ΟΧΙ')

		--Report2
		, YPSComments =  stuff((
						select CHAR(10) + CAST(ROW_NUMBER() OVER(PARTITION BY bpcom.businesspointid ORDER BY bpcom.CommentDate) AS VARCHAR(max)) + '. '  + bpcom.CommentText  
						from dbo.BusinessPointComment bpcom
						where bpcom.BusinessPointID = bp.ID
						order by bpcom.CommentDate
						for xml path('')
					),1,1,'') 
		
		--Report2
		, CustomerSourceName = ISNULL(cuss.DESCRIPTION, '')
		, CustomerUpdateName = ISNULL(cu.DESCRIPTION, '')
		
		--Report2
		, LastModificationDate = bp.LastUpdateOn
		
		--Report2
		--Pedia Formas Epikairopoihshs
		, IdentityName = bpiden.[Name] 
		, IdentityBrandName = bpiden.[BrandName]
		, IdentityTaxCode = bpiden.[TaxCode] 
		, IdentityProfession = bpiden.[Profession] 
		, IdentityPhone1 = bpiden.[Phone1] 
		, IdentityPhone2 = bpiden.[Phone2]
		, IdentityPhone3 = bpiden.[Phone3]
		, IdentityMobile = bpiden.[MobilePhone] 
		, IdentityFac = bpiden.[Fax] 
		, IdentityEmail1 = bpiden.[Email1] 
		, IdentityEmail2 = bpiden.[Email2] 
		, IdentityAddress = bpiden.[Address] 
		, IdentityCity = bpiden.[City]
		, IdentityPostalCode = bpiden.[PostalCode]
		, IdentityLegalForm = bpiden.[LegalForm]
		, IdentityTaxOffice = bpiden.[TaxOffice] 
		, IdentityBillingAddress = bpiden.[BillingEmailAddress] 
		, IdentityBillingPhone = bpiden.[BillingTelephone] 
		, IdentityExtraInfo = bpiden.[ExtraInformation] 
		

		--Report1														
		, PhoneNum = ISNULL((SELECT TOP 1 Number 
								FROM dbo.PointPhone pp 
								WHERE pp.BusinessPointID = bp.ID AND pp.Usage = 1 --etairiko
								ORDER BY pp.ID
									),
									(SELECT TOP 1 Number 
								FROM dbo.PointPhone pp 
								WHERE pp.BusinessPointID = bp.ID 
									AND pp.Usage not in (7,8) --ola ektos fax
								ORDER BY pp.ID
									)
									)
							
FROM (
SELECT  CreatedUserId =  bpch.PerformedByID
		, CreatedUser = u.LastName
		, BPId = bpch.BusinessPointID 				
		, ActionType = CASE ActionType WHEN 0 THEN 'Επιβεβαίωση στοιχείων'
									   WHEN 1 THEN 'Διόρθωση/ Συμπλήρωση στοιχείων'
									   WHEN 2 THEN 'Νέα εγγραφή'
						END
        , ActionDate = DATEADD(MINUTE, DATEDIFF(MINUTE, 0, bpch.TimeStamp), 0) --bpch.TimeStamp
        , BusinessPointIdentificationID
        
FROM dbo.BusinessPointConfirmationHistory bpch
		JOIN dbo.[User] u ON bpch.PerformedByID = u.ID
       
WHERE   

bpch.TimeStamp BETWEEN @StartDate AND dateadd(day,1,@EndDate)

--bpch.TimeStamp BETWEEN @StartDate AND CAST( CAST( CAST(@EndDate AS DATE) AS VARCHAR(50)) + ' 23:59:59' AS datetime) 

AND u.ID IN (@Users)
		
--gia tis kinhseis pou exoun ginei me ton palio tropo ulopoihshs
UNION 	


SELECT  CreatedUserId = u.ID
		
		, CreatedUser = u.LastName
		
		, BPId = bpi.BusinessPointID 				
		
		, ActionType = CASE t.TaskTypeID WHEN 239 THEN 'Νέα εγγραφή'
                      					 WHEN 236 THEN 'Διόρθωση/ Συμπλήρωση στοιχείων'
                     				END
        
		, ActionDate = DATEADD(MINUTE, DATEDIFF(MINUTE, 0, c.CreatedOn), 0) -- c.CreatedOn

		, BusinessPointIdentificationID = bpi.ID
		
FROM    [Case] c
		
	JOIN dbo.[User] u ON c.CreatedBy = u.UserName
        
	JOIN Task t ON c.ID = t.CaseID	and c.ContextEntityType = 'BPI'
	AND t.TaskTypeID IN ( 236, 239 )
        
	JOIN BusinessPointIdentification bpi ON c.ContextEntityID = bpi.ID
       


WHERE   

c.CreatedOn BETWEEN @StartDate AND dateadd(day,1,@EndDate)

--c.CreatedOn BETWEEN @StartDate AND CAST( CAST( CAST(@EndDate AS DATE) AS VARCHAR(50)) + ' 23:59:59' AS datetime)
		
	AND u.ID in (@Users)
		


UNION 


SELECT CreatedUserId = u.ID
		
	, CreatedUser = u.LastName
		
	, BPId = al.EntityID
		
	, ActionType = 'Επιβεβαίωση στοιχείων'       
	, ActionDate = DATEADD(MINUTE, DATEDIFF(MINUTE, 0, al.Timestamp), 0) --al.Timestamp
        
	, BusinessPointIdentificationID = NULL

FROM [dbo].[AuditLog] AS al
	
	JOIN dbo.[User] u ON al.UserID = u.ID



WHERE 
al.TIMESTAMP BETWEEN @StartDate AND dateadd(day,1,@EndDate)

--al.TIMESTAMP BETWEEN @StartDate AND CAST( CAST( CAST(@EndDate AS DATE) AS VARCHAR(50)) + ' 23:59:59' AS datetime)

	AND u.ID IN  (@Users)
	and NOT EXISTS (SELECT 1 
				
			FROM [Case] c
						
				JOIN dbo.[User] uin ON c.CreatedBy = uin.UserName
						
				JOIN Task t ON c.ID = t.CaseID AND c.ContextEntityType = 'BPI' AND t.TaskTypeID IN ( 236, 239 )
						
				JOIN BusinessPointIdentification bpi ON c.ContextEntityID = bpi.ID
				
			WHERE uin.ID = u.ID 
						
				AND al.EntityID = bpi.BusinessPointID
						
				AND DATEDIFF(minute, al.Timestamp, c.CreatedOn) = 0
						
			)



    ) A
    
    LEFT JOIN dbo.BusinessPointIdentification bpiden ON A.BusinessPointIdentificationID = bpiden.ID
    
	JOIN dbo.BusinessPoint bp ON a.BPId = bp.ID
	LEFT JOIN dbo.CustomerSource cuss ON bp.CustomerSourceID = cuss.ID
	LEFT JOIN dbo.CustomerUpdate cu ON bp.CustomerUpdateID = cu.ID

	LEFT JOIN dbo.TaxOffice txo ON bp.TaxOfficeID = txo.ID
	JOIN dbo.Address ad ON bp.LocationAddressID = ad.ID
	--main activity
	LEFT JOIN dbo.Classification clf ON bp.ID = clf.BusinessPointID AND clf.ClassifierID = 3
    LEFT JOIN dbo.ClassifierNode cn ON clf.ClassifierNodeID = cn.ID 
    --secondary activity
    LEFT JOIN dbo.Classification sclf ON bp.ID = sclf.BusinessPointID AND sclf.ClassifierID = 4
    LEFT JOIN dbo.ClassifierNode scn ON sclf.ClassifierNodeID = scn.ID
    LEFT JOIN dbo.Classification clfci ON bp.ID = clfci.BusinessPointID AND clfci.ClassifierID = 5
    LEFT JOIN dbo.ClassifierNode cnci ON clfci.ClassifierNodeID = cnci.ID 
    LEFT JOIN dbo.TaxCode tc ON bp.ID = tc.ID
    
WHERE   (
		EXISTS(select 1
						 from dbo.BroaderArea Pba 
						 where bp.LocationAddressID = pba.AddressID 
							and EXISTS (SELECT bal.[BroaderAreaID] FROM dbo.BroaderAreaLookUp bal 
	
									WHERE BroaderAreaID IN (@BroaderAreas) 
			
										AND pba.[BroaderAreaID] = bal.BroaderAreaID		
						
									) 
		)
	)

	or ( not exists (select 1 from dbo.BroaderArea Pba where bp.LocationAddressID = pba.AddressID)
			and '0' in (@BroaderAreas)
		)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
