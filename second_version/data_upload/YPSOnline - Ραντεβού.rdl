        <CommandText>
select usr.id
  from [YPS].[dbo].[user] usr 
 where 
 case 
 when @username = 'amatzorou' 
 then 'amantzorou'
 else @username
 end
 = usr.UserName</CommandText>
      </Query>
      <Fields>
