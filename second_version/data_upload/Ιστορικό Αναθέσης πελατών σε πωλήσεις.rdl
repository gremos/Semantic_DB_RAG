  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>YPS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@bpcode">
            <Value>=Parameters!bpcode.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@datefrom">
            <Value>=Parameters!datefrom.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@dateto">
            <Value>=Parameters!dateto.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>--declare @datefrom datetime
--set @datefrom='20121029'
--declare @dateto datetime
--set @dateto='20141110'
--declare @bpcode varchar(10)
--set @bpcode='278563'

;with xx as
(Select MIN(T.CreatedOn) mindd,tgiv.id as tgivid  
   from TargetGroupItemValue tgiv
   join TargetGroupItem tgi on tgi.ID=tgiv.TargetGroupItemID
   JOIN [Case] c ON c.ContextEntityID = tgiv.ID AND c.ContextEntityType = 'TGIV'
   JOIN [Case] cc ON cc.MasterCaseID = c.ID
   JOIN Task t ON t.CaseID = cc.ID
   where tgi.Code=@bpcode
   group by tgiv.id
)

SELECT  tgivid = tgiv.id
      , '['+cmp.code+'] '+cmp.Name campaign
      , u.UserName ActualUser
      , t.Description Task
      , t.CreatedOn TaskCreatedOn
      , t.CompletedOn TaskCompletedOn
      ,  '['+pro.code +'] '+pro.Description as product
      , adv.AdvBeginDate,tgiv.altofferid as contractid
      , '['+mv.Code+'] '+mv.Description AS Medium 
      ,case t.outcome when 'ΑΔΥΝ' then 'Αδυναμία εύρεσης πελάτη'
                      when 'ΕΚΠΤ' then 'Αίτημα Εκπτωσης'
                      when 'ΑΡΝΕ' then 'Αρνηση Επικοινωνίας'
                      when 'ΡΑΝΤ' then 'Κλείσιμο Ραντεβού'
                      when 'ΠΡΟΣΦ' then 'Δημιουργία Προσφοράς'
                      when 'ΝΕΟΡ' then  'Επισκεψη - Νέο Ραντεβού'
                      when 'ΑΡΝ' then 'Αρνηση Συνεργασίας'
                      when 'ΑΝΣΥ' then 'Αναμονή Συμβολαίου'
                      when 'ΑΠΟΔΥ' then 'Aποδοχή με υπογραφή'
                      when 'ΑΠΟΔΜ' then 'Αποδοχή με email'
                      when 'ΣΚΦΦ' then 'Συμπλήρωση κωδικού φυσικού φακέλου'
                      when 'ΔΣΥΜ' then 'Διόρθωση συμβολαίου'
                      when 'ΕΚΠΤΑ' then 'Εξέταση αιτήματος έκπτωσης'
                      when 'ΕΛΟΠΣ' then 'Έλεγχος ορθότητας και πληρότητας Συμβολαίου'
                      when 'ΕΞΜΣ' then 'Εξέταση μεταφοράς στόχου'
                      when 'ΕΞΟΠΣ' then 'Εξέταση ορθότητας και πληρότητας συμβολαίου'
                      when 'ΑΕΚΠΤ' then 'Απάντηση αιτήματος έκπτωσης'
                      when 'ΑΠΟΔΠ' then 'Αποδοχή πρότασης'
                      when 'ΟΛΟΚΒΗΜ' then 'Ολοκλήρωση βήματος'
                      when 'ΣΥΜΥΛ' then 'Συμπλήρωση Κειμένων και Μεταφράσεων'
                      when 'Forward' then 'Προώθηση'
         else  t.Outcome end outcome
      , Case  t.Outcome when 'Επανάκληση' then pr.Description 
                        when 'ΑΡΝ' then nr.Description 
                        when 'ΑΔΥΝ' then nc.Description
        End as reason 
      , pr.Description as postponeReason
      , nr.Description as negationReason 
      , nc.Description as nocommReason
FROM    xx  join TargetGroupItemValue tgiv on xx.tgivid=tgiv.id
        join TargetGroupItem tgi on tgi.ID=tgiv.TargetGroupItemID
        JOIN [Case] c ON c.ContextEntityID = tgiv.ID AND c.ContextEntityType = 'TGIV'
        JOIN [Case] cc ON cc.MasterCaseID = c.ID
        JOIN Task t ON t.CaseID = cc.ID
        join Campaign cmp on cmp.ID=tgi.CampaignID
        JOIN dbo.TaskType tt ON t.TaskTypeID = tt.ID and tt.ActivityType='H'
		LEFT JOIN dbo.[User] u ON t.ActualOwnerID = u.ID
		left join dbo.PostponeReason pr on pr.ID=tgiv.PostponeReasonID
		left join dbo.NegationReason nr on nr.ID=tgiv.NegationReasonID
		left join dbo.NoCommReason nc on nc.ID=tgiv.NoCommReasonID
		LEFT JOIN dbo.Advertisement ADV ON Cc.ContextEntityID = ADV.ID AND Cc.ContextEntityType = 'ADV'
		LEFT JOIN dbo.Product pro ON adv.ProductID = pro.ID
		LEFT JOIN dbo.VersionSection vs on vs.ID=adv.VersionSectionID
        LEFT JOIN dbo.MediumVersion mv on mv.ID=vs.MediumVersionID
WHERE   tgi.code=@bpcode
and xx.mindd between @datefrom and @dateto+1
order by tgiv.ID,t.id</CommandText>
