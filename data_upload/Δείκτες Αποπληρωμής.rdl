        <Field Name="Απαιτητό_Ποσό">
          <DataField>Απαιτητό Ποσό</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Εξοφλημένο">
          <DataField>Εξοφλημένο</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Ανεξόφλητο">
          <DataField>Ανεξόφλητο</DataField>
