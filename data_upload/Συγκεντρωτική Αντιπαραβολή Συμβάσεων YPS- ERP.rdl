  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>ITApps</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@dateFrom">
            <Value>=Parameters!dateFrom.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@dateTo">
            <Value>=Parameters!dateTo.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>--declare @dateFrom date, @dateTo date;
--set @dateFrom = '20130201';
--set @dateTo = '20130301';



if object_id('tempdb..#xx') is not null
drop table #xx

select ct.ID as ctid,ct.State as ctstate,ct.CreatedOn,
      cast((select SUM (adv.price-adv.discount-adv.rulesdiscount) 
			  from cardinal11.yps.dbo.Advertisement adv join cardinal11.yps.dbo.ContractProduct cp on cp.ID=adv.ContractProductID
			  where cp.RelatedContractID=ct.id)as float) ypsValue,
      isnull((select top 1 1 from ypsebs.ebscontractvalues where contractcode=convert (varchar(20),ct.id)),0) hasPar,
      (select sum(ebsparvalue) from ypsebs.ebscontractvalues where contractcode=convert (varchar(20),ct.id)) ebsParvalue,
      (select sum(ebstpyvalue) from ypsebs.ebscontractvalues where contractcode=convert (varchar(20),ct.id)) ebsTimvalue
into #xx
from cardinal11.yps.dbo.Contract ct 
where ct.State not in ( 3,5) AND ct.type IN (0,2,3,4) 
and ct.CreatedOn&gt;=@dateFrom and ct.CreatedOn&lt;=@dateTo





select 
    case when ctstate=1 and haspar=0 then 'ΣΕ YPS'
         when ctstate=1 and haspar=1 then 'ΣΕ YPS-ERP'
         when ctState = 0 then 'Σε YPS Pending'  
         when ctState =2 then 'ΣΕ YPS Rejected' 
         end as  'Κατάσταση',
    count(ctid) 'Πλήθος Συμβολαίων',
    cast (sum(ypsValue) as float) 'Αξία YPS', 
    cast (sum(ebsparValue) as float) 'Αξία ERP Παραγγελιών', 
    cast(sum(ebsTimvalue) as float) as 'Αξία ERP Τιμολογίων',
    case when ctstate=1 and haspar=1 then 1
         when ctstate=1 and haspar=0 then 2  
         when ctState = 0 then 3
         when ctState =2 then 4
         end as  orderInd
    from #xx 
    group by ctstate,haspar
    order by 6</CommandText>
