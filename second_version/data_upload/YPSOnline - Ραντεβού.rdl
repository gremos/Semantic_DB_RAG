    <DataSet Name="SalesRep">
      <Query>
        <DataSourceName>YPS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@pSupervisor">
            <Value>=Parameters!pSupervisor.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select distinct sth.SalesmanID,'['+UserSalesCode+'] '+UserFullName as SalesRep,UserFullName
  from  [YPS].dbo.SalesTreeHistory sth
  join [ITApps].[dbo].[YPSUsersAll]  sup on sup.YPSUserId = sth.SalesmanID
  where sth.SupervisorID in (@pSupervisor) 
    and sth.year  = year(getdate()) 
	and sth.SegmentID = month(getdate()) 
	order by UserFullName</CommandText>
      </Query>
      <Fields>
