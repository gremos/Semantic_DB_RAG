    <DataSet Name="Salesmen">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@supervisor">
            <Value>=Parameters!supervisor.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select distinct '['+u.UserSalesCode+'] '+u.UserFullName as Salesman
	from yps.dbo.SalesTreeHistory sth
	join [ITApps].[dbo].[YPSUsersAll] u on u.YPSUserId=sth.SalesmanID
	join [ITApps].[dbo].[YPSUsersAll] s on s.YPSUserId=sth.SupervisorID 
	where sth.Year = YEAR(getdate()) and sth.IsDeleted = 0 and ('['+s.UserSalesCode+'] '+s.UserFullName) in (@supervisor)
and u.UserSalesCode is not null</CommandText>
      </Query>
      <Fields>
        <Field Name="Salesman">
          <DataField>Salesman</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
