      <Fields>
        <Field Name="Καμπάνια">
          <DataField>Καμπάνια</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="BP_Κωδικός">
          <DataField>BP Κωδικός</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="BP_Επωνυμία">
          <DataField>BP Επωνυμία</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="BP_Διακριτικός_Τίτλος">
          <DataField>BP Διακριτικός Τίτλος</DataField>
          <rd:TypeName>System.String</rd:TypeName>
