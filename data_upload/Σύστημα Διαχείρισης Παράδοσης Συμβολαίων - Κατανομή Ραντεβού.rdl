  <DataSets>
    <DataSet Name="DateCountPerAssignedUser">
      <Query>
        <DataSourceName>ItAppsDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@pDateFrom">
            <Value>=Parameters!pDateFrom.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT au.Name AS 'Ανάθεση', cast(ti.AppointmentDateFrom as date) AS 'Ημ/νία', COUNT(DISTINCT ci.ContractId) AS 'Αρ.Συμβολαίων' 
FROM cdm.Task t
INNER JOIN cdm.TaskInfo AS ti on ti.TaskId = t.Id
INNER JOIN cdm.AssignUsers as au ON au.Id = ti.AssignedTo
INNER JOIN                        cdm.CaseInfo AS ci ON ci.CaseId = t.CaseId 
WHERE t.TaskTypeId IN (2,5,7) and t.Closed = 0 and ti.AppointmentDateFrom &gt;= @pDateFrom
GROUP BY au.Name,cast(ti.AppointmentDateFrom as date)
ORDER BY cast(ti.AppointmentDateFrom as date)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Ανάθεση">
          <DataField>Ανάθεση</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Ημ_νία">
          <DataField>Ημ/νία</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="Αρ_Συμβολαίων">
          <DataField>Αρ.Συμβολαίων</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="AvailableDates">
      <Query>
        <DataSourceName>ItAppsDS</DataSourceName>
        <CommandText>SELECT DISTINCT ti.AppointmentDateFrom AS 'Ημ/νία'
FROM            cdm.CaseInfo AS ci INNER JOIN
                         cdm.ContractAppointment AS ca ON ca.ContractId = ci.ContractId INNER JOIN
                         cdm.Task AS t ON t.CaseId = ci.CaseId INNER JOIN
                         cdm.TaskInfo AS ti ON ti.TaskId = t.Id INNER JOIN
                         cdm.AssignUsers AS au ON au.Id = ti.AssignedTo
WHERE        (ti.AppointmentDateFrom &gt;= GETDATE())</CommandText>
      </Query>
      <Fields>
        <Field Name="Ημ_νία">
          <DataField>Ημ/νία</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
