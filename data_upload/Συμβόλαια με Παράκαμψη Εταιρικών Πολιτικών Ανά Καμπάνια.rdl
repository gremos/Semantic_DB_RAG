  <DataSources>
    <DataSource Name="ITApps">
      <DataSourceReference>ITApps</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>281cc3dc-7b45-4e2c-bf6b-fcf43cf390b4</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>ITApps</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@datefrom">
            <Value>=Parameters!datefrom.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@dateto">
            <Value>=Parameters!dateto.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>with ypsData as (
select ct.ID as contractid,
       isnull('['+cmp.Code+'] '+cmp.Name,'(χωρίς καμπάνια)') as Campaign,
       isnull(cht.Description,'(χωρίς κανάλι)')  as ChannelType,
       ct.ReleaseDate as releasedDate,
       value=(select sum(CASE WHEN cp.state in (3,4) THEN cp.AlteredAmount ELSE  cp.Price - cp.Discount - cp.RulesDiscount	END )
       from yps.dbo.ContractProduct cp where cp.RelatedContractID=ct.ID)                                                                 
from yps.dbo.contract ct
left join yps.dbo.campaign cmp on ct.CampaignID=cmp.ID
left join yps.dbo.ChannelType cht on cht.ID=ct.ChannelTypeID
where ct.State not in (3,5)
)
,alldata as (
select * ,
      hasTicket =isnull((select top 1 1  from ITApps.bcp.Ticket bcp where TicketType='Εξαίρεση από πολιτική προκαταβολής' 
                         and bcp.ContractID=ypsData.contractid ),0),
      hasApprovedTicket =isnull((select top 1 1  from ITApps.bcp.Ticket bcp where TicketType='Εξαίρεση από πολιτική προκαταβολής' 
                          and Status in ('Έγκριση','Επαναφορά','Κλειστό')  and bcp.ContractID=ypsData.contractid  ),0)
from ypsdata
where releasedDate &gt;=@datefrom
and releasedDate&lt;dateadd(dd,1,@dateto)
)

select Campaign,ChannelType,COUNT(contractid)cntContracts,SUM(value) sumContractsValue,
       SUM(hasTicket) cntTickets,
       sum(Case when hasTicket=1 then value else 0 end)sumTicketsValue,
       SUM(hasApprovedTicket) cntApprovedTickets,
       sum(Case when hasApprovedTicket=1 then value else 0 end)sumApprovedTicketsValue,
       round((cast((SUM(hasTicket)*100) as float)/COUNT(contractid)),2) percTickets,
       round((cast((SUM(hasApprovedTicket)*100) as float)/COUNT(contractid)),2) percApprovedTickets
from alldata
group by Campaign,ChannelType
order by 1,2</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Campaign">
          <DataField>Campaign</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="cntContracts">
          <DataField>cntContracts</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="ChannelType">
          <DataField>ChannelType</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="sumContractsValue">
          <DataField>sumContractsValue</DataField>
