          </QueryParameter>
        </QueryParameters>
        <CommandText>

select 
DISTINCT brl.AreaName, brl.BroaderAreaID 
FROM Address adr
join BroaderArea br on br.AddressID = adr.id
JOIN BroaderAreaLookUp  prf on prf.BroaderAreaID = br.BroaderAreaID
and prf.BroaderAreaID in (@perfecture)
join BroaderArea brm on brm.AddressID = adr.id
JOIN BroaderAreaLookUp  brl on brl.BroaderAreaID = brm.BroaderAreaID 
WHERE  
brl.BroaderAreaID like 'hb%'

order by 1</CommandText>
      </Query>
      <Fields>
        <Field Name="AreaName">
          <DataField>AreaName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="BroaderAreaID">
          <DataField>BroaderAreaID</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
